function RaijinHittedKunai(playerid)
{
    if(!IsValidDynamicObject(PlayerRaijinVoador[playerid][raijinKunaiObj])) return 0;
    //SendClientMessage(playerid, -1, "raijinhitted");
    new Float:kunaiPos[3], vw = GetPlayerVirtualWorld(playerid);
    new Float:ninjaPos[3];
    GetDynamicObjectPos(PlayerRaijinVoador[playerid][raijinKunaiObj], kunaiPos[0], kunaiPos[1], kunaiPos[2]);
    for(new i; i<MAX_PLAYERS; i++)
    {
        switch(GetShootedJutsu(kunaiPos[0], kunaiPos[1], kunaiPos[2], 2.5, false, false, vw, playerid, i))
        {
            case PLAYER:
            {
                if(Invunerable[i] == 1) return 0;
                if(Izanagi[i][IzanagiAtivado]){SetTimerEx("IzanagiVoltar", 100, false, "d", i);}
                PlayerRaijinVoador[playerid][raijinAtingiu] = 1;
                AudioInPlayer(playerid, 35.0, 140);
                PlayerRaijinVoador[playerid][raijinTarget] = i;
                GetPlayerPos(playerid, ninjaPos[0], ninjaPos[1], ninjaPos[2]);
                ObjTeleportRaijin[playerid][0] = CreateDynamicObject(15115, ninjaPos[0], ninjaPos[1], ninjaPos[2], 0.0, 0.0, 0.0, vw);
                GetPlayerPos(i, PlayerRaijinVoador[playerid][raijinPosTarget][0], PlayerRaijinVoador[playerid][raijinPosTarget][1], PlayerRaijinVoador[playerid][raijinPosTarget][2]);
                PlayerRaijinVoador[i][raijinAtingido] = 1;
                ApplyAnimation(i, "Mtr_mnt", "Hit_1", 4.1, 0, 0, 0, 1, 0, 1);
                //SendClientMessage(playerid, -1, "RaijinHittedKunai");
                StartAvanco(playerid, i);
                DestroyDynamicObject(PlayerRaijinVoador[playerid][raijinKunaiObj]);
                return 1;
            }
        }
    }
    SetTimerEx("RaijinHittedKunai", 50, false, "d", playerid);
    return 0;
}
