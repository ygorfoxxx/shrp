        if( Info[playerid][pJailed] > 0 ) return SendClientMessageEx(playerid, COLOR_WHITE, "{FFFF00}[ERROR]:{FFFFFF} Voc? n?o pode usar este comando agora.");
        if(GetPVarInt(playerid, "IsInArena") == 1) {
            SendClientMessageEx(playerid, COLOR_WHITE, "No puedes fazer isto na arena!");
