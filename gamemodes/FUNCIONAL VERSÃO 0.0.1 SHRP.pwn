stock VerificarPromocaoHP(playerid, giveplayerid)
{
    new medico = GetHPPatente(giveplayerid);
    new horasNecessario = 0;
    new string[128];
    switch(medico)
    {
        case 0: return 0;
        case 1: {horasNecessario = HORAS_MEDICOAPRENDIZ;} // Medico Aprendiz Iwagakure
        case 2: {horasNecessario = HORAS_MEDICOAUXILIAR;} // Medico Auxiliar Iwagakure
        case 3: {horasNecessario = HORAS_MEDICOAMEDICO;} // Medico Iwagakure
        case 4: {horasNecessario = HORAS_MEDICOSUPERIOR;} // Medico Superior Iwagakure
        case 5: {horasNecessario = HORAS_MEDICOCHEFE;} // Medico Chefe Iwagakure
        case 6: {horasNecessario = HORAS_MEDICOAPRENDIZ;} // Medico Aprendiz Kirigakure
        case 7: {horasNecessario = HORAS_MEDICOAUXILIAR;} // Medico Auxiliar Kirigakure
        case 8: {horasNecessario = HORAS_MEDICOAMEDICO;} // Medico Kirigakure
        case 9: {horasNecessario = HORAS_MEDICOSUPERIOR;} // Medico Superior Kirigakure
        case 10: {horasNecessario = HORAS_MEDICOCHEFE;} // Medico Chefe Kirigakure
    }
    if(Info[giveplayerid][pHPHours] >= horasNecessario)
    {
        //Iwagakure
        if(Info[giveplayerid][pHPPatente] == 1){Info[giveplayerid][pHPPatente] = 2;} // Medico Aprendiz Iwagakure para Medico Auxiliar Iwagakure
        else if(Info[giveplayerid][pHPPatente] == 2 && Info[giveplayerid][pRank] >= 2){Info[giveplayerid][pHPPatente] = 3;} // Medico Auxiliar Iwagakure para Medico Iwagakure
        else if(Info[giveplayerid][pHPPatente] == 3 && Info[giveplayerid][pRank] >= 2){Info[giveplayerid][pHPPatente] = 4;} // Medico Iwagakure para Medico Superior Iwagakure
        else if(Info[giveplayerid][pHPPatente] == 4 && Info[giveplayerid][pRank] == 3){Info[giveplayerid][pHPPatente] = 4;} // Medico Iwagakure para Medico Iwagakure
        //Kiri
        else if(Info[giveplayerid][pHPPatente] == 6){Info[giveplayerid][pHPPatente] = 7;} // Medico Aprendiz Kirigakure para Medico Auxiliar Kirigakure
        else if(Info[giveplayerid][pHPPatente] == 7 && Info[giveplayerid][pRank] >= 2){Info[giveplayerid][pHPPatente] = 8;} // Medico Auxiliar Kirigakure para Medico Kirigakure
        else if(Info[giveplayerid][pHPPatente] == 8 && Info[giveplayerid][pRank] >= 2){Info[giveplayerid][pHPPatente] = 9;} // Medico Kirigakure para Medico Superior Kirigakure
        else if(Info[giveplayerid][pHPPatente] == 9 && Info[giveplayerid][pRank] == 3){Info[giveplayerid][pHPPatente] = 9;} // Medico Kirigakure para Medico Kirigakure
        format(string, sizeof(string), "{AB7C4E}(AVISO) Voc? promovou o ninja ({FFFFFF}%s{AB7C4E}).", PlayerNameDados(giveplayerid));
        SendClientMessage(playerid, -1, string);
