CarregarChakra(playerid, newkeys, oldkeys)
{
    new Float:pPos[3];
    GetPlayerVelocity(playerid, pPos[0], pPos[1], pPos[2]);
    if(pPos[2] != 0) return 0;
    if(Meditando[playerid] == 1) return 0;
    if(Info[playerid][pNinjaQuebrado] == 1 || Info[playerid][pNinjaQuebrado] == 3){
        AudioInPlayer(playerid, 30.0, 33);
        AparecerMapaJutsu(playerid, 100.0);
        DiminuirVidaPorcentagem(playerid);
        return 1;
    }
    if(BtnDireito[playerid] == 1 || IsPlayerInAir[playerid] == 1 || lastJutsu[playerid][jutsuLast] >= 1 || ImpossibleHitChakra[playerid] == 1 || DashUsado[playerid] == 1 || Pescando[playerid] == 1) return 0;
    if(HakkeshouON[playerid] == 1 || lastHitted[playerid][hitReason] > 1 || SuirouON[playerid] == 1 || Info[playerid][pPrisaoBase] == 1 || ByakuganON[playerid] == 1 || TsutenHittedAlvo[playerid] == 1) return 0;
    if(Info[playerid][pEstado] != 0 || KawarimiUsado[playerid] == 1 || HittedDoroji[playerid] == 1 || HittedKageshibari[playerid] == 1 || Info[playerid][pDentroDeCasa] == 1 || SuirouHIT[playerid] == 1 || KatonHIT[playerid] == 1 || Dormido[playerid] != 0 || GetPVarInt(playerid, "Inconsciente") >= 1) return 0;
    if(PRESSED(KEY_CTRL_BACK) && oldkeys != KEY_CTRL_BACK){FCarregandoChakra(playerid);}
    else if(CarregandoChakra[playerid] == 1 && RELEASED(KEY_CTRL_BACK)){PCarregandoChakra(playerid);}
}
