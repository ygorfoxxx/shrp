function HittedPapelBombaRoubo(playerid)
{
    if(!IsValidObject(EffectExplosaoBomba[playerid])) return 0;
    for(new i; i<MAX_PLAYERS; i++)
    {
        if(GetPlayerVirtualWorld(i) == 0 && IsPlayerInRangeOfPoint(i, 10.0, DataPapelBombaEle[playerid][papelbombaPos][0], DataPapelBombaEle[playerid][papelbombaPos][1], DataPapelBombaEle[playerid][papelbombaPos][2]) && AtingindoPelaBomba[i] == 0)
        {
            if(AtingindoPelaBomba[i] == 1) return 0;
            AtingindoPelaBomba[i] = 1;
            SetDamageToPlayer(playerid, i, GetDamageToPlayer(playerid, KATON, 2, 30.0));
            KatonHIT[i] = 1;
            SetTimerEx("LimparKHit", 1000, false, "i", i);
            SetTimerEx("OffVarBomba", 200, false, "d", i);
        }
    }
    SetTimerEx("HittedPapelBombaRoubo", 100, false, "d", playerid);
    return 0;
}
