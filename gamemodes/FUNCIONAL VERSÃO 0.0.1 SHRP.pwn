                    PlayerDrunk[i] -= 1;
                    PlayerDrunkTime[i] = 0;
                }
                PlayerDrunkTime[i] += 1;
            }
            if(PlayerDrunk[i] < 10 && PlayerDrunk[i] > 0)
            {
