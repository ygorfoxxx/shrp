public GlobalTime()
{
    new Hour, Minute, Second;
    gettime(Hour, Minute, Second);
    new tiempo = random(5);
    if(Hour == 22) // Tiempo nublado/bueno.
    {
        //SendClientMessageToAll(COLOR_GREEN, "[Anuncio Radio] Buenas noches San Andreas, que tengan una agradable noche. Sigan con nosotros en la radio.");
        print("El clima del servidor fue cambiado autom?ticamente [22H].");
        if(tiempo == 0 || tiempo ==2 || tiempo == 4){SetWeather(10);} // El bueno.
        else {SetWeather(14);} // El Malo (Nublado).
    }
    if(Hour == 2) // Oscuro(Nublado) o normal.
    {
        //SendClientMessageToAll(COLOR_GREEN, "[Estaci?n meteorol?gica]: Atenci?n, en las pr?ximas horas podr?a haber formaci?n de borrasca, posible lluvia.");
        print("El clima del servidor fue cambiado autom?ticamente [2AM]");
        if(tiempo == 0 || tiempo ==2 || tiempo == 4) {SetWeather(17);} // El bueno.
        else {SetWeather(20);} // El malo (Muy nublado)
    }
    if(Hour == 4) // Lluvia o normal.
    {
        //SendClientMessageToAll(COLOR_GREEN, "[Estaci?n meteorol?gica]: Atenci?n, se espera en las pr?ximas horas lluvias torrenciales constantes.");
        print("El clima del servidor fue cambiado autom?ticamente [4AM].");
        if(tiempo == 0 || tiempo ==2) {SetWeather(43);} // Muy noche.
        else {SetWeather(8);} // El malo, Lluvia. (M?s posibilidades)
    }
    if(Hour == 6) // Niebla o normal.
    {
        //SendClientMessageToAll(COLOR_GREEN, "[Estaci?n meteorol?gica]: Atenci?n, se ha detectado posible formaci?n de niebla durante las primeas horas del dia.");
        print("El clima del servidor fue cambiado autom?ticamente [6AM].");
        if(tiempo == 0 || tiempo ==2 || tiempo == 4) {SetWeather(4);} // El bueno, comienza el dia.
        else {SetWeather(9);} // El malo, mucha niebla.
    }
    if(Hour == 10) // Desaparici?n de toda existencia de niebla/lluvia.
    {
        //SendClientMessageToAll(COLOR_GREEN, "[Anuncio Radio] Buenos d?as San Andreas, les espera un d?a maravilloso. Disfruten de nuestra programaci?n de hoy.");
        print("El clima del servidor fue cambiado autom?ticamente [10H].");
        if(tiempo == 0 || tiempo ==2 || tiempo == 4) {SetWeather(3);} // El bueno, comienza el dia.
        else {SetWeather(0);} // Tambi?n es bueno. No habr? niebla y lluvia hasta que sea de noche.
    }
    return 1;
}
