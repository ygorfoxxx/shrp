        if(sscanf(params, "d", giveplayerid)) return SendClientMessageEx(playerid, COLOR_WHITE, "Utilize: /examinar (ID)");
        if(PlayerIsLogado[giveplayerid] == 1){
            if(playerid == giveplayerid) return SendClientMessageEx(playerid, COLOR_WHITE, "(AVISO) Voc? n?o pode examinar a si mesmo.");
            if(ProxDetectorS(2.5, playerid, giveplayerid))
            {
                new Float:VidaEmUso = Info[giveplayerid][pHealthEnUso], Float:VidaMaxima = Info[giveplayerid][pHealthMaximo];
                new Float:PorcentagemDVida = (VidaEmUso/VidaMaxima)*100;
                if(PorcentagemDVida >= 90){ExaminarAlvo[playerid] = 1;}
                else if(PorcentagemDVida >= 70){ExaminarAlvo[playerid] = 2;}
                else if(PorcentagemDVida >= 35){ExaminarAlvo[playerid] = 3;}
                else if(PorcentagemDVida >= 0){ExaminarAlvo[playerid] = 4;}
                if(Info[giveplayerid][pNinjaFraturado] == 1){ExaminarFratura[playerid] = 1;}
                if(Info[giveplayerid][pNinjaFraturado] == 2){ExaminarFratura[playerid] = 2;}
                if(Info[playerid][pMaskuse] == 1){
                    format(string, sizeof(string), "* (Mascarado) est? examinando um ninja.");
                    ProxDetector(25.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
                }else{
                    format(string, sizeof(string), "* %s est? examinando um ninja.", PlayerNameDados(playerid));
                    ProxDetector(25.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
                }
                Examinando[playerid] = 1;
                ExaminarQuebrado[playerid] = giveplayerid;
                ApplyAnimation(playerid, "int_shop", "shop_loop", 4.0, 1, 0, 0, 1, 5000, 1);//Anima??o de examinar
                SetTimerEx("FinalizarExaminar", 5000, false, "i", playerid);
            }
        }
    }
    return 1;
}
function FinalizarExaminar(playerid)
{
    if(Examinando[playerid] == 0) return 0;
    new string[128], vida[32], fratura[32];
    switch(ExaminarAlvo[playerid])
    {
        case 1: vida = "Cheia";
        case 2: vida = "Alta";
        case 3: vida = "Media";
        case 4: vida = "Baixa";
    }
    switch(ExaminarFratura[playerid])
    {
        case 1: fratura = "Bra?o";
        case 2: fratura = "Perna";
    }
    format(string, sizeof(string), "{AB7C4E}(EXAMINAR) O ninja se encontra com a vida ({FFFFFF}%s{AB7C4E}).", vida);
    SendClientMessage(playerid, -1, string);
    if(ExaminarFratura[playerid] == 1 || ExaminarFratura[playerid] == 2){
        format(string, sizeof(string), "{AB7C4E}(EXAMINAR) O ninja se encontra com uma fratura em seu(a) ({FFFFFF}%s{AB7C4E}).", fratura);
        SendClientMessage(playerid, -1, string);
    }
    if(Info[ExaminarQuebrado[playerid]][pNinjaQuebrado] == 1)
    {
        format(string, sizeof(string), "{AB7C4E}(EXAMINAR) O ninja se encontra com a perna ({FFFFFF}Quebrada{AB7C4E}).");
        SendClientMessage(playerid, -1, string);
    }
    if(Info[ExaminarQuebrado[playerid]][pNinjaQuebrado] == 2)
    {
        format(string, sizeof(string), "{AB7C4E}(EXAMINAR) O ninja se encontra com o bra?o ({FFFFFF}Quebrado{AB7C4E}).");
        SendClientMessage(playerid, -1, string);
    }
    if(Info[ExaminarQuebrado[playerid]][pNinjaQuebrado] == 3)
    {
        format(string, sizeof(string), "{AB7C4E}(EXAMINAR) O ninja se encontra com a perna e o bra?o ({FFFFFF}Quebrados{AB7C4E}).");
        SendClientMessage(playerid, -1, string);
    }

    if(Info[playerid][pMaskuse] == 1){
        format(string, sizeof(string), "* (Mascarado) termina de examinar o ninja.");
        ProxDetector(25.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
    }else
    {
        format(string, sizeof(string), "* %s termina de examinar o ninja.", PlayerNameDados(playerid));
        ProxDetector(25.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
    }
    ApplyAnimation(playerid, "int_shop", "shop_loop", 4.0, 0, 0, 0, 0, 1, 1);//Anima??o de examinar
    ExaminarAlvo[playerid] = 0;
    ExaminarQuebrado[playerid] = -1;
    return 1;
}
function PararDeExaminar(playerid)
{
    if(Examinando[playerid] == 0) return 0;
