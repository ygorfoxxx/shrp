// Ajuste fino da distncia (quanto maior, mais longe recua)
#define KAWARIMI_RECUO_FORCA   (0.50) // antes era -0.5 no SubstituicaoSpeed
#define KAWARIMI_RECUO_ZVEL    (1.10) // mantm o pulo/elevao como no seu SubstituicaoSpeed

// Impulso para trs baseado na direo que a CMERA est olhando
stock Kawarimi_RecuoContraCamera(playerid, Float:forca = KAWARIMI_RECUO_FORCA, Float:zVel = KAWARIMI_RECUO_ZVEL)
{
    new Float:fx, Float:fy, Float:fz;
    GetPlayerCameraFrontVector(playerid, fx, fy, fz);

    // Usa apenas X/Y (pra no afundar/subir se a cmera estiver olhando pra baixo/cima)
    new Float:len = floatsqroot((fx * fx) + (fy * fy));
    if (len < 0.0001) return 0;

    fx /= len;
    fy /= len;

    // "Para trs" = vetor contrrio do front da cmera
    return SetPlayerVelocity(playerid, -fx * forca, -fy * forca, zVel);
}

// === Kawarimi Jutsu Kawirimi
function KawarimiJutsu(playerid)
{

    // ==========================================================
// [NNRP] Anti-freeze: Kawarimi durante AirCombo / Hitstun
// - Se o player estiver em AirCombo, quebra o par na hora
// - Se estiver em hitstun do ch?o (NNRP_GSTUN), libera tamb?m
// ==========================================================
#if defined _NNRP_COMBATE_AIR_
    if(AirCombo[playerid][airIn] == 1)
    {
        new other = AirCombo[playerid][airTarget];

        // Quebra o combo (limpa timers/flags dos dois lados)
        if(other != -1 && IsPlayerConnected(other))
            NNRP_AirComboBreak(playerid, other, 77 /* reason: kawarimi */);
        else
            NNRP_AirComboBreak(playerid, playerid, 77);
    }
#endif

// Se estava no stun do combo do ch?o, libera (evita travar ap?s Kawarimi)
if(GetPVarInt(playerid, "NNRP_GSTUN") == 1)
{
    DeletePVar(playerid, "NNRP_GSTUN");
    TogglePlayerControllable(playerid, 1);
}

    // n?o deixa usar kawarimi durante RapidDash (ou quando estiver marcado pelo lock)
	if(GetPVarInt(playerid, "NNRP_NO_KAWARIMI") == 1)
    return SendClientMessage(playerid, -1, "{EF0D02}(AVISO){FFFFFF} Voc? n?o pode usar Kawarimi durante uma investida.");
    if(Meditando[playerid] == 1) return 0;
    if(lastHitted[playerid][hitReason] > 1 || lastJutsu[playerid][jutsuLast] >= 1) return 0;
    if(GetPVarInt(playerid, "Inconsciente") >= 1 || HakkeshouON[playerid] == 1 || HittedDoroji[playerid] == 1 || HittedKageshibari[playerid] == 1 || Info[playerid][pDentroDeCasa] == 1 || DaikodanHitAlvo[playerid] == 1 || TsutenHittedAlvo[playerid] == 1 ||
       UsandoKit[playerid] == 1 || EstaRoubando[playerid] == 1 || SeladoS[playerid] == 1 || ChidoriON[playerid] == 2 || SuirouON[playerid] == 1 || PlayerParalisado[playerid] == 1 || SetPlayerState[playerid] == 1 || ChuteCimaHitted[playerid] == 1 || SuirouHIT[playerid] == 1 || KatonHIT[playerid] == 1 ||
       Pescando[playerid] == 1) return 0;

    if(Info[playerid][pEstado] != 0 || Dormido[playerid] != 0 || GetPVarInt(playerid, "Inconsciente") >= 1) return 0;
    if(KawarimiTimer[playerid] > gettime()) return JutsuNotReady2(playerid, COLOR_WHITE, "Substituio", KawarimiTimer[playerid]);

    KawarimiTimer[playerid] = gettime() + KAWARIMI_COOLDOWN;

    SetPlayerArmedWeapon(playerid, 0);
    KawarimiUse[playerid] = 1;
    KawarimiUsado[playerid] = 1;

    SustJutsu(playerid);

    ApplyAnimation(playerid, "Shinobi_Anim", "JumpB", 4.1, 0, 1, 1, 0, 0);

    KawarimiUseTimer[1][playerid] = SetTimerEx("SustJutsuOff", 2000, false, "i", playerid);
    KawarimiUseTimer[0][playerid] = SetTimerEx("UsingKawarimi", 15000, false, "i", playerid);

    return 1;
}

