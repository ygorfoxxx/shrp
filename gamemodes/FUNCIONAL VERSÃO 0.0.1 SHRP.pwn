// PVar usada pelo sistema ferido para mudar o que aparece no nametag
#define FERIDO_PVAR_TAGMODE  "FeridoTagMode"
#define TAGMODE_NORMAL       (0)
#define TAGMODE_LIMITADO     (1) // Brutalmente ferido
#define TAGMODE_MORTO        (2) // Morto

// Se ainda no existir no seu cdigo, crie:
new Text3D:NomeClanRankShadow[MAX_PLAYERS] = {Text3D:INVALID_3DTEXT_ID, ...};

// =======================================
//  NORMAL: (ID) Nome Cl (Rank)
// =======================================
stock BuildPersonagemNameTag(playerid, out[], out_size)
{
    new nomeChar[MAX_PLAYER_NAME];
    format(nomeChar, sizeof nomeChar, "%s", Info[playerid][pNome]);

    if (nomeChar[0] == '\0')
        GetPlayerName(playerid, nomeChar, sizeof nomeChar);

    new clan[24];
    GetClanNameByID(Info[playerid][pClan], clan, sizeof clan);

    new rk[4];
    GetFamaRankStr(NinjaFamaRank[playerid], rk, sizeof rk);

    if (clan[0])
        format(out, out_size, "{FFFFFF}(%d) %s %s (%s)", playerid, nomeChar, clan, rk);
    else
        format(out, out_size, "{FFFFFF}(%d) %s (%s)", playerid, nomeChar, rk);

    return 1;
}

// =======================================
//  LIMITADO: (ID) Nome   (sem cl/rank)
// =======================================
stock BuildPersonagemMiniTag(playerid, out[], out_size)
{
    new nomeChar[MAX_PLAYER_NAME];
    format(nomeChar, sizeof nomeChar, "%s", Info[playerid][pNome]);

    if (nomeChar[0] == '\0')
        GetPlayerName(playerid, nomeChar, sizeof nomeChar);

    format(out, out_size, "{FFFFFF}(%d) %s", playerid, nomeChar);
    return 1;
}

