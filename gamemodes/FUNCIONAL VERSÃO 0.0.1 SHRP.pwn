function HousenkaSeguir(playerid, balls, Float:distancia, Float:velocidad, targetid)
{
    if(!IsValidDynamicObject(Housenka[playerid][houObject][balls])) return 0;

    new zOffset = 0.0, rxOffset = 0.0, ryOffset = 0.0, castedfrom = 1.5;
    new Float:fPX, Float:fPY, Float:fPZ,
    Float:fVX, Float:fVY, Float:fVZ,
    Float:object_x, Float:object_y, Float:object_z,
    Float:fZPitch, Float:arc, Float:currentDist, Float:CP[3];

    GetPlayerPos(targetid, fPX, fPY, fPZ);
    GetPlayerCameraFrontVector(targetid, fVX, fVY, fVZ);
    CP[0] = fPX + floatmul(fVX, castedfrom);
    CP[1] = fPY + floatmul(fVY, castedfrom);
    CP[2] = fPZ+zOffset;
    GetPlayerPos(targetid, object_x, object_y, object_z);
    fZPitch = atan2(object_y - fPY, object_x - fPX) - 90.0;
    arc = asin((object_z - fPZ) / distancia);
    currentDist = GetDistance3D(object_x, object_y, object_z, CP[0], CP[1], CP[2]);
    MoveDynamicObject(Housenka[playerid][houObject][balls], object_x, object_y, object_z, velocidad, arc+rxOffset, ryOffset, fZPitch);

    SetTimerEx("HousenkaSeguir", 25, 0, "ddffd", playerid, balls, distancia, velocidad, targetid);
    return 0;
}
