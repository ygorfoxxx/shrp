// Inicio Mizurappa no Jutsu
function SJutsuMizurappa(playerid)
{
    if(Info[playerid][pSuiton] == 0) return 0;
    if(Info[playerid][pSuiton] >= 1)
    if(Info[playerid][pChakraEnUso] < 250.0) return SemChakra(playerid);
    {

        if(TimerMizurappa[playerid] > gettime()) return JutsuNotReady2(playerid, COLOR_WHITE, "Mizurappa no Jutsu", TimerMizurappa[playerid]);
        TimerMizurappa[playerid] = gettime() + MIZURAPPA_COOLDOWN;
        if(IsPlayerInAir[playerid] == 1){SetXYPlayerVelocity(playerid, 0.03);}
        SetPlayerFacingAngleToCamera(playerid);
        if(Meditacao[playerid][meditacaoEspecialista] == 1){Treinamento(playerid, 4);}else{Treinamento(playerid, 7);}
        AudioInPlayer(playerid, 10.0, 72);
        MizurappaSay(playerid);
        MizurappaJutsu(playerid);
        Info[playerid][pChakraEnUso] -= 250.0;
    }
    return 1;
}
