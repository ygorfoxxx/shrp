            DestroyDynamicObject(Cogumelos[h][pCoguObj]);
            DestroyDynamic3DTextLabel(Cogumelos[h][pCoguText]);
        }
    }
    return 1;
}
function LoopCriarCogumelos()
{
    new mundo = 0, string[128];
    for(new h = 0; h < sizeof(TimingCogumeloRespawn); h++)
    {
        if(TimingCogumeloRespawn[h] != 0 && gettime() >= TimingCogumeloRespawn[h])
        {
            PegouCogumelos[h] = 0;
            Cogumelos[h][pCoguObj] = CreateDynamicObject(Cogumelos[h][pCoguID], Cogumelos[h][pCogumeloX], Cogumelos[h][pCogumeloY], Cogumelos[h][pCogumeloZ]-1.05, 0.0, 0.0, 0.0, mundo);
