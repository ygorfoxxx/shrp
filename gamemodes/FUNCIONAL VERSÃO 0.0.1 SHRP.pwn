        PlayerAndando[playerid] = 1;
    }
    if(indexAnim == 1266 && PlayerIsCorrendo[playerid] == 0 && Info[playerid][pNinjaFraturado] == 2)
    {
        PlayerIsCorrendo[playerid] = 1;
        PlayerAndando[playerid] = 0;
        CorrendoEvento[playerid] = 1;
