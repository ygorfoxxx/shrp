stock AirFinisher_TryExecute(playerid)
{
    if(!AirFinisherReady[playerid]) return 0;

    // expira se passou a janela (opcional)
    if(AirFinisherUntil[playerid] && gettime() > AirFinisherUntil[playerid])
    {
        AirFinisher_Reset(playerid);
        return 0;
    }

    new targetid = AirFinisherTarget[playerid];
    if(targetid == -1 || !IsPlayerConnected(targetid))
    {
