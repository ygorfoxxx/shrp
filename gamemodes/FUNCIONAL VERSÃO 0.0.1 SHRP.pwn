        if(Inventario[playerid][pSave][i] == item && Inventario[playerid][pTotalSave][i] >= quanti && IsPlayerInRangeOfPoint(playerid, 1.0, FogueiraD[playerid][FogueiraPos][0], FogueiraD[playerid][FogueiraPos][1], FogueiraD[playerid][FogueiraPos][2]))
        {
            GetPlayerPos(playerid, FogueiraD[playerid][FogueiraPosF][0], FogueiraD[playerid][FogueiraPosF][1], FogueiraD[playerid][FogueiraPosF][2]);
            RemoveItem(playerid, item, quanti);
            AudioInPlayer(playerid, 30.0, 133);
            ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 4.1, 0, 0, 0, 0, 1000, 1);
            UsandoKit[playerid] = 1;
            format(string, sizeof(string), "* %s esta tentando acender uma fogueira.", PlayerNameDados(playerid));
            ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
            TimingAcenderFogueira[playerid] = SetTimerEx("AcendeuFogueira", 4500, 0, "dd", playerid, 0);
        }
    }
    return 1;
}
function AcendeuFogueira(playerid)
{
    new string[128];
    KillTimer(TimingAcenderFogueira[playerid]);
    UsandoKit[playerid] = 0;
    FogueiraCast[playerid] = 1;
    TimingFogueira[playerid] = gettime() + TEMPO_FOGUEIRA;
    CuraFogueira(playerid);
    ClearAnimations(playerid);
    ObjetoFumacaFogueira[playerid] = CreateDynamicObject(18727, FogueiraD[playerid][FogueiraPosF][0], FogueiraD[playerid][FogueiraPosF][1], FogueiraD[playerid][FogueiraPosF][2]-1.0, 0.0, 0.0, -90.0, -1, -1, -1, 200.0);
    format(string, sizeof(string), "* %s terminou de acender uma fogueira.", PlayerNameDados(playerid));
    ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
    return 1;
}
function CuraFogueira(playerid)
{
    if(!FogueiraCast[playerid]) return 0;
    for(new i; i<MAX_PLAYERS; i++)
    {
        if(IsPlayerInRangeOfPoint(i, 2.5, FogueiraD[playerid][FogueiraPosF][0], FogueiraD[playerid][FogueiraPosF][1], FogueiraD[playerid][FogueiraPosF][2]) && GetPlayerVirtualWorld(i) == 0)
        {
            if(GetPVarInt(i, "Inconsciente") >= 1){
                KillTimer(InconscienteTimerA[i]);
                KillEMSQueue(i);
                ClearAnimations(i);
                ClearInconsciente(i);
                SetPlayerWeapons(i);
                UnfreezePlayer(i);
                NewSetHP(i, 3);
                SeladoInconsciente[i] = 0;
            }else{
                new Float:HealthEnUso, Float:HealthMaximo;
                HealthEnUso = Info[i][pHealthEnUso];
                HealthMaximo = Info[i][pHealthMaximo];
                new Float:QuantidadeCura = (HealthMaximo * 0.03); // 3% de cura
                new Float:NovaVida = HealthEnUso + QuantidadeCura;
                if(NovaVida > HealthMaximo){
                    NewSetHP(i, HealthMaximo);
                }else{
                    NewSetHP(i, NovaVida);
                }
            }
        }
    }
    SetTimerEx("CuraFogueira", 7800, false, "d", playerid);
    return 0;
}
