function IsShinkuhaHitted(playerid, wind)
{
    if(!IsValidDynamicObject(Shinkuha[playerid][shinObject][wind])) return 0;
    new Float:shinPos[3], vw = GetPlayerVirtualWorld(playerid);
    GetDynamicObjectPos(Shinkuha[playerid][shinObject][wind], shinPos[0], shinPos[1], shinPos[2]);
    for(new i; i<MAX_PLAYERS; i++)
    {
        switch(GetShootedJutsu(shinPos[0], shinPos[1], shinPos[2], 4.0, false, true, vw, playerid, i))
        {
            case DUMMY:
            {
                ShinkuhaOff(playerid, wind);
                AudioInPlayer(i, 40.0, 100);
                return 1;
            }
            case WALL:
            {
                ShinkuhaOff(playerid, wind);
                AudioInPlayer(i, 40.0, 100);
            }
            case PLAYER:
            {
                if(Izanagi[i][IzanagiAtivado]){
                    SetTimerEx("IzanagiVoltar", 100, false, "d", i);
                }else{
                    if(EntrouArenaPvP[i] == 1){LastHitArenaPvP(playerid, i);}
                    ClearSelo(i);
                    HittedShinkuha[wind][playerid][i] = 1;
                    EmCombateIniciar(playerid);
                    EmCombateIniciar(i);
                    PCarregandoChakra(i);
                    ShinkuhaOff(playerid, wind);
                    ApplyAnimation(i, "PED", "KART_drive", 4.1, 0, 0, 0, 0, 0, 1);
                    AudioInPlayer(i, 40.0, 100);
                    AparecerMapaJutsu(i, 40.0);
                    SetDamageToPlayer(playerid, i, GetDamageToPlayer(playerid, FUTON, 3, 95.0));
                    FraturarNinja(i);
                }
            }
        }
    }
    SetTimerEx("IsShinkuhaHitted", 50, false, "dd", playerid, wind);
    return 0;
}
ShinkuhaOff(playerid, wind)
{
    if(!IsValidDynamicObject(Shinkuha[playerid][shinObject][wind])) return 0;
    DestroyDynamicObject(Shinkuha[playerid][shinObject][wind]);
    Shinkuha[playerid][shinObject][wind] = INVALID_OBJECT_ID;
    if(wind)
    {
        for(new i; i<MAX_PLAYERS; i++)
        {
            for(new x; x<2; x++)
            {
                if(HittedShinkuha[x][playerid][i]) HittedShinkuha[x][playerid][i] = 0;
            }
        }
    }
    return 1;
}
