function FollowPlayer(playerid, targetid, Float:speed)
{
    if(SeguindoPlayer[playerid] == 0 || KawarimiUsado[targetid] == 1) return 0;
    new Float:posX, Float:posY, Float:posZ;
    new Float:targetX, Float:targetY, Float:targetZ;
    new Float:distance, Float:dirX, Float:dirY, Float:dirZ;

    // Pega a posi??o do player e do target
    GetPlayerPos(playerid, posX, posY, posZ);
    GetPlayerPos(targetid, targetX, targetY, targetZ);

    // Calcular a diferen?a de altura entre o playerid e o targetid
    new Float:verticalDifference = targetZ - posZ;

    // Ajuste vertical para alinhar o playerid com o targetid
    if(verticalDifference > speed)
        posZ += speed;
    else if(verticalDifference < -speed)
        posZ -= speed;
    else
        posZ = targetZ;

    // Calcular a dist?ncia tridimensional entre o playerid e o targetid
    distance = GetDistance3D(posX, posY, posZ, targetX, targetY, targetZ);
    if(distance > 0.0)
    {
        dirX = (targetX - posX) / distance;
        dirY = (targetY - posY) / distance;
        dirZ = (targetZ - posZ) / distance;
    }

    // Definir a velocidade do playerid na dire??o do targetid
    SetPlayerVelocity(playerid, dirX * speed, dirY * speed, dirZ + 0.2 * speed);

    // Ajustar o ?ngulo do playerid para olhar em dire??o ao targetid
    SetPlayerLookAt(playerid, targetX, targetY);

    // Chamar a fun??o novamente para continuar o movimento e o ajuste de dire??o
    //SetTimerEx("FollowPlayer", 25, 0, "ddf", playerid, targetid, speed);
    return 0;
}
static SetPlayerLookAt(playerid, Float:x, Float:y)
{
    new Float:Px, Float:Py, Float:Pa;
    GetPlayerPos(playerid, Px, Py, Pa);

    // Calcular o ?ngulo para o target
    Pa = floatabs(atan((y - Py) / (x - Px)));
    if (x <= Px && y >= Py)        Pa = floatsub(180.0, Pa);
    else if (x < Px && y < Py)      Pa = floatadd(Pa, 180.0);
    else if (x >= Px && y <= Py)    Pa = floatsub(360.0, Pa);

    Pa = floatsub(Pa, 90.0);
    if (Pa >= 360.0)
        Pa = floatsub(Pa, 360.0);

    // Define o ?ngulo de vis?o do playerid
    SetPlayerFacingAngle(playerid, Pa);
    return;
}
