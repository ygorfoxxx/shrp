            MoveObject(PortaBaseSuna, BaseSuna[playerid][basePortaPos][0], BaseSuna[playerid][basePortaPos][1], BaseSuna[playerid][basePortaPos][2]-7.5, 0.8);
            TimerBase[playerid] = SetTimerEx("BaseFechar", 13000, false, "dd", playerid, 0);
            AudioInArea(playerid, 129, BaseSuna[playerid][basePos][0], BaseSuna[playerid][basePos][1], BaseSuna[playerid][basePos][2], 250.0);
