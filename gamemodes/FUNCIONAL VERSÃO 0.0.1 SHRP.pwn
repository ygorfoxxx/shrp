public OnPlayerCommandReceived(playerid, cmdtext[])
{
    printf("[CMD] [%s]: %s", PlayerNameDados(playerid), cmdtext);
    if(!gPlayerLogged{playerid}) return SendClientMessageEx(playerid, COLOR_GRAD2, "* No puedes utilizar comandos si no est?s logeado en el servidor");
    if(strfind(cmdtext, "|", true) != -1)
    {
        SendClientMessageEx(playerid, COLOR_GRAD2, "No puedes usar el car?cter '|' en un comando.");
        return 0;
    }
    if(GetPVarInt(playerid, "LoadingObjects") == 1)
    {
        SendClientMessageEx(playerid, COLOR_GRAD2, "N?o pode usar comandos agora.");
        return 0;
    }
    if(Info[playerid][pAdminZC] < 999999)
    {
        if(strfind(cmdtext, ":", true) != -1)
        {
            new
                i_numcount,
                i_period,
                i_pos;
            while(cmdtext[i_pos])
            {
                if('0' <= cmdtext[i_pos] <= '9') i_numcount++;
                else if(cmdtext[i_pos] == '.') i_period++;
                i_pos++;
            }
            if(i_numcount >= 8 && i_period >= 3)
            {
                new string[128];
                format(string,sizeof(string),"Atenci?n: (ID: %d) %s puede estar haciendo SPAM: '%s'.", playerid, PlayerNameDados(playerid),cmdtext);
                SendAdminMessage(COLOR_RED, string);
                return 0;
            }
        }
    }
    return 1;
}

