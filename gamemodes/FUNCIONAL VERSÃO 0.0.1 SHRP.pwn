    PlayerTextDrawHide(playerid, BotoesStatsePerfil[playerid][1]); // Botao Stats
    PlayerTextDrawHide(playerid, InventarioBKG[playerid][0]); // InventarioHUD
    PlayerTextDrawHide(playerid, BotaoENegociar[playerid][0]); // Botao Negociar
    PlayerTextDrawHide(playerid, BotaoENegociar[playerid][1]); // Texto Negociar
