function SistemaBandanaIDStatus(playerid)
{
    new Vila[64], status[64];
    new Aldeia = Info[playerid][pMember];
    new StatusP = Status[playerid];
    new string[256];

    // ---------- VILA ----------
    switch(Aldeia)
    {
        case 1:  Vila = "{8A391C}Iwagakure";
        case 2:  Vila = "{168500}Sunagakure";
        case 3:  Vila = "{030085}Kirigakure";
        case 4:  Vila = "{DD0200}Konohagakure";
        case 5:  Vila = "{DDA303}Kumogakure";
        case 6:  Vila = "{8A391C}Renegado";
        case 7:  Vila = "{030085}Renegado";
        case 8:  Vila = "{C5CA49}Myoboku";
        case 9:  Vila = "{C5CA49}Mascarado";
        case 10: Vila = "{DD0200}Feudal";
        case 11: Vila = "{DDA303}Renegado";
        default: Vila = "{FFFFFF}SemBandana";
    }

    // ---------- STATUS (s usado no modo normal) ----------
    switch(StatusP)
    {
        case 1: status = "{FF030F}Brutalmente Ferido";
        case 2: status = "{FF030F}Gravemente Ferido";
        case 3: status = "{FF030F}Ferido";
        case 4: status = "{DDA303}Ausente";
        case 5: status = "{55ADC1}Em Atendimento";
        case 6: status = "{D4CAC3}Paralisado";
        case 7: status = "{B38212}Com Sono";
        case 8: status = "{FF030F}Em Combate";
        default: status = "{21DD00}...";
    }

    // =========================================================
    //  LIMPA QUALQUER LABEL ANTIGA SEPARADA (se ainda existir)
    // =========================================================
    if (IsValidDynamic3DTextLabel(NomeID[playerid]))
    {
        DestroyDynamic3DTextLabel(NomeID[playerid]);
        NomeID[playerid] = Text3D:INVALID_3DTEXT_ID;
    }
    if (IsValidDynamic3DTextLabel(BandanaPlayer[playerid]))
    {
        DestroyDynamic3DTextLabel(BandanaPlayer[playerid]);
        BandanaPlayer[playerid] = Text3D:INVALID_3DTEXT_ID;
    }
    if (IsValidDynamic3DTextLabel(StatusPlayer[playerid]))
    {
        DestroyDynamic3DTextLabel(StatusPlayer[playerid]);
        StatusPlayer[playerid] = Text3D:INVALID_3DTEXT_ID;
    }

    // =========================================================
    //  MODO LIMITADO (BRUTAL/MORTO): s ID Nome + Vila
    // =========================================================
    new tagmode = GetPVarInt(playerid, FERIDO_PVAR_TAGMODE);
    if (tagmode == TAGMODE_LIMITADO || tagmode == TAGMODE_MORTO)
    {
        new topoMini[144];
        BuildPersonagemMiniTag(playerid, topoMini, sizeof topoMini);

        // 2 linhas, SEM status, SEM fama/cl/rank
        format(string, sizeof(string), "%s\n{FFFFFF}%s", topoMini, Vila);
    }
    else
    {
        // Normal
        new topo[144];
        BuildPersonagemNameTag(playerid, topo, sizeof topo);

        format(string, sizeof(string), "%s\n{FFFFFF}%s | %s", topo, Vila, status);
    }
    // ---------- PRINCIPAL ----------
    if (IsValidDynamic3DTextLabel(NomeClanRank[playerid]))
        UpdateDynamic3DTextLabelText(NomeClanRank[playerid], NT_COLOR_MAIN, string);
    else
        NomeClanRank[playerid] = CreateDynamic3DTextLabel(
            string, NT_COLOR_MAIN,
            0.0, 0.0, NT_Z_MAIN,
            NT_DRAW_DISTANCE, playerid
        );

    return 1;
}

