            case 2: {SetPlayerAttachedObject(playerid, SlotBracelete, 15898, 4); EditandoPontoHP[playerid] = 2;}// Medico Auxiliar
            case 3: {SetPlayerAttachedObject(playerid, SlotBracelete, 15899, 4); EditandoPontoHP[playerid] = 3;}// Medico
            case 4: {SetPlayerAttachedObject(playerid, SlotBracelete, 15900, 4); EditandoPontoHP[playerid] = 4;}// Medico Superior
            case 5: {SetPlayerAttachedObject(playerid, SlotBracelete, 15901, 4); EditandoPontoHP[playerid] = 5;}// Medico Chefe
            case 6: {SetPlayerAttachedObject(playerid, SlotBracelete, 15897, 4); EditandoPontoHP[playerid] = 6;}// Medico Aprendiz
            case 7: {SetPlayerAttachedObject(playerid, SlotBracelete, 15898, 4); EditandoPontoHP[playerid] = 7;}// Medico Auxiliar
            case 8: {SetPlayerAttachedObject(playerid, SlotBracelete, 15899, 4); EditandoPontoHP[playerid] = 8;}// Medico
            case 9: {SetPlayerAttachedObject(playerid, SlotBracelete, 15900, 4); EditandoPontoHP[playerid] = 9;}// Medico Superior
            case 10:{SetPlayerAttachedObject(playerid, SlotBracelete, 15901, 4); EditandoPontoHP[playerid] = 10;}// Medico Chefe
            case 11: {SetPlayerAttachedObject(playerid, SlotBracelete, 15897, 4); EditandoPontoHP[playerid] = 11;}// Medico Aprendiz
            case 12: {SetPlayerAttachedObject(playerid, SlotBracelete, 15898, 4); EditandoPontoHP[playerid] = 12;}// Medico Auxiliar
            case 13: {SetPlayerAttachedObject(playerid, SlotBracelete, 15899, 4); EditandoPontoHP[playerid] = 13;}// Medico
            case 14: {SetPlayerAttachedObject(playerid, SlotBracelete, 15900, 4); EditandoPontoHP[playerid] = 14;}// Medico Superior
            case 15: {SetPlayerAttachedObject(playerid, SlotBracelete, 15901, 4); EditandoPontoHP[playerid] = 15;}// Medico Chefe
        }
        EditandoPonto[playerid] = 1;
        Editing[playerid] = 1;
        EditAttachedObject(playerid, SlotBracelete);
        if(Info[playerid][pMember] == 1){QuantidadeMedicoIwaga++;} // Iwagakure
        else if(Info[playerid][pMember] == 3){QuantidadeMedicoKiri++;} // Kirigakure
    }
    else if(PontoBatidoHP[playerid] == 1)
    {
        Editing[playerid] = 0;
        PontoBatidoHP[playerid] = 0;
        EditandoPontoHP[playerid] = 0;
        RemovePlayerAttachedObject(playerid, 1);
        if(Info[playerid][pMember] == 1){QuantidadeMedicoIwaga--;} // Iwagakure
        else if(Info[playerid][pMember] == 3){QuantidadeMedicoKiri--;} // Kirigakure
    }
    return 1;
}
stock SistemaDeProgressoHP(playerid)
{
    new medico = Info[playerid][pHPPatente], progresso = Info[playerid][pProgressoHP];
    if(progresso == 6000) return SendClientMessage(playerid, -1, "{AB7C4E}(AVISO) Voc? n?o consegue mais treinar seu {FFFFFF}Ninjutsu Medico{AB7C4E}.");
    if(PontoBatidoHP[playerid] == 0) return 0;
    if(medico == 1 || medico == 6) return 0;
    //Iwagakure Maximos
    if(medico == 2 && progresso == 600) return SendClientMessage(playerid, -1, "{AB7C4E}(AVISO) Voc? n?o consegue mais treinar seu {FFFFFF}Ninjutsu Medico{AB7C4E}.");
    if(medico == 3 && progresso == 3000) return SendClientMessage(playerid, -1, "{AB7C4E}(AVISO) Voc? n?o consegue mais treinar seu {FFFFFF}Ninjutsu Medico{AB7C4E}.");
    //Kirigakure Maximos
    if(medico == 7 && progresso == 600) return SendClientMessage(playerid, -1, "{AB7C4E}(AVISO) Voc? n?o consegue mais treinar seu {FFFFFF}Ninjutsu Medico{AB7C4E}.");
    if(medico == 8 && progresso == 3000) return SendClientMessage(playerid, -1, "{AB7C4E}(AVISO) Voc? n?o consegue mais treinar seu {FFFFFF}Ninjutsu Medico{AB7C4E}.");
    //Iwagakure
    if(medico == 2 && progresso >= 0 && progresso <= 600){Info[playerid][pProgressoHP]++;} // Medico Auxiliar
    if(medico == 3 && progresso >= 600 && progresso <= 3000){Info[playerid][pProgressoHP]++;} // Medico
    if(medico == 4 && progresso >= 3000 && progresso <= 6000){Info[playerid][pProgressoHP]++;} // Medico Superior
    if(medico == 5){Info[playerid][pProgressoHP]++;}
    // Kirigakure
    if(medico == 7 && progresso >= 0 && progresso <= 600){Info[playerid][pProgressoHP]++;} // Medico Auxiliar
    if(medico == 8 && progresso >= 600 && progresso <= 3000){Info[playerid][pProgressoHP]++;} // Medico
    if(medico == 9 && progresso >= 3000 && progresso <= 6000){Info[playerid][pProgressoHP]++;} // Medico Superior
    if(medico == 10){Info[playerid][pProgressoHP]++;}
    return 1;
}
