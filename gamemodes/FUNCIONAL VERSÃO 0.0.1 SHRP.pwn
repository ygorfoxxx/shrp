stock RemoveItem(playerid, item, cantidad)
{
    new resto = 0;
    for(new x; x<GetBackpackSpace(playerid); x++)
    {
        if(Inventario[playerid][pSave][x] == item)
        {
            Inventario[playerid][pTotalSave][x] -= resto ? resto : cantidad;
            if(Inventario[playerid][pTotalSave][x] <= 0)
            {
                Inventario[playerid][pSave][x] = 0;
                resto = -Inventario[playerid][pTotalSave][x];
            }
            else resto = 0;

            if(!resto) break;
        }
    }
    AtualizarInventario(playerid);
    return 1;
}
