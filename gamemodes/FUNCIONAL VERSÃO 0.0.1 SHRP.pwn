// Verifica se o player pode ser atingido
IsPlayerHittableKageshibari(playerid)
{
    if(HakkeshouON[playerid]) return 0;
    if(AlvoInvestidaAir[playerid]) return 0;
    if(IsPlayerInAir[playerid]) return 0;
    if(HittedKageshibari[playerid]) return 0;
    if(SuirouHIT[playerid]) return 0;
    if(lastHitted[playerid][hitReason] == 10) return 0; // Genjutsu
    if(lastHitted[playerid][hitReason] == 11) return 0; // Investida Samurai
    return 1;
}

// Aplica efeitos nos alvos
function KageshibariCast(playerid)
{
    if(!Kageshibari[playerid][kageshiCast]) return 0;

    for(new i; i<MAX_PLAYERS; i++)
    {
        if(i != playerid && IsPlayerInRangeOfPoint(i, 0.5 + Kageshibari[playerid][kageshiHitBox], Kageshibari[playerid][kageshiPos][0], Kageshibari[playerid][kageshiPos][1], Kageshibari[playerid][kageshiPos][2])
            && GetPlayerVirtualWorld(i) == Kageshibari[playerid][kageshiVW] && !Kageshibari[playerid][kageshiHitted][i] && IsPlayerHittable(i) && IsPlayerHittableKageshibari(i))
        {
            EmCombateIniciar(playerid);
            EmCombateIniciar(i);

            ClicouKageshi[playerid] = 1;
            Kageshibari[playerid][kageshiAtivado] = 1;
            Kageshibari[playerid][kageshiHitted][i] = 1;
            HittedKageshibari[i] = 1;

            FreezePlayer(i);
            ApplyAnimation(i, "Shinobi_Anim", "Nara_01", 1.0, 0, 1, 1, 1, 0, 1);

            TimerAtivadoKageshibari[playerid] = SetTimerEx("KageshibariOff", 800, false, "d", playerid);
        }
    }

    SetTimerEx("KageshibariCast", 50, false, "d", playerid);
    return 0;
}

// Desativa jutsu
function KageshibariOff(playerid)
{
    if(!Kageshibari[playerid][kageshiAtivado]) return 0;
    for(new i; i<MAX_PLAYERS; i++)
    {
        if(Kageshibari[playerid][kageshiHitted][i] == 1)
        {
            KillTimer(TimerAtivadoKageshibari[playerid]);
            Kageshibari[playerid][kageshiCast] = 0;
            Kageshibari[playerid][kageshiAtivado] = 0;
            Kageshibari[playerid][kageshiHitBox] = 0.0;
            lastJutsu[playerid][jutsuLast] = 0;

            UnfreezePlayer(i);
            RemovePlayerAttachedObject(i, 8);
            ApplyAnimation(i, "PED", "KART_drive", 3.0, 0, 1, 1, 0, 0, 1);

            UnfreezePlayer(playerid);
            RemovePlayerAttachedObject(playerid, 9);
            ApplyAnimation(playerid, "Shinobi_Anim", "Nara_01", 1.0, 0, 1, 1, 0, 1, 1);

            Kageshibari[playerid][kageshiHitted][i] = 0;
            HittedKageshibari[i] = 0;
        }
    }
    return 1;
}

// Estouro do jutsu
function EstouroKageshibari(playerid)
{
    KillTimer(TimerAtivadoKageshibari[playerid]);
    if(!Kageshibari[playerid][kageshiAtivado]) return 0;
    for(new i; i<MAX_PLAYERS; i++)
    {
        if(Kageshibari[playerid][kageshiHitted][i] == 1)
        {
            Kageshibari[playerid][kageshiHitted][i] = 0;
            HittedKageshibari[i] = 0;
        }
    }
    return 1;
}

