RegistroMensagem(playerid, Float:radius, string[])
{
    new file[128], file2[128], Arquivo[128], Nome[128], str[300];
    new Float:x, Float:y, Float:z;
    GetPlayerPos(playerid, x, y, z);
    GetPVarString(playerid, "GuardadoName", Arquivo, sizeof(Arquivo));
    format(file, sizeof(file), PASTA_Apresentados, Arquivo);
    format(file2, sizeof(file2), "PlayerApresentados/%s_Conhece.ini", Arquivo);
    if(!DOF2_FileExists(file))
    {
        DOF2_CreateFile(file);
        DOF2_CreateFile(file2);
    }
    else
    {
        if(Info[playerid][pMaskuse] == 1){
            format(str, sizeof(str), "{C2A2DA}Voc? (%d) %s", playerid, string);
            SendClientMessage(playerid, -1, str);
            foreach(Player, i)
            {
                if(playerid != i && IsPlayerInRangeOfPoint(i, radius, x, y, z) && GetPVarInt(i, "Inconsciente") == 0 && GetPlayerVirtualWorld(i) == GetPlayerVirtualWorld(playerid))
                {
                    format(str, sizeof(str), "{C2A2DA}(Mascarado) (%d) %s", playerid, string);
                    SendClientMessage(i, -1, str);
                    SetPlayerChatBubble(playerid,string,COLOR_WHITE,75.0,5000);
                }
            }
        }else{
            format(str, sizeof(str), "{C2A2DA}Voc? (%d) %s", playerid, string);
            SendClientMessage(playerid, -1, str);
            foreach(Player, i)
            {
                if(playerid != i && IsPlayerInRangeOfPoint(i, radius, x, y, z) && GetPVarInt(i, "Inconsciente") == 0 && GetPlayerVirtualWorld(i) == GetPlayerVirtualWorld(playerid))
                {
                    format(Nome, sizeof(Nome), "%s", PlayerNameDados(i));
                    if(DOF2_GetBool(file2, Nome) == true)
                    {
                        format(str, sizeof(str), "{C2A2DA}%s (%d) %s", PlayerNameDados(playerid), playerid, string);
                        SendClientMessage(i, -1, str);
                        SetPlayerChatBubble(playerid,string,COLOR_WHITE,20.0,5000);
                    }
                    else
                    {
                        format(str, sizeof(str), "{C2A2DA}Desconhecido (%d) %s",playerid, string);
                        SendClientMessage(i, -1, str);
                        SetPlayerChatBubble(playerid,string,COLOR_WHITE,75.0,5000);
                    }
                }
            }
        }
    }
    return 1;
}
