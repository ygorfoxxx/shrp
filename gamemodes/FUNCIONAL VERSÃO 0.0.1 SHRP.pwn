            ApplyAnimation(playerid, "Combo_1", "Parado_2", 3.5, 0, 0, 0, 0, 1, 1);
            DarXPPorTreinamento(playerid);
        }else if(GetPVarInt(playerid, "Inconsciente") == 0){
            DarXPPorTreinamento(playerid);
            ApplyAnimation(playerid, "Combo_1", "Parado_1", 3.5, 0, 0, 0, 0, 1, 1);
