AoClicarNoStatsUp(playerid, PlayerText:playertextid)
{
    new string[128];
    if(playertextid == BtnUparStats[playerid][0]) // Vida
    {
        if(Info[playerid][pPuntosNinja] < 1)
        {
            format(string, sizeof(string), "(AVISO) Voc? n?o tem mais pontos suficientes.");
            SendClientMessage(playerid, -1, string);
            Info[playerid][pPuntosNinja] = 0;
        }
        Info[playerid][pPuntosNinja] -= 1;
        Info[playerid][pHealthMaximo] += 10.0;
        new VidaPJ = floatround(Info[playerid][pHealthMaximo], floatround_floor);
        MostrarStatus(playerid); CarregarStatus(playerid);
        return 1;
    }
    else if(playertextid == BtnUparStats[playerid][1]) // Chakra
    {
        if(Info[playerid][pPuntosNinja] < 1)
        {
            format(string, sizeof(string), "(AVISO) Voc? n?o tem mais pontos suficientes.");
            SendClientMessage(playerid, -1, string);
            Info[playerid][pPuntosNinja] = 0;
        }
        Info[playerid][pPuntosNinja] -= 1;
        Info[playerid][pChakra] += 10.0;
        new ChakraPJ = floatround(Info[playerid][pChakra], floatround_floor);
        MostrarStatus(playerid); CarregarStatus(playerid);
        return 1;
    }
    else if(playertextid == BtnUparStats[playerid][2]) // Taijutsu
    {
        if(Meditacao[playerid][meditacaoEspecialista] == 3){
            if(Info[playerid][pPuntosNinja] < 1)
            {
                format(string, sizeof(string), "(AVISO) Voc? n?o tem mais pontos suficientes.");
                SendClientMessage(playerid, -1, string);
                Info[playerid][pPuntosNinja] = 0;
            }
            else if(Info[playerid][pRank] == 0 || Info[playerid][pRank] == 1 && Info[playerid][pTaijutsu] > 63)
            {
                format(string, sizeof(string), "(AVISO) Voc? n?o consegue mais colocar pontos em (Taijutsu).");
                SendClientMessage(playerid, -1, string);
            }
            else if(Info[playerid][pRank] == 2 && Info[playerid][pTaijutsu] > 119)
            {
                format(string, sizeof(string), "(AVISO) Voc? n?o consegue mais colocar pontos em (Taijutsu).");
                SendClientMessage(playerid, -1, string);
            }
            else if(Info[playerid][pRank] == 3 && Info[playerid][pTaijutsu] > 186)
            {
                format(string, sizeof(string), "(AVISO) Voc? n?o consegue mais colocar pontos em (Taijutsu).");
                SendClientMessage(playerid, -1, string);
            }else{
                Info[playerid][pPuntosNinja] -= 1;
                Info[playerid][pTaijutsu] += 1;
                MostrarStatus(playerid); CarregarStatus(playerid);
            }

        }else{SendClientMessage(playerid, -1, "{AB7C4E}(ESPECIALIDADE) Voc? n?o ? especialista em ({FFFFFF}Taijutsu{AB7C4E}) para distribuir pontos em ninjutsu");}
        return 1;
    }
    else if(playertextid == BtnUparStats[playerid][3]) // Ninjutsu
    {
        if(Meditacao[playerid][meditacaoEspecialista] == 1){
            if(Info[playerid][pPuntosNinja] < 1)
            {
                format(string, sizeof(string), "(AVISO) Voc? n?o tem mais pontos suficientes.");
                SendClientMessage(playerid, -1, string);
                Info[playerid][pPuntosNinja] = 0;
            }
            else if(Info[playerid][pRank] == 0 || Info[playerid][pRank] == 1 && Info[playerid][pNinjutsu] > 70)
            {
                format(string, sizeof(string), "(AVISO) Voc? n?o consegue mais colocar pontos em (Ninjutsu).");
                SendClientMessage(playerid, -1, string);
            }
            else if(Info[playerid][pRank] == 2 && Info[playerid][pNinjutsu] > 157)
            {
                format(string, sizeof(string), "(AVISO) Voc? n?o consegue mais colocar pontos em (Ninjutsu).");
                SendClientMessage(playerid, -1, string);
            }
            else if(Info[playerid][pRank] == 3 && Info[playerid][pNinjutsu] > 245)
            {
                format(string, sizeof(string), "(AVISO) Voc? n?o consegue mais colocar pontos em (Ninjutsu).");
                SendClientMessage(playerid, -1, string);
            }else{
                Info[playerid][pPuntosNinja] -= 1;
                Info[playerid][pNinjutsu] += 1;
                MostrarStatus(playerid); CarregarStatus(playerid);
            }
        }else{SendClientMessage(playerid, -1, "{AB7C4E}(ESPECIALIDADE) Voc? n?o ? especialista em ({FFFFFF}Ninjutsu{AB7C4E}) para distribuir pontos em ninjutsu");}
        return 1;
    }
    else if(playertextid == BtnUparStats[playerid][5]) // Genjutsu
    {
        if(Info[playerid][pPuntosNinja] < 1)
        {
            format(string, sizeof(string), "(AVISO) Voc? n?o tem mais pontos suficientes.");
            SendClientMessage(playerid, -1, string);
            Info[playerid][pPuntosNinja] = 0;
        }
        else if(Info[playerid][pRank] == 0 || Info[playerid][pRank] == 1 && Info[playerid][pGenjutsu] > 70)
        {
            format(string, sizeof(string), "(AVISO) Voc? n?o consegue mais colocar pontos em (Genjutsu).");
            SendClientMessage(playerid, -1, string);
        }
        else if(Info[playerid][pRank] == 2 && Info[playerid][pGenjutsu] > 157)
        {
            format(string, sizeof(string), "(AVISO) Voc? n?o consegue mais colocar pontos em (Genjutsu).");
            SendClientMessage(playerid, -1, string);
        }
        else if(Info[playerid][pRank] == 3 && Info[playerid][pGenjutsu] > 245)
        {
            format(string, sizeof(string), "(AVISO) Voc? n?o consegue mais colocar pontos em (Genjutsu).");
            SendClientMessage(playerid, -1, string);
        }
        else{
            Info[playerid][pPuntosNinja] -= 1;
            Info[playerid][pGenjutsu] += 1;
            MostrarStatus(playerid); CarregarStatus(playerid);
        }
        return 1;
    }
    return 0;
