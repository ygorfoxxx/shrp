TeclarUtilizarCombo(playerid, newkeys, oldkeys)
{
    // =======================================================
    //  FINISHER DO AIR COMBO (RMB + F)
    //  - S executa se o finisher estiver ARMADO (AirFinisherReady)
    //  - Input: segurar RMB (KEY_AIM) + apertar F (KEY_F)
    // =======================================================

    if((newkeys & KEY_F) && !(oldkeys & KEY_F))
    {
        // precisa estar segurando o boto direito no exato momento do F
        if(newkeys & KEY_AIM)
        {
            if(AirFinisher_TryExecute(playerid)) return 1;
        }
    }

    // =======================================================
    //  FINISHER DO COMBO NO CHO (RMB + F)
    //  - No pode ativar apenas com F
    // =======================================================

    if(UtilizouComboClick[playerid]) return 0;

    if(AtivadoComboClick[playerid] == 1 && (newkeys & KEY_F) && !(oldkeys & KEY_F))
    {
        // Evita qualquer ativao fora do cho
        if(IsPlayerInAir[playerid] == 1) return 1;

        if(newkeys & KEY_AIM)
        {
            new victimid = ComboTargetID[playerid];
            if(victimid != INVALID_PLAYER_ID && IsPlayerConnected(victimid))
            {
                if(GetPlayerVirtualWorld(playerid) == GetPlayerVirtualWorld(victimid))
                {
                    new Float:vx, Float:vy, Float:vz;
                    GetPlayerPos(victimid, vx, vy, vz);

                    // Alcance curto (mantm feeling original)
                    if(IsPlayerInRangeOfPoint(playerid, 2.8, vx, vy, vz))
                    {
                        ChutaoForteSpeed(victimid, -0.6);
                        SetTimerEx("AtivarChuteForteCombo", 100, false, "d", victimid);

                        ComboTimerResetClick[playerid] = 0;
                        AtivadoComboClick[playerid] = 0;
                        UtilizouComboClick[playerid] = 1;
                        ComboTargetID[playerid] = -1;
                        ComboCount[playerid] = 0;
                    }
                }
