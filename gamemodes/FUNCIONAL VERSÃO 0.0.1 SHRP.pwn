AoPlayerMoverItem(playerid, slot)
{
    if(slot == markedSlot[0][playerid]) return 0;

    new bool:movable = Inventario[playerid][pSave][slot] == 0, fromSlot = markedSlot[0][playerid], item = Inventario[playerid][pSave][slotX[playerid]];
    if(!movable) movable = Inventario[playerid][pSave][slot] == Inventario[playerid][pSave][fromSlot] && EspacioSlot(playerid, slot, Inventario[playerid][pSave][slot], Inventario[playerid][pTotalSave][fromSlot]);
    if(movable)
    {
        GivePlayerItem(playerid, slot, Inventario[playerid][pSave][fromSlot], Inventario[playerid][pTotalSave][fromSlot]);

        Inventario[playerid][pSave][fromSlot] = 0;
        Inventario[playerid][pTotalSave][fromSlot] = 0;

        AtualizarInventario(playerid);

        AtualizarItemDescricao(playerid, Inventario[playerid][pSave][slot], Inventario[playerid][pTotalSave][slot]);
        AbrirDescricao(playerid, 0);
        InventoryAction[playerid] = 0;
        return 1;
    }
    return 0;
}
