stock IsPlayerInRangeOfPlayer(playerid, targetid, Float:range)
{
    new Float:x, Float:y, Float:z;
    new Float:tx, Float:ty, Float:tz;
    if(!IsPlayerConnected(playerid) || !IsPlayerConnected(targetid)) return 0;
    GetPlayerPos(playerid, x, y, z);
    GetPlayerPos(targetid, tx, ty, tz);
    if(GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(targetid)) return 0;
    if(GetPlayerInterior(playerid) != GetPlayerInterior(targetid)) return 0;
    return (floatpower(floatpower(x-tx, 2) + floatpower(y-ty, 2) + floatpower(z-tz, 2), 0.5) <= range);
}

