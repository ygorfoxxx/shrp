function JutsuMarcaMaldicao(playerid)
{
    if(Info[playerid][pSenin] == 0 || Info[playerid][pSenin] == 1 || Info[playerid][pSenin] == 3 || Info[playerid][pSenin] == 4) return 0;
    new str[200], Float:playerPos[3], Float:targetPos[3], vw = GetPlayerVirtualWorld(playerid);
    if(MarcaMaldicao[playerid][marcamalTimer] > gettime()) return JutsuNotReady2(playerid, COLOR_WHITE, "Noroi no Shirushi", MarcaMaldicao[playerid][marcamalTimer]);
    if(Info[playerid][pChakraEnUso] < 350) return SemChakra(playerid);
    ClearSelo(playerid);
    for(new i; i<MAX_PLAYERS; i++)
    {
        switch(GetLookingTarget(1.5, 2.0, Invunerable[i] == 1, false, vw, playerid, i))
        {
            case NONE:
            {
                if(i != MAX_PLAYERS-1) continue;
                format(str, sizeof(str), "{EF0D02}[Noroi no Shirushi] Voc? precisa est? mirando em alguem");
                SendClientMessage(playerid, COLOR_WHITE, str);
                break;
            }
            case PLAYER:
            {
                format(str, sizeof(str), "{EF0D02}[Noroi no Shirushi] O alvo j? tem uma marca da maldi??o");
                if(Info[i][pMarcaDMaldicao] == 1) return SendClientMessage(playerid, COLOR_WHITE, str);
                MarcaMaldicao[playerid][marcamalTimer] = gettime() + MARCAMALDICAO_COOLDOWN;
                format(str, sizeof(str), "* %s morde o pesco?o do %s", PlayerNameDados(playerid), PlayerNameDados(i));
                SendMessageProx(playerid, 25.0, str, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
                format(str, sizeof(str), "* %s cai no ch?o gritando de dor", PlayerNameDados(i));
                SendMessageProx(i, 25.0, str, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
                HittedMarca[i] = 1;
                Info[i][pMarcaDMaldicao] = 1;
                ApplyAnimation(i, "Shinobi_Anim", "Cansado", 4.0, 0, 0, 0, 0, 3000, 1);
                HittedMarcaTimer[playerid] = SetTimerEx("MarcaDaMaldicao", 2500, false, "dd", playerid, i);
                break;
            }
        }
    }
    return 1;
}
