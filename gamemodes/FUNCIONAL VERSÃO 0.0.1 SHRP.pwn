            if(Cogumelos[h][pCoguTipo] == 2 && PegouCogumelos[h] == 0){GivePlayerItemEx(playerid, 39, 1); PegouCogumelos[h] = 1; TimingCogumeloRespawn[h] = gettime() + SHIMEJI_COOLDOWN; GivePlayerExperiencia(playerid, 5); GivePlayerCash(playerid, 5); RyoseXPTxd(playerid, 5, 5); Audio_Play(playerid, 80); SubirDLevel(playerid);}// Shimeji
            if(Cogumelos[h][pCoguTipo] == 3 && PegouCogumelos[h] == 0){GivePlayerItemEx(playerid, 41, 1); PegouCogumelos[h] = 1; TimingCogumeloRespawn[h] = gettime() + SHITAKE_COOLDOWN; GivePlayerExperiencia(playerid, 5); GivePlayerCash(playerid, 5); RyoseXPTxd(playerid, 5, 5); Audio_Play(playerid, 80); SubirDLevel(playerid);}// Shitake
            if(Cogumelos[h][pCoguTipo] == 4 && PegouCogumelos[h] == 0){GivePlayerItemEx(playerid, 40, 1); PegouCogumelos[h] = 1; TimingCogumeloRespawn[h] = gettime() + GYROMITRA_COOLDOWN; GivePlayerExperiencia(playerid, 5); GivePlayerCash(playerid, 5); RyoseXPTxd(playerid, 5, 5); Audio_Play(playerid, 80); SubirDLevel(playerid);}// Gyromitra
