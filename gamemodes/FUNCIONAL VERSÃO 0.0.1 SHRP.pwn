function SeladoPosCorpo(playerid)
{
    if(SealedCorpo[playerid] == 0) return 0;
    SealedCorpo[playerid] = 0;
    new corpoid = GetPVarInt(playerid, "IDDoCorpo"),
        string[128], Aldea[64];
    GetPlayerPos(playerid, CorpoData[corpoid][corpoX], CorpoData[corpoid][corpoY], CorpoData[corpoid][corpoZ]);
    //Corpo
    ActorMorto[CorpoData[corpoid][corpoID]] = CreateDynamicActor(CorpoData[corpoid][corpoSkin], CorpoData[corpoid][corpoX], CorpoData[corpoid][corpoY], CorpoData[corpoid][corpoZ], CorpoData[corpoid][corpoVuneravel], 100.0, CorpoData[corpoid][corpoVW]);
    ApplyDynamicActorAnimation(ActorMorto[CorpoData[corpoid][corpoID]], "ped", "EV_step", 4.1, 0, 0, 0, 1, 1);
    //Status
    format(string, sizeof(string), "{FFFFFF}Status {B85903}Inconsciente");
    CorpoData[corpoid][corpoStatus] = CreateDynamic3DTextLabel(string, COLOR_WHITE, CorpoData[corpoid][corpoX], CorpoData[corpoid][corpoY], CorpoData[corpoid][corpoZ]+0.2, 6);
    //Bandana
    switch(CorpoData[corpoid][corpoVila])
    {
        case 1: Aldea = "{403800}Iwa";
        case 2: Aldea = "{168500}Suna";
        case 3: Aldea = "{030085}Kiri";
        case 4: Aldea = "{403800}Iwa";
        case 5: Aldea = "{FEFF00}Kumo";
        case 6: Aldea = "{DD0200}Renegado";
        case 7: Aldea = "{030085}Renegado";
        case 8: Aldea = "{C5CA49}Myoboku";
        case 9: Aldea = "{C5CA49}Mascarado";
        default: Aldea = "SemBandana";
    }
    format(string, sizeof(string), "{FFFFFF}Bandana %s", Aldea);
    CorpoData[corpoid][corpoBandana] = CreateDynamic3DTextLabel(string, COLOR_WHITE, CorpoData[corpoid][corpoX], CorpoData[corpoid][corpoY], CorpoData[corpoid][corpoZ]+0.1, 6);
    //ID
    format(string, sizeof(string), "(%d)", CorpoData[corpoid][corpoID]);
    CorpoData[corpoid][corpoIDSt] = CreateDynamic3DTextLabel(string, COLOR_WHITE, CorpoData[corpoid][corpoX], CorpoData[corpoid][corpoY], CorpoData[corpoid][corpoZ], 6);

    SalvarCorpo(corpoid);
    return 1;
}
