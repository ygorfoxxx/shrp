#define GENJUTSU_COOLDOWN 60     // em segundos
#define GENJUTSU_BASE_DURACAO 15  // dura??o base em ms (15s)

new PlayerWeather[MAX_PLAYERS];

enum eGenjutsuRinne
{
    genjutsuTimer,
    genjutsuObj,
    genjutsuWeather,
    genjutsuTarget
}
new GenjutsuRinneData[MAX_PLAYERS][eGenjutsuRinne];

stock SetPlayerWeatherEx(playerid, clima)
{
    SetPlayerWeather(playerid, clima);
    PlayerWeather[playerid] = clima;
}

function GenjutsuRinne(playerid)
{
    if(Info[playerid][pSenin] < 4) return 0;

    if(GenjutsuRinneData[playerid][genjutsuTimer] > gettime())
        return JutsuNotReady2(playerid, COLOR_WHITE, "Genjutsu", GenjutsuRinneData[playerid][genjutsuTimer]);

    if(Info[playerid][pChakraEnUso] < 600) return SemChakra(playerid);

    ClearSelo(playerid);

    new str[128];
    new vw = GetPlayerVirtualWorld(playerid);

    for(new i = 0; i < MAX_PLAYERS; i++)
    {
        switch(GetLookingTarget(50.0, 2.0, Invunerable[i] == 0, false, vw, playerid, i))
        {
            case NONE:
            {
                if(i != MAX_PLAYERS-1) continue;
                format(str, sizeof(str), "{EF0D02}[Genjutsu] Voc? precisa mirar em algu?m.");
                SendClientMessage(playerid, COLOR_WHITE, str);
                break;
            }
            case PLAYER:
            {
                GenjutsuRinneData[playerid][genjutsuTimer] = gettime() + GENJUTSU_COOLDOWN;
                GenjutsuRinneData[playerid][genjutsuTarget] = i;

                // guarda clima real
                GenjutsuRinneData[i][genjutsuWeather] = PlayerWeather[i];

                // aplica clima ilus?rio
                SetPlayerWeatherEx(i, 107);

                // aplica anima??es
                ApplyAnimation(playerid, "Shinobi_Anim", "Uchihalvl3", 4.1, 0, 0, 0, 0, 0, 1);
                ApplyAnimation(i, "Shinobi_Anim", "Uchihalvl3_Hit", 3.0, 1, 0, 0, 0, 0, 1);

                // cria objeto no alvo
                new Float:tx, Float:ty, Float:tz;
                GetPlayerPos(i, tx, ty, tz);
                GenjutsuRinneData[i][genjutsuObj] = CreateObject(18668, tx, ty, tz - 1.0, 0.0, 0.0, 0.0);

                AudioInPlayer(playerid, 100.0, 72);

                // CONGELA o alvo
                TogglePlayerControllable(i, 0);

                // aplica dano no usu?rio (rebote)
                SetDamageToPlayer(playerid, playerid, GetDamageToPlayer(playerid, Suiton, 3, 250.0));

                // calcula dura??o baseada no pGenjutsu do usu?rio
                new Float:duracao = GENJUTSU_BASE_DURACAO * (0.0 + Float:Info[playerid][pGenjutsu] * 0.1);

                SetTimerEx("EncerrarGenjutsuRinne", duracao, false, "ii", playerid, i);

                format(str, sizeof(str), "{EF0D02}[Genjutsu] Voc? lan?ou Genjutsu em %s.", PlayerNameDados(i));
                SendClientMessage(playerid, COLOR_WHITE, str);

                SendClientMessage(i, COLOR_WHITE, "{EF0D02}[Genjutsu] Voc? caiu em um Genjutsu! Est? paralisado!");
                break;
            }
        }
    }
    return 1;
}

public EncerrarGenjutsuRinne(casterid, targetid)
{
    if(!IsPlayerConnected(targetid)) return 0;

    ClearAnimations(targetid);

    // restaura controle
    TogglePlayerControllable(targetid, 1);

    // restaura clima salvo
    SetPlayerWeatherEx(targetid, GenjutsuRinneData[targetid][genjutsuWeather]);

    if(GenjutsuRinneData[targetid][genjutsuObj])
    {
        DestroyObject(GenjutsuRinneData[targetid][genjutsuObj]);
        GenjutsuRinneData[targetid][genjutsuObj] = 0;
        ClearAnimations(targetid);
    }

    return 1;
}


