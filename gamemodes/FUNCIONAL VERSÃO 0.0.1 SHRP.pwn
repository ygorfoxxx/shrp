function PontosDeMeditacao(playerid)
{
    new Meditacaoo;
    if(GetPVarInt(playerid, "Inconsciente") >= 1 || PlayerParalisado[playerid] == 1) return 0;
    if(!ComprouTimingMeditacao[playerid]) return SendClientMessage(playerid, -1, "{AB7C4E}(MEDITA??O) Voc? precisa comprar o treinamento para iniciar sua medita??o");
    if(TipoMeditacao[playerid] == 1){
        if(ComprouTimingMeditacao[playerid] && Info[playerid][pRank] == 0 && Info[playerid][pNinjutsu] > 63)
        {
            Meditacaoo = 1;
        }
        else if(ComprouTimingMeditacao[playerid] && Info[playerid][pRank] == 1 && Info[playerid][pNinjutsu] > 63)
        {
            Meditacaoo = 1;
        }
        else if(ComprouTimingMeditacao[playerid] && Info[playerid][pRank] == 2 && Info[playerid][pNinjutsu] > 119)
        {
            Meditacaoo = 1;
        }
        else if(ComprouTimingMeditacao[playerid] && Info[playerid][pRank] == 3 && Info[playerid][pNinjutsu] > 186)
        {
            Meditacaoo = 1;
        }
        else{SendClientMessage(playerid, -1, "{AB7C4E}(MEDITA??O) Voc? precisa est? no maximo de atributo do seu rank para conseguir meditar");}
    }
    if(TipoMeditacao[playerid] == 2){
        if(ComprouTimingMeditacao[playerid] && Info[playerid][pRank] == 0 && Info[playerid][pGenjutsu] >= 0)
        {
             Meditacaoo = 1;
        }
        else if(ComprouTimingMeditacao[playerid] && Info[playerid][pRank] == 1 && Info[playerid][pGenjutsu] >= 0)
        {
             Meditacaoo = 1;
        }
        else if(ComprouTimingMeditacao[playerid] && Info[playerid][pRank] == 2 && Info[playerid][pGenjutsu] > 15)
        {
            Meditacaoo = 1;
        }
        else if(ComprouTimingMeditacao[playerid] && Info[playerid][pRank] == 3 && Info[playerid][pGenjutsu] > 30)
        {
            Meditacaoo = 1;
        }else{SendClientMessage(playerid, -1, "{AB7C4E}(MEDITA??O) Voc? precisa est? no maximo de atributo do seu rank para conseguir meditar");}
    }
    return Meditacaoo;
}
function IniciarMeditacao(playerid)
{
    new str[128];
    if(PontosDeMeditacao(playerid))
    {
        if(Info[playerid][pChakraEnUso] != Info[playerid][pChakra]) return SendClientMessage(playerid, -1, "{AB7C4E}(MEDITA??O) Voc? precisa est? com o chakra completo");
        if(Info[playerid][pChakraEnUso] == Info[playerid][pChakra] && ComprouTimingMeditacao[playerid]){
            if(Meditacao[playerid][meditacaoNinPM] > 45){Meditacao[playerid][meditacaoNinPM] = 45;}
            ApplyAnimation(playerid, "ped", "SEAT_up", 4.0, 0, 0, 0, 1, 0, 1);
            Meditando[playerid] = 1;
            Meditacao[playerid][meditacaoNinP] = 0;
            SortearSeloR(playerid);
        }
    }
    return 1;
}
stock SortearSeloR(playerid)
{
    new str[128];
    new indiceSorteado = random(TOTAL_SELOS);
    SeloSorteadoP[playerid] = indiceSorteado;
    format(str, sizeof(str), "%s", SelosR[indiceSorteado]);
    if(Meditacao[playerid][meditacaoNinPM] > 150){Meditacao[playerid][meditacaoNinPM] = 150;}
    format(str, sizeof(str), "Pressione ''%s'' (%d/%d)",  SelosR[indiceSorteado], Meditacao[playerid][meditacaoNinP], Meditacao[playerid][meditacaoNinPM]);
    PlayerTextDrawSetString(playerid, PressTD[playerid], str);
    PlayerTextDrawShow(playerid, PressTD[playerid]);
    return SelosR[indiceSorteado];
}
function MeditacaoIniciada(playerid, newkeys, oldkeys)
{
    if(Meditando[playerid] == 1 && !HittedDoroji[playerid] && !BtnDireito[playerid])
    {
        new string[128], ChakraTotal = Info[playerid][pChakra], ChakraUso = Info[playerid][pChakraEnUso];
        new seloSorteado;
        if(Meditacao[playerid][meditacaoNinPM] == Meditacao[playerid][meditacaoNinP])
        {
            if(TipoMeditacao[playerid] == 1){// Ninjutsu
                Info[playerid][pNinjutsu]++;
                if(Info[playerid][pRank] == 2 && Info[playerid][pNinjutsu] >= 136){
                    format(string, sizeof(string), "{AB7C4E}(MEDITA??O) Voc? atingiu o maximo do seu ({FFFFFF}Ninjutsu{AB7C4E}) para seu rank.");
                    SendClientMessage(playerid, -1, string);
                    Meditando[playerid] = 0;
                    Meditacao[playerid][meditacaoNinP] = 0;
                    PlayerTextDrawHide(playerid, PressTD[playerid]);
                    Info[playerid][pNinjutsu] = 135;
