Float:GetDamageToPlayer(playerid, type, jutsu, Float:damage)
{
    new Float:Ninjutsu = float(Info[playerid][pNinjutsu]);
    new Float:Taijutsu = float(Info[playerid][pTaijutsu]);
    new Float:Kenjutsu = float(Info[playerid][pKenjutsu]);
    new Float:TaijutsuAnimal = float(Animal[playerid][taijutsuAnimal]);
    switch(type) {
        case KATON: {
            switch(jutsu)
            {
                case 2: damage += Ninjutsu*0.30;
                case 3: damage += Ninjutsu*0.21;
                default: damage += Ninjutsu*0.30;
            }
        }
        case FUTON: {
            switch(jutsu) {
                case 2: damage += Ninjutsu*0.030;
                case 3: damage += Ninjutsu*0.22;
                case 4: damage += Ninjutsu*0.05; // Atsugai
                default: damage += Ninjutsu*0.30;
            }
        }
        case RAITON: {
            switch(jutsu) {
                case 3: damage += Ninjutsu*0.30;
                default: damage += Ninjutsu*0.30;
            }
        }
        case DOTON: {
            switch(jutsu) {
                default: damage += Ninjutsu*0.30;
            }
        }
        case Suiton: {
            switch(jutsu) {
                case 2: damage += Ninjutsu*0.075;
                case 3: damage += Ninjutsu*0.30;
                case 4: damage += Ninjutsu*0.95; // dano total Daikodan
                case 5: damage += Ninjutsu*0.30; // dano Daikodan
                case 6: damage += Ninjutsu*0.15; // Suikoudan
                default: damage += Ninjutsu*0.30;
            }
        }
        case MELEE:
        {
            switch(jutsu)
            {
                case 2: damage += Taijutsu*0.22;
                case 3: damage += Ninjutsu*0.50; // Tsutenkyakuu
                case 4: damage += Ninjutsu*0.30; // Hakkeshou
                case 10: damage += Ninjutsu*0.30; // Kiru no Chakra
                case 11: damage += Ninjutsu*0.90; // Investida Samurai
                case 12: damage += Taijutsu*0.30; // Juuho Soshiken
                case 13: damage += Taijutsu*1.50; // Juuho Soshiken 2
                case 14: damage += TaijutsuAnimal*1.00; // Animal Taijutsu
                case 15: damage += Taijutsu*0.65; // Taijutsu Jinchuriki
                case 16: damage += Taijutsu*0.05; // Kunaii
                case 17: damage += Taijutsu*0.40; // CALCULO DANO AWK
                default: damage += Taijutsu*0.20;
            }
        }
        case CLAN:
        {
            switch(jutsu)
            {
                case 1: damage += (float(Info[playerid][pTaijutsu])+Ninjutsu)*0.15;
            }
        }
        case KENJUTSU:
        {
            switch(jutsu)
            {
                case 4: damage += Kenjutsu*0.05;
                case 7: damage += Kenjutsu*0.05;
                case 8: damage += Kenjutsu*0.05; // Hiramekarai 1 m?o
                case 50: damage += Kenjutsu*0.05; // Hiramekarai 2 m?o
                case 14: damage += Taijutsu*0.05+Info[playerid][pChakra]*0.008;
                case 30: damage += Kenjutsu*0.05; // Saberu
                case 31: damage += Kenjutsu*0.05; // Gunbai
                case 32: damage += Ninjutsu*0.05; // Mesu
                default: damage += Kenjutsu*0.01;
            }
        }
        case MENTOR:
        {
            switch(jutsu)
            {
                case 1: damage += Ninjutsu*0.25; // Rasengan
                case 2: damage += Ninjutsu*0.1; // Minato Rasengan Raijin
                default: damage += Ninjutsu*0.01;
            }
        }
    }
    return damage;
}
