function FecharInventarioLivro(playerid)
{
    FecharStatus(playerid, 2);
    PlayerTextDrawHide(playerid, StatuSnPerfil[playerid][0]); // Background Invetarioo
    PlayerTextDrawHide(playerid, StatuSnPerfil[playerid][1]); // BotaoP
    PlayerTextDrawHide(playerid, StatuSnPerfil[playerid][2]); // BotaoPP
