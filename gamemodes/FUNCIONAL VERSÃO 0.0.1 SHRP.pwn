        format(string, sizeof(string), "(AVISO) Voc? pagou (%d Ryos) de sua mochila.", amount);
        SendClientMessage(playerid, COLOR_WHITE, string);
    }else if(RyosBanco >= amount){//se o jogador n?o tiver dinheiro no bolso ele vai pagar com o dinheiro do banco
        Info[playerid][pAccount] -amount;
        format(string, sizeof(string), "(AVISO) Voc? pagou (%d Ryos) do seu banco.", amount);
        SendClientMessage(playerid, COLOR_WHITE, string);
    }else if(RyosBanco != amount || RyosBolso != amount){//caso n?o tenha dinheiro em nenhum ele vai falar que voc? n?o tem dinheiro
        format(string, sizeof(string), "(AVISO) Voc? n?o possu? (%d Ryos) nem no banco e nem na mochila.", amount);
        SendClientMessage(playerid, COLOR_WHITE, string);
    }
    return 1;
}
