CMD:criarcodigo(playerid, params[])
{
    new codigo, tipo, valor;
    new string[128];
    if(Info[playerid][pAdminZC] == 0 || Info[playerid][pAdminZC] == 2 || Info[playerid][pAdminZC] == 3 ||Info[playerid][pAdminZC] == 4) return SendClientMessage(playerid, 0xFF0000AA, "[ERRO]: Este comando n?o existe!");
    if(sscanf(params, "dd", codigo, tipo)) return SendClientMessageEx(playerid, COLOR_WHITE, "[INFO]: Use /criarcodigo [CODIGO] [TIPO]");
    {
        InfoGolds[codigo][CodigoUsado] = 0; // Se ja foi usado ou n?o
        InfoGolds[codigo][CodigoGold] = codigo; // Codigo
        switch(tipo)
        {
            case 0: valor = 1500;
            case 1: valor = 2350;
            case 2: valor = 3350;
            case 3: valor = 5350;
            case 4: valor = 11300;
        }
        InfoGolds[codigo][ValorCodigoGolds] = valor; // Valor do codigo
        SalvarCodigo(codigo);
        format(string, sizeof(string), "[AVISO]: Voc? criou o codigo %d tipo %d com o valor %d", codigo, tipo, valor);
        SendClientMessage(playerid, COLOR_WHITE, string);
    }
    return 1;
}
function SalvarCodigo(idcodigo)
{
    new string[128];
    format(string, sizeof(string), PASTA_GOLDS, idcodigo);
    if(!INI_Exist(string))
    {
        new INI:File = INI_Open(string);
        INI_SetTag(File, "GoldsDATA");
        INI_WriteInt(File, "CodigoUsado", InfoGolds[idcodigo][CodigoUsado]);
        INI_WriteInt(File, "CodigoGold", InfoGolds[idcodigo][CodigoGold]);
        INI_WriteInt(File, "ValorCodigoGolds", InfoGolds[idcodigo][ValorCodigoGolds]);
        INI_Close(File);
    }
    return 1;
}
