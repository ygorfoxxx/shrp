            return 1;
        }
        else if(SeloSorteadoP[playerid] == 1 && PRESSED(newkeys, KEY_JUMP)) // Tigre
        {
            SortearSeloR(playerid);
            Meditacao[playerid][meditacaoNinP]++;
            Audio_Play(playerid, 117);
            return 1;
        }
        else if(SeloSorteadoP[playerid] == 2 && PRESSED(newkeys, KEY_CROUCH)) // Drag?o
        {
            SortearSeloR(playerid);
            Meditacao[playerid][meditacaoNinP]++;
            Audio_Play(playerid, 117);
            return 1;
        }
        else if(SeloSorteadoP[playerid] == 3 && PRESSED(newkeys, KEY_NO)) // Cobra
        {
            SortearSeloR(playerid);
            Meditacao[playerid][meditacaoNinP]++;
            Audio_Play(playerid, 117);
            return 1;
        }
