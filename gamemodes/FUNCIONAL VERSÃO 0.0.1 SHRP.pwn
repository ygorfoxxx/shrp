timer PauseOn[1000]()
{
    foreach(Player, i)
    {
        if(PlayerIsLogado[i] == 1)
        {
            if(!IsPlayerPaused(i))
            {
                if(Pausado[i] == 4)
                {
                    TimingPausado[i] = SetTimerEx("PlayerIsPaused", 500, false, "i", i);
                }
                else
                {
                    Pausado[i] = 3; //Jogador n?o est? pausado
                    TimingPausado[i] = SetTimerEx("PlayerIsPaused", 500, false, "i", i);
                }
            }
            else
            {
                if(Pausado[i] == 2)
                {
                    TimingPausado[i] = SetTimerEx("PlayerIsPaused", 500, false, "i", i);
                }
                else
                {
                    Pausado[i] = 1; //Jogador est? pausado
                    TimingPausado[i] = SetTimerEx("PlayerIsPaused", 500, false, "i", i);
                }
            }
        }
    }
}
function PlayerIsPaused(playerid)
{
    if(Pausado[playerid] == 4) return KillTimer(TimingPausado[playerid]); // aqui ele vai retornar para nao deixar em loop
    if(Pausado[playerid] == 2) return KillTimer(TimingPausado[playerid]);    // aqui ele vai retornar para nao deixar em loop
    if(Pausado[playerid] == 1 && KawarimiUsado[playerid] == 0)
    {
        //Aqui dentro ele ficara com o status de ausente
        Pausado[playerid] = 2;
        Status[playerid] = 4;
        StatusTimerC[playerid] = SetTimerEx("SistemaBandanaIDStatus", 100, false, "i", playerid);
        KillTimer(TimingPausado[playerid]);
    }
    if(Pausado[playerid] == 3 && KawarimiUsado[playerid] == 0)
    {
        //Aqui dentro ele volta ao status do 3 pontinho
        Pausado[playerid] = 4;
        Status[playerid] = 0;
        StatusTimerC[playerid] = SetTimerEx("SistemaBandanaIDStatus", 100, false, "i", playerid);
        KillTimer(TimingPausado[playerid]);
    }
    return 1;
}
