        format(str, sizeof(str), "Arte S?bia: Espiral de Vento!");
        }else{
        format(str, sizeof(str), "Estilo Vento: Espiral de Vento!");
        }
       // format(str, sizeof(str), "Estilo Vento: Espiral de Vento");
        MensagemJutsu(playerid, 20.0, str);
        //lastJutsu[playerid][jutsuLast] = 6; // Hanachi
        Info[playerid][pChakraEnUso] -= 300.0;
        //Cooldown Rasengan
        Rasengan[playerid][RasenganTimer] = gettime() + RASENGAN_COOLDOWN;
