function SustJutsuOff(playerid)
{
    // ==========================================================
    // FIX KAWARIMI: garante que o player nunca fique "travado"
    // - Libera controles (caso tenha ficado com TogglePlayerControllable(0))
    // - Desativa flags que bloqueiam pulo/jutsu (KawarimiUsado/KawarimiUse)
    // - Restaura skin de forma segura (sem depender de PVar)
    // ==========================================================
    if(!IsPlayerConnected(playerid)) return 0;

    if(KawarimiUseTimer[1][playerid])
    {
        KillTimer(KawarimiUseTimer[1][playerid]);
        KawarimiUseTimer[1][playerid] = 0;
    }

    // Sempre limpar primeiro (mesmo se alguma fun??o abaixo falhar)
    KawarimiUsado[playerid] = 0;
    KawarimiUse[playerid]   = 0;

    if(GetPVarInt(playerid, "NNRP_GSTUN") >= 1) DeletePVar(playerid, "NNRP_GSTUN");
    TogglePlayerControllable(playerid, 1);

    // Restaura a skin: prioridade -> SkinSust -> pCharKawarimi -> pChar
    // SA-MP 0.3.DL: skins custom usam IDs >= 20000 (AddCharModel)
    new skinRestore = SkinSust[playerid];
    if(skinRestore <= 0 || (skinRestore > 311 && skinRestore < 20000)) skinRestore = Info[playerid][pCharKawarimi];
    if(skinRestore <= 0 || (skinRestore > 311 && skinRestore < 20000)) skinRestore = Info[playerid][pChar];
    if(skinRestore <= 0 || (skinRestore > 311 && skinRestore < 20000)) skinRestore = 299; // fallback seguro

    SetPlayerSkin(playerid, skinRestore);
    // 0.3.DL: for?a re-aplicar a skin custom depois de um pequeno delay
    // (evita casos em que o modelo "some" ao voltar de uma skin normal)
    if(skinRestore >= 20000)
    {
        SetTimerEx("NNRP_Reskin", 250, false, "ii", playerid, skinRestore);
    }
    DeletePVar(playerid, "SkinSust");

    if(CharakNoSaberuUse[playerid] == 1)
    {
        SetPlayerAttachedObject(playerid, 4, 15411, 5, 0.1, 0.03, 0.0, 164.7, 0.0, 83.7);
        SetPlayerAttachedObject(playerid, 5, 15411, 6, 0.1, 0.03, 0.0, 9.7, 0.0, 83.7);
        Streamer_Update(playerid);

        Ponertoys(playerid);
        ApplyAnimation(playerid, "selos", "rato", 4.0, 0, 0, 0, 0, 1, 1);
        SistemaBandanaIDStatus(playerid);
    }
    else
    {
        ColocarMesu(playerid);
        ColocarJutsuAgua(playerid);
        ColocarBolsaRoubo(playerid);
        ColocarPerga(playerid);
        ColocarMascara(playerid);
        ColocarChapeuKage(playerid);
        ColocarArma(playerid);
        ColocarAcessorioNoPlayer(playerid);

        Ponertoys(playerid);
        ApplyAnimation(playerid, "selos", "rato", 4.0, 0, 0, 0, 0, 1, 1);
        SistemaBandanaIDStatus(playerid);
    }

    foreach(Player, i)
    {
        ShowPlayerNameTagForPlayer(i, playerid, 1);
    }
    return 1;
}


