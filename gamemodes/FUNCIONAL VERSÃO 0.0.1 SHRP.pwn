        DataPlayerCombateSL[playerid][dataPlayerInCombateAnim] = 0;
        DataPlayerCombateSL[playerid][dataPlayerInCombate] = 0;
        DataPlayerCombateSL[playerid][dataPlayerInCombateTecla] = 0;
