            PergaminhoTerra[playerid] = 0; PergaminhoCeu[playerid] = 0;
            RemovePlayerAttachedObject(playerid, 5); RemovePlayerAttachedObject(playerid, 8);
            Info[playerid][pChuninIns] = 0;
        }
        TimerC[playerid] = SetTimerEx("DelTxd", 250, false, "d", playerid);
        TextDrawHideForAll(TimingExame[0]);
        //Info[playerid][pChuninIns] = 3;
        return 1;
    }
    TimerS[playerid] = SetTimerEx("TempoSegundaFase", 950, false, "d", playerid);
    return 1;
