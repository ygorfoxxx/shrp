RaikyuuCast(playerid, newkeys)
{
    new Float:aO[3] = { 45.0, -45.0, 0.0 }, Float:zO[3] = { 0.0, 0.0, 0.5 };
    if(newkeys & KEY_FIRE)
    {
        for(new x; x<3; x++)
        {
            if(IsValidObject(Raikyuu[playerid][raiEffect][x]))
            {
                DestroyObject(Raikyuu[playerid][raiEffect][x]);
                Raikyuu[playerid][raiEffect][x] = INVALID_OBJECT_ID;
                Raikyuu[playerid][raiObject][x] = LanzarJutsu2(playerid, 15100, -0.5, 70.0, 35.0, aO[x], zO[x]);
                if(x == 0){IsRaikyuuHitted1(playerid); TimerRaikyuu[playerid][0] = SetTimerEx("RaikyuuOff1Lanzado", 850, 0, "d", playerid);}
                if(x == 1){TimerRaikyuu[playerid][1] = SetTimerEx("RaikyuuOff2Lanzado", 850, 0, "d", playerid);}
                if(x == 2){TimerRaikyuu[playerid][2] = SetTimerEx("RaikyuuOff3Lanzado", 850, 0, "d", playerid);}
                IsRaikyuuHitted(playerid, x);
                AudioInPlayer(playerid, 30.0, 8);
                RaikyuuLanzar[playerid]++;
                AudioInPlayer(playerid, 10.0, 71);
                ApplyAnimation(playerid,"selos","coelho",4.0, 0, 0, 0, 0, 0, 1);
                return 1;
            }
        }
    }
    else if(RaikyuuLanzar[playerid] == 0 && newkeys & KEY_HANDBRAKE){CastRaikyuuTodos(playerid);}
    return 0;
}
