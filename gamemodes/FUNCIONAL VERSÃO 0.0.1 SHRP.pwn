            ObjectPuloE(playerid);
        }
        if(Info[playerid][pNinjaFraturado] == 2)
        {TimerDanoFraturaPulo[playerid] = SetTimerEx("SetarFraturaJump", 300, false, "i", playerid);}
    }
    else if(DoubleJump[playerid] == 1 && IsPlayerInAir[playerid] == 1) // ? Segundo Pulo (mais confi?vel)
	{
	Treinamento(playerid, 5);
    // ? consome o double aqui (nunca deixa sobrar)
    DoubleJump[playerid] = 0;
    // ? mata timers que sobrescrevem anima??o (principalmente ResetIsAnimJump)
    if(TimingJumpAnim[playerid]) { KillTimer(TimingJumpAnim[playerid]); TimingJumpAnim[playerid] = 0; }
    IsPlayerInAir[playerid] = 1;
    SetXYPlayerVelocity2(playerid, 0.4);
    EstaminaJump(playerid);
    IsJumping[playerid] = 2; // mant?m seu comportamento anti-spam
    // ? N?O aplica fall_fall antes. Mostra o launch com tempo > 0
    ApplyAnimation(playerid, "ped", "JUMP_Launch_D", 4.0, 0, 0, 0, 0, 280, 1);
    AudioInPlayer(playerid, 15.0, 1);
    TimerDoublePulo(playerid);
    ObjectPuloE(playerid);
    if(Info[playerid][pNinjaFraturado] == 2)
    {TimerDanoFraturaPulo[playerid] = SetTimerEx("SetarFraturaJump", 300, false, "i", playerid);}
}
    else if(VX == 0 && VY == 0 && VZ == 0) // Pulo Neutro
    {
    TaijutsuVar[playerid][taiHits] = 0;
    TaijutsuAirHit[playerid] = 0;
    IsJumping[playerid] = 1;          // ? FALTAVA
    DoubleJump[playerid] = 1;         // ? FALTAVA (permite 2? pulo correto)
    IsPlayerInAirP[playerid] = 1;
    Treinamento(playerid, 5);
    AudioInPlayer(playerid, 15.0, 1);
    EstaminaJump(playerid);
    JumpParado[playerid] = 1;
    IsPlayerInAir[playerid] = 1;
    TimerJumpPOnPlayerUpdate[playerid] = SetTimerEx("UpdateJumpParado", 50, false, "i", playerid);
    TimingJumpP[playerid] = SetTimerEx("ResetJumpParado", 500, false, "i", playerid);
    // ? mant?m o mesmo ?fall_fall de seguran?a? do 1? pulo tamb?m
    TimingJumpAnim[playerid] = SetTimerEx("ResetIsAnimJump", 300, false, "i", playerid);
    ObjectPuloE(playerid);
    if(Info[playerid][pNinjaFraturado] == 2)
    {TimerDanoFraturaPulo[playerid] = SetTimerEx("SetarFraturaJump", 500, false, "i", playerid);}
}
    return 1;
}
function ObjectPuloE(playerid)
{
    new Float:ninjaPos[3];
    GetPlayerPos(playerid, ninjaPos[0], ninjaPos[1], ninjaPos[2]);
    if(SL_IsValidObject(JumpEffect[playerid]))
    {
        SL_DestroyObject(JumpEffect[playerid]);
        JumpEffect[playerid] = SL_CreateObject(15125, ninjaPos[0], ninjaPos[1], ninjaPos[2]-0.6, 0.0, 0.0, 0.0, 300.0);
    }else{JumpEffect[playerid] = SL_CreateObject(15125, ninjaPos[0], ninjaPos[1], ninjaPos[2]-0.6, 0.0, 0.0, 0.0, 300.0);}
    TimerEffectJump[playerid] = SetTimerEx("DestruirEffectPulo", 500, false, "d", playerid);
    return 1;
}
function DestruirEffectPulo(playerid)
{
    KillTimer(TimerEffectJump[playerid]);
    if(SL_IsValidObject(JumpEffect[playerid])){SL_DestroyObject(JumpEffect[playerid]);}
}
function UpdateJump(playerid)
{
    if(!IsJumping[playerid]) return 0;
    KillTimer(TimerOnPlayerUpdate[playerid]);
    if(IsJumping[playerid] == 1){OnPlayerUpdate(playerid);}
    SetTimerEx("UpdateJump", 50, false, "i", playerid);
    return 1;
}
function UpdateJumpParado(playerid)
{
    if(!JumpParado[playerid]) return 0;
    KillTimer(TimerJumpPOnPlayerUpdate[playerid]);
    if(JumpParado[playerid] == 1){OnPlayerUpdate(playerid);}
    SetTimerEx("UpdateJumpParado", 50, false, "i", playerid);
    return 1;
}
function ReleaseJump(playerid)
{
    if(IsJumping[playerid] == 2) return 0;
    JumpParado[playerid] = 0;
    IsJumping[playerid] = 2;
    ApplyAnimation(playerid, "ped", "fall_fall", 4.0, 0, 0, 0, 0, 2500, 1);
    return 1;
}
function ResetIsJump(playerid)
{
    KillTimer(TimingJump[playerid]);
    IsJumping[playerid] = 2;
    JumpParado[playerid] = 0;
    return 1;
}
function ResetIsAnimJump(playerid)
{
    KillTimer(TimingJumpAnim[playerid]);
    TimingJumpAnim[playerid] = 0;
    // ? Se j? gastou o double (ou j? est? no 2? est?gio), N?O sobrescreve anima??o
    if(DoubleJump[playerid] == 0) return 1;
    ApplyAnimation(playerid, "ped", "fall_fall", 4.0, 0, 0, 0, 0, 2500, 1);
    return 1;
}
function ResetJumpParado(playerid)
{
    KillTimer(TimingJumpP[playerid]);
    JumpParado[playerid] = 0;
    return 1;
}
function EstaminaJump(playerid)
{
    switch(Info[playerid][pRank])
    {
        case 0:{Info[playerid][pEnergiaEmUso] -= 5.0;}
        case 1:{Info[playerid][pEnergiaEmUso] -= 5.0;}
        case 2:{Info[playerid][pEnergiaEmUso] -= 9.0;}
        case 3:{Info[playerid][pEnergiaEmUso] -= 10.0;}
    }
    return 1;
}
