stock GivePlayerItemEx(playerid, item, cantidad, noslot = -1)
{
    if(TieneEspacio(playerid) || TieneItem(playerid, item, cantidad))
    {
        new slot;
        for(new i; i<GetBackpackSpace(playerid); i++)
        {
            slot = i;
            if(Inventario[playerid][pSave][i] == 0 && KeyItem(item))
            {
                Inventario[playerid][pSave][i] = item;
                Inventario[playerid][pTotalSave][i] = cantidad;
                break;
            }
            else if(Inventario[playerid][pSave][i] == item && (Inventario[playerid][pTotalSave][i]+cantidad) <= 0 && !KeyItem(item))
            {
                Inventario[playerid][pSave][i] = 0;
                Inventario[playerid][pTotalSave][i] = 0;
                break;
            }
            else if(Inventario[playerid][pSave][i] == item && EspacioSlot(playerid, i, item, cantidad) && !KeyItem(item) && i != noslot)
            {
                Inventario[playerid][pTotalSave][i] += cantidad;
                break;
            }
            else if(Inventario[playerid][pSave][i] == 0 && cantidad > 0)
            {
                Inventario[playerid][pSave][i] = item;
                Inventario[playerid][pTotalSave][i] = cantidad;
                break;
            }
        }
        AtualizarInventario(playerid);
        return slot;
    }
    else SendClientMessageEx(playerid, COLOR_WHITE, "Seu inventario se encontra cheio.");
    SalvarConta(playerid);
    return 0;
}
