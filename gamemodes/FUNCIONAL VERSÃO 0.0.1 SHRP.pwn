function StartRaijin(playerid)
{
    ApplyAnimation(playerid, "Mtr_mnt", "Minato_2.3", 4.1, 0, 0, 0, 1, 0, 1);
    SetPlayerAttachedObject(playerid, 7, 15113, 6, 0.094899, 0.022900, 0.001200, 0.0000, 0.0000, 0.0000);//Criar o objeto na m?o
    PlayerRaijinVoador[playerid][raijinTempo] = 2;
    return 1;
}
function ErrouAlvo(playerid)
{
    if(PlayerRaijinVoador[playerid][raijinAtingiu] == 1) return 0;
    DestroyDynamicObject(PlayerRaijinVoador[playerid][raijinKunaiObj]);
    RemovePlayerAttachedObject(playerid, 7);
    PlayerRaijinVoador[playerid][raijinUsado] = 0;
    PlayerRaijinVoador[playerid][raijinTempo] = 0;
    ClearAnimations(playerid);
    return 1;
}
function StartAvanco(playerid, targetid)
{
    if(PlayerRaijinVoador[playerid][raijinTempo] == 0) return 0;
    if(PlayerRaijinVoador[playerid][raijinTarget] != -1 && PlayerRaijinVoador[playerid][raijinTempo] >= 2){
        SetPlayerVelocity(playerid, 0.0, 0.0, 0.01);
        if(PlayerRaijinVoador[playerid][raijinAnimAvanco] == 0){
            DestroyDynamicObject(ObjTeleportRaijin[playerid][0]);
            ObjTeleportRaijin[playerid][1] = CreateDynamicObject(15115, PlayerRaijinVoador[playerid][raijinPosTarget][0], PlayerRaijinVoador[playerid][raijinPosTarget][1], PlayerRaijinVoador[playerid][raijinPosTarget][2]+3.0, 0.0, 0.0, 0.0, -1);
            SetPlayerPos(playerid, PlayerRaijinVoador[playerid][raijinPosTarget][0], PlayerRaijinVoador[playerid][raijinPosTarget][1], PlayerRaijinVoador[playerid][raijinPosTarget][2]+3.0);
            PlayerRaijinVoador[playerid][raijinAnimAvanco] = 1;
            ApplyAnimation(playerid, "Mtr_mnt", "Minato_4", 4.1, 0, 0, 0, 1, 0, 1);
        }
        if(PlayerRaijinVoador[playerid][raijinTempo] == 10 && PlayerRaijinVoador[playerid][raijinAnimAvanco] == 1)
        {
            DestroyDynamicObject(ObjTeleportRaijin[playerid][1]);
            AudioInPlayer(targetid, 35.0, 141);
            SetPlayerAttachedObject(playerid, 4, 15116, 1, 1.050999, 0.000000, 0.000000, 82.399955, -99.700134, 86.400016, 1.0, 1.0, 1.0);
            SetPlayerAttachedObject(playerid, 5, 15117, 1, 1.209998, 0.175000, 0.466999, -87.100067, -87.100120, -108.199829, 1.0, 1.0, 1.0);
        }
        else if(PlayerRaijinVoador[playerid][raijinTempo] == 20 && PlayerRaijinVoador[playerid][raijinAnimAvanco] == 1){
            PlayerRaijinVoador[playerid][raijinDamage] = 1;
            DmgRa(playerid, targetid);
            PlayerRaijinVoador[playerid][raijinRasenganObj] = CreateDynamicObject(15114, PlayerRaijinVoador[playerid][raijinPosTarget][0], PlayerRaijinVoador[playerid][raijinPosTarget][1], PlayerRaijinVoador[playerid][raijinPosTarget][2], 0.0, 0.0, 0.0, -1);
            SetTimerEx("OffRaijin", 250, false, "d", playerid);
        }
        //SendClientMessage(playerid, -1, "START AVANCO");
        PlayerRaijinVoador[playerid][raijinTempo]++;
    }
    SetTimerEx("StartAvanco", 50, false, "dd", playerid, targetid);
    return 1;
}
function DmgRa(playerid, targetid)
{
    if(PlayerRaijinVoador[playerid][raijinDamage] == 0) return 0;
    ApplyAnimation(targetid, "Mtr_mnt", "Hit_1", 4.1, 0, 0, 0, 1, 0, 1);
    SetDamageToPlayer(playerid, targetid, GetDamageToPlayer(playerid, MENTOR, 1, 10.0));
