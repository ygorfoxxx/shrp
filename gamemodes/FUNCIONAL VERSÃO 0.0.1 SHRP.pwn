function AdjustExpirationDate(playerid)
{
    new daysToAdd, currentDay, currentMonth, currentYear, daysInMonth;

    // Obter a data atual
    getdate(currentYear, currentMonth, currentDay);

    // Obter o n?mero de dias no m?s atual
    daysInMonth = GetDaysInMonth(currentYear, currentMonth);

    // Calcular quantos dias precisamos adicionar para ajustar a data
    daysToAdd = JogadorVip[playerid][playerVIPExpirationDay] - currentDay;

    // Se os dias a serem adicionados forem maiores do que os dias restantes no m?s atual,
    // ajustamos o m?s e o ano conforme necess?rio
    if (daysToAdd > daysInMonth)
    {
        daysToAdd -= daysInMonth;
        JogadorVip[playerid][playerVIPExpirationMonth]++;

        if (JogadorVip[playerid][playerVIPExpirationMonth] > 12)
        {
            JogadorVip[playerid][playerVIPExpirationMonth] = 1;
            JogadorVip[playerid][playerVIPExpirationYear]++;
        }

        // Obter o n?mero de dias no novo m?s
        daysInMonth = GetDaysInMonth(JogadorVip[playerid][playerVIPExpirationYear], JogadorVip[playerid][playerVIPExpirationMonth]);
    }

    // Atualizar o dia da data de expira??o com o valor corrigido
    if(daysToAdd == 15){JogadorVip[playerid][playerVIPExpirationDay] = daysToAdd-4;}
    if(daysToAdd == 30){JogadorVip[playerid][playerVIPExpirationDay] = daysToAdd-2;}
}
stock GetDaysInMonth(year, month)
{
    static const daysPerMonth[12] = {31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};
    new days = daysPerMonth[month - 1];
    if (month == 2 && (year % 4 == 0 && (year % 100 != 0 || year % 400 == 0)))
        days = 29; // O ano ? bissexto, fevereiro tem 29 dias
    return days;
}
function ExpireVIP(playerid)
{
    new currentDay, currentMonth, currentYear;
    getdate(currentYear, currentMonth, currentDay);
    if(JogadorVip[playerid][playerVIP] && JogadorVip[playerid][playerVIPExpirationDay] == currentDay && JogadorVip[playerid][playerVIPExpirationMonth] == currentMonth){
        JogadorVip[playerid][playerVIP] = 0;
        JogadorVip[playerid][playerVIPTipo] = 0;
        JogadorVip[playerid][playerVIPExpirationYear] = 0;
        JogadorVip[playerid][playerVIPExpirationMonth] = 0;
        JogadorVip[playerid][playerVIPExpirationDay] = 0;
        new str[128];
        format(str, sizeof(str), "{AB7C4E}(CARTA-VIP) Sua carta vip expirou.");
        SendClientMessage(playerid, -1, str);
    }
}
