    if(Amarrado[playerid] == 1 || RasenganAt[playerid] == 1 || Info[playerid][pNinjaQuebrado] == 1 || Info[playerid][pNinjaQuebrado] == 3) return 0;
    if(Info[playerid][pDentroDeCasa] == 1 || lastJutsu[playerid][jutsuLast] >= 1 || Info[playerid][pPrisaoBase] == 1 || ByakuganON[playerid] == 1) return 0;
    if(KawarimiUsado[playerid] == 1 || lastHitted[playerid][hitReason] > 1 || PlayerStumWait[playerid] == 1 || TsutenHittedAlvo[playerid] == 1) return 0;
    if(BtnDireito[playerid] == 1 || KagutsuchiON[playerid] == 1 || IkazuchiHitAlvo[playerid] == 1 || HittedDoroji[playerid] == 1 || HittedKageshibari[playerid] == 1 || GetPVarInt(playerid, "Inconsciente") >= 1 || HakkeshouON[playerid] == 1 || UsandoKit[playerid] == 1 ||
       EstaRoubando[playerid] == 1 || SeladoS[playerid] == 1 || SuirouHIT[playerid] == 1 || SuirouON[playerid] == 1 || ChuteCimaHitted[playerid] == 1 || PlayerParalisado[playerid] == 1 || SetPlayerState[playerid] == 1 || Dormido[playerid] == 1 || DormindoNoColchonete[playerid] == 1) return 0;
