new gLastTestObj[MAX_PLAYERS] = { INVALID_OBJECT_ID, ... };

CMD:obj(playerid, params[])
{
    // Se usar /obj sem nada -> remove o ?ltimo
    if(params[0] == '\0')
    {
        if(gLastTestObj[playerid] == INVALID_OBJECT_ID)
            return SendClientMessage(playerid, -1, "Use: /obj <modelid>  |  ou /obj (sem id) para remover o ultimo.");

        #if defined CreateDynamicObject
            DestroyDynamicObject(gLastTestObj[playerid]);
        #else
            DestroyObject(gLastTestObj[playerid]);
        #endif

        gLastTestObj[playerid] = INVALID_OBJECT_ID;
        SendClientMessage(playerid, -1, "Objeto removido.");
        return 1;
    }

    // Tem par?metro -> tenta ler modelid
    new modelid = strval(params);
    if(modelid < 100 || modelid > 20000) // faixa s? pra evitar lixo (ajuste se quiser)
        return SendClientMessage(playerid, -1, "Modelid invalido. Ex: /obj 19300");

    // Se j? existe um, remove antes
    if(gLastTestObj[playerid] != INVALID_OBJECT_ID)
    {
        #if defined CreateDynamicObject
            DestroyDynamicObject(gLastTestObj[playerid]);
        #else
            DestroyObject(gLastTestObj[playerid]);
        #endif
        gLastTestObj[playerid] = INVALID_OBJECT_ID;
    }

    // Cria na frente do player
    new Float:x, Float:y, Float:z, Float:a;
    GetPlayerPos(playerid, x, y, z);
    GetPlayerFacingAngle(playerid, a);

    // offset simples pra ficar ? frente
    new Float:ox = x + (1.5 * floatsin(-a, degrees));
    new Float:oy = y + (1.5 * floatcos(-a, degrees));
    new Float:oz = z;

    #if defined CreateDynamicObject
        // Streamer (respeita VW/Interior)
        new vw = GetPlayerVirtualWorld(playerid);
        new interior = GetPlayerInterior(playerid);

        gLastTestObj[playerid] = CreateDynamicObject(
            modelid,
            ox, oy, oz,
            0.0, 0.0, a,
            vw, interior,
            -1,              // playerid (deixa -1)
            200.0, 200.0     // stream/draw distance
        );
    #else
        // SA-MP padr?o (global, n?o respeita VW)
        gLastTestObj[playerid] = CreateObject(modelid, ox, oy, oz, 0.0, 0.0, a, 200.0);
    #endif

    if(gLastTestObj[playerid] == INVALID_OBJECT_ID)
        return SendClientMessage(playerid, -1, "Falhou ao criar o objeto (limite ou model inv?lido).");

    new msg[96];
    format(msg, sizeof msg, "Objeto criado: model %d (id %d). Use /obj para remover.", modelid, gLastTestObj[playerid]);
    SendClientMessage(playerid, -1, msg);
    return 1;
}

