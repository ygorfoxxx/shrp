function CheckWater(playerid)
{
    if(DaikodanHitAlvo[playerid] == 1 || HittedDoroji[playerid] == 1 || HittedKageshibari[playerid] == 1 || lastHitted[playerid][hitReason] > 1 || AlvoInvestidaAir[playerid] == 1 || HakkeshouON[playerid] == 1) return 0;
    if(IsJumping[playerid] == 1 || IsPlayerInAir[playerid] == 1 || ChidoriON[playerid] == 2 || SuirouON[playerid] == 1 || ChuteCimaHitted[playerid] == 1 || PlayerParalisado[playerid] == 1 || SetPlayerState[playerid] == 1 || SuirouHIT[playerid] == 1 || TsutenHittedAlvo[playerid] == 1 || KatonHIT[playerid] == 1) return 0;
    new Float:ninjaPos[3];
    GetPlayerPos(playerid, ninjaPos[0], ninjaPos[1], ninjaPos[2]);
    if(AreaKinobori[playerid] == 0 && ninjaPos[2] >= 1.0 && PlayerIsJutsuWater[playerid] == 1){
        //Normal
        TimerCheckWater[playerid] = 0;
        PlayerIsJutsuWater[playerid] = 0;
        DestroyObject(KinoboriObject[playerid]);
        KinoboriObject[playerid] = INVALID_OBJECT_ID;
        RemovePlayerAttachedObject(playerid, 6);
        RemovePlayerAttachedObject(playerid, 7);
        SendClientMessageEx(playerid, COLOR_WHITE, "{2826FE}[INFO-CHAKRA]{FFFFFF} Voc? ficou muito tempo fora da agua por isso seu jutsu foi desativado.");
    }
    else if(AreaKinobori[playerid] == 1 && ninjaPos[2] >= 29.0 && PlayerIsJutsuWater[playerid] == 1){
        //Kirigakure
        TimerCheckWater[playerid] = 0;
        PlayerIsJutsuWater[playerid] = 0;
        DestroyObject(KinoboriObject[playerid]);
        KinoboriObject[playerid] = INVALID_OBJECT_ID;
        RemovePlayerAttachedObject(playerid, 6);
        RemovePlayerAttachedObject(playerid, 7);
        SendClientMessageEx(playerid, COLOR_WHITE, "{2826FE}[INFO-CHAKRA]{FFFFFF} Voc? ficou muito tempo fora da agua por isso seu jutsu foi desativado.");
    }
    else if(AreaKinobori[playerid] == 3 && ninjaPos[2] >= 83.0 && PlayerIsJutsuWater[playerid] == 1){
        //Vale Do Fim
