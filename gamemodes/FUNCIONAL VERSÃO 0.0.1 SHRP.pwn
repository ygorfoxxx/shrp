    if(!IsValidObject(DaikodanBall[playerid])) return 0;
    new Float:playerPos[3], vw = GetPlayerVirtualWorld(playerid);
    GetObjectPos(DaikodanBall[playerid], playerPos[0], playerPos[1], playerPos[2]);
    for(new i; i<MAX_PLAYERS; i++)
    {
        switch(GetShootedJutsu(playerPos[0], playerPos[1], playerPos[2], 5.5, false, false, vw, playerid, i))
        {
            case PLAYER:
            {
                DaikodanHitAlvo[i] = 1;
                SetPlayerToFacePlayer(playerid, i);
                DaikodanAlvoObj(playerid, i);
                DamageDaikodan1(playerid, i);
                SetTimerEx("LimparDaikodanAlvo", 450, false, "i", i);
            }
        }
    }
    SetTimerEx("DaikodanHitted", 50, false, "d", playerid);
    return 0;
