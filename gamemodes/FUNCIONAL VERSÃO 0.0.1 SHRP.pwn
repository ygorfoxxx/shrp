CMD:roubar(playerid, params[])
{
    new string[128], giveplayerid, Float:alvoPos[3];
    if(sscanf(params, "d", giveplayerid)) return SendClientMessage(playerid, COLOR_WHITE, "(AVISO) Use /roubar [ID]");
    GetPlayerPos(giveplayerid, alvoPos[0], alvoPos[1], alvoPos[2]);
    if(EstaRoubando[playerid] == 1) return SendClientMessage(playerid, COLOR_WHITE, string);
    format(string, sizeof(string), "(ROUBO) Voc? n?o pode roubar ninjas com rank menor que 5.");
    if(Info[giveplayerid][pLevel] <= 5) return SendClientMessage(playerid, COLOR_WHITE, string);
    if(IsPlayerInRangeOfPoint(playerid, 1.5, alvoPos[0], alvoPos[1], alvoPos[2])){
        if(Dormido[giveplayerid] == 1 || DormindoNoColchonete[giveplayerid] == 1 || GetPVarInt(giveplayerid, "Inconsciente") >= 1)
        {
            InventarioClickRoubo[playerid] = 0;
            EstaRoubando[playerid] = 1;
            EstaRoubandoAlvo[playerid] = giveplayerid;
            AbrirInventarioRoubo(playerid, 1, giveplayerid);
            format(string, sizeof(string), "est? roubando o ninja.");
            RegistroMensagem(playerid, 30.0, string);
        }else{format(string, sizeof(string), "{AB7C4E}(ROUBO) Voc? s? pode roubar ninja inconsciente ou dormindo."); SendClientMessage(playerid, COLOR_WHITE, string);}
    }else{format(string, sizeof(string), "{AB7C4E}(ROUBO) Voc? s? pode roubar ninja proximo de voc?."); SendClientMessage(playerid, COLOR_WHITE, string);}
    return 1;
}
