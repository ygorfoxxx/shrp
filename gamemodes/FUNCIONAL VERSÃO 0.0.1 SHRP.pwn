        for(new i=0; i<MAX_PLAYERS; i++)
        {
            if(InteriorSwatVan[i] != 0)
            {
                SetPlayerHealth(i, 0);
                SendClientMessage(i, COLOR_GREY, "[Atenci?n]:{FFFFFF} El veh?culo en el que estabas dentro explot?, todos los que estaban dentro han muerto.");
            }
        }
    }
    if(GetVehicleModel(vehicleid) == 508)
    {
        for(new i=0; i<MAX_PLAYERS; i++)
        {
            if(InteriorCaravana[i] != 0)
            {
                SetPlayerHealth(i, 0);
                SendClientMessage(i, COLOR_GREY, "[Atenci?n]:{FFFFFF} El veh?culo en el que estabas dentro explot?, todos los que estaban dentro han muerto.");
            }
        }
    }
    if(GetVehicleModel(vehicleid) == 511)
    {
        for(new i=0; i<MAX_PLAYERS; i++)
        {
            if(InteriorBeagle[i] != 0)
            {
                SetPlayerHealth(i, 0);
                SendClientMessage(i, COLOR_GREY, "[Atenci?n]:{FFFFFF} El veh?culo en el que estabas dentro explot?, todos los que estaban dentro han muerto.");
