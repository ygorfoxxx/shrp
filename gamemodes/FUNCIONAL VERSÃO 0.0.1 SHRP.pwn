AudioInArea(playerid, audioid, Float:x, Float:y, Float:z, Float:radius)
{
    GetPlayerPos(playerid, x, y, z);
    new AudioIndent, vw = GetPlayerVirtualWorld(playerid);
    for(new i = 0; i < MAX_PLAYERS; i++)
    {
        if(IsPlayerConnected(i) && i != INVALID_PLAYER_ID && GetPlayerVirtualWorld(i) == vw)
        {
            if(IsPlayerInRangeOfPoint(i, radius, x, y, z))
            {
                AudioIndent = Audio_Play(i, audioid);
                Audio_Set3DPosition(i, AudioIndent, x, y, z, radius);
                Audio_SetVolume(i, AudioIndent, 100);
            }
        }
    }
    return 1;
}
