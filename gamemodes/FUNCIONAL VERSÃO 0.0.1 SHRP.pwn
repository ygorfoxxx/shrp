public OnPlayerText(playerid, text[])
{
    new strspam[128], string[228];
    //if(GetPVarInt(playerid, "Inconsciente") >= 1) {SendClientMessage(playerid, -1, "{AB7C4E}(ERRO) Voc? n?o pode falar inconsciente"); return 0;}
    if(DetectarSpam(text)) {format(strspam, sizeof(strspam), "{5FDE35}A palavra {FFFFFF}%s{5FDE35} n?o ? permitida no servidor{FFFFFF}.", text); SendClientMessage(playerid, -1, strspam); return 0;}
    // Anti-MG
    if(strfind(text, "=)", true) != -1 || strfind(text, "xD", true) != -1 || strfind(text, "=V", true) != -1 || strfind(text, "Server", true) != -1 || strfind(text, "Noob", true) != -1 || strfind(text, "RolePlay", true) != -1 || strfind(text, ":D", true) != -1 || strfind(text, ":c", true) != -1)
    {
        format(strspam, sizeof(strspam), "{5FDE35}A palavra {FFFFFF}%s{5FDE35} n?o ? permitida no servidor{FFFFFF}.", text);
       SendClientMessage(playerid, -1, strspam);
       return 0;
    }
    // ==== Atendimento ====//
    if(Atendendo[playerid] == 1)
    {
        format(string, sizeof(string), "{AB7C4E}(Atendimento){FFFFFF} %s: %s", PlayerNameDados(playerid), text);
        SendClientMessage(playerid, COLOR_WHITE, string);
        SendClientMessage(ChatAtendimentoStaff[playerid], COLOR_WHITE, string);
        return 0;
    }
    if(SendoAtendido[playerid] == 1)
    {
        format(string, sizeof(string), "{AB7C4E}(Atendimento){FFFFFF} %s: %s", PlayerNameDados(playerid), text);
        SendClientMessage(playerid, COLOR_WHITE, string);
        SendClientMessage(ChatAtendimentoPlayer[playerid], COLOR_WHITE, string);
        return 0;
    }
    //=== Final Atendimento ===//
    if(GetPVarInt(playerid, "Inconsciente") >= 1) return SendClientMessage(playerid, -1, "{AB7C4E}(ERRO) Voc? n?o pode falar inconsciente");
    if(Info[playerid][pMaskuse] == 1 && SendoAtendido[playerid] == 0 && Atendendo[playerid] == 0)
    {
        new Float: x, Float: y, Float: z;
        GetPlayerPos(playerid, x, y, z);
        format(string, sizeof(string), "{C2A2DA}Voc?{FFFFFF}: %s", text);
        SendClientMessage(playerid, -1, string);
        foreach(Player, i)
        {
            if(playerid != i && IsPlayerInRangeOfPoint(i, 20.0, x, y, z))
            {
                format(string, sizeof(string), "{C2A2DA}(Mascarado) (%d) diz{FFFFFF}: %s", playerid, text);
                SendClientMessage(i, -1, string);
                SetPlayerChatBubble(playerid, text, COLOR_WHITE, 20.0, 5000);
            }
        }
        return 0;
    }
    //Sistema falar desconhecido
    if(Info[playerid][pMaskuse] == 0 && SendoAtendido[playerid] == 0 && Atendendo[playerid] == 0 && GetPVarInt(playerid, "Inconsciente") == 0)
    {
        new file[128], file2[128], Arquivo[128], Nome[128];
        new Float:x, Float:y, Float:z;
        GetPlayerPos(playerid, x, y, z);
        GetPVarString(playerid, "GuardadoName", Arquivo, sizeof(Arquivo));
        format(file, sizeof(file), PASTA_Apresentados, Arquivo);
        format(file2, sizeof(file2), "PlayerApresentados/%s_Conhece.ini", Arquivo);
        if(!DOF2_FileExists(file))
        {
            DOF2_CreateFile(file);
            DOF2_CreateFile(file2);
        }else{
            format(string, sizeof(string), "Voc? (%d): %s", playerid, text);
            SendClientMessage(playerid, -1, string);
            foreach(Player, i)
            {
                if(playerid != i && IsPlayerInRangeOfPoint(i, 20.0, x, y, z) && GetPVarInt(i, "Inconsciente") == 0)
                {
                    format(Nome, sizeof(Nome), "%s", PlayerNameDados(i));
                    if(DOF2_GetBool(file, Nome) == true)
                    {
                        new falou[128];
                        format(falou, sizeof(falou), "%s", PlayerNameDados(playerid));
                        if(strfind(text, falou) != -1)
                        {
                            DOF2_SetBool(file2, Nome, true);
                        }
                        if(DOF2_GetBool(file2, Nome) == true)
                        {
                            format(string, sizeof(string), "%s (%d): %s", PlayerNameDados(playerid),playerid, text);
                            SendClientMessage(i, -1, string);
                            SetPlayerChatBubble(playerid,text,COLOR_WHITE,20.0,5000);
                        }
                        else
                        {
                            format(string, sizeof(string), "Desconhecido (%d): %s",playerid, text);
                            SendClientMessage(i, -1, string);
                            SetPlayerChatBubble(playerid,text,COLOR_WHITE,20.0,5000);
                        }
                    }else{
                        DOF2_SetBool(file2, Nome, false);
                        DOF2_SetBool(file, Nome, true);
                        if(i == playerid)
                        {
                            DOF2_SetBool(file2, Nome, true);
                        }
                    }
                }
            }
        }
    }
    return 0;
}
function FixHour(hour)
{
    hour = timeshift+hour;
    if (hour < 0)
    {
        hour = hour+24;
    }
    else if (hour > 23)
    {
        hour = hour-24;
    }
    shifthour = hour;
    return 1;
}

