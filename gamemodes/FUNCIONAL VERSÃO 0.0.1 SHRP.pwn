//***ayuda por textdraw***
public OnPlayerClickTextDraw(playerid, Text:clickedid)
{
    if(clickedid == INVALID_TEXT_DRAW && InventoryOpen[playerid] == 1){
        AbrirInventario(playerid, 0);
    }
    if(clickedid == INVALID_TEXT_DRAW && InventarioRouboAberto[playerid] == 1){
       AbrirInventarioRoubo(playerid, 2);
    }
    if(clickedid == INVALID_TEXT_DRAW && LendoLivro[playerid] == 1){
        FecharLivro(playerid);
    }
    if(clickedid == INVALID_TEXT_DRAW && LendoLivro[playerid] == 2)
    {
        FecharLivroAcademia(playerid);
    }
    if(clickedid == INVALID_TEXT_DRAW && MissoesNormalOpen[playerid] >= 1){
        MissoesNormalOpen[playerid] = 0;
        TextDrawHideForPlayer(playerid, MissoesBKG[0]);
        PlayerTextDrawHide(playerid, MissoesNew[playerid][0]);
        PlayerTextDrawHide(playerid, MissoesNew[playerid][1]);
        PlayerTextDrawHide(playerid, MissoesNew[playerid][2]);
        PlayerTextDrawHide(playerid, MissoesNew[playerid][3]);
        PlayerTextDrawHide(playerid, MissoesNew[playerid][4]);
        PlayerTextDrawHide(playerid, MissoesNew[playerid][5]);
        PlayerTextDrawHide(playerid, MissoesNew[playerid][6]);
        PlayerTextDrawHide(playerid, MissoesNew[playerid][7]);
        CancelSelectTextDraw(playerid);
        Audio_Play(playerid, 58);
    }
    if(clickedid == INVALID_TEXT_DRAW && LendoCarta[playerid] == 1){
        Audio_Play(playerid, 58);
        LendoCarta[playerid] = 0;
        PlayerTextDrawHide(playerid, CartaTXD[playerid][0]);
        PlayerTextDrawHide(playerid, CartaTXD[playerid][1]);
        PlayerTextDrawHide(playerid, CartaTXD[playerid][2]);
        CancelSelectTextDraw(playerid);
    }
    if(clickedid == INVALID_TEXT_DRAW && EscolhendoSeV[playerid] == 1){
        SelectTextDraw(playerid, COLOR_WHITE);
        Audio_Play(playerid, 58);
    }
    if(clickedid == INVALID_TEXT_DRAW && VendoRegistroGeral[playerid] == 1){
        CancelSelectTextDraw(playerid);
        FecharRegistroGeral(playerid, TargetMostrandoRegistroG[playerid]);
        Audio_Play(playerid, 58);
    }
    //Inicio Sistema Login/Registro
    if(clickedid == BotaoLogin[0])
    {
        INI_ParseFile(UserPath(playerid), "LoadUser_pass", .bExtra = true, .extra = playerid);
        ShowPlayerDialog(playerid, DLOGIN, DIALOG_STYLE_PASSWORD, "{E9FE23}Login", "{FFFFFF}Coloque sua {E9FE23}Senha{FFFFFF}.", "Entrar", "Sair");
    }
    if(clickedid == BotaoRegistro[0])
    {
        ShowPlayerDialog(playerid, DREGISTER, DIALOG_STYLE_PASSWORD, "{E9FE23}Registro", "{FFFFFF}Coloque uma {E9FE23}Senha{FFFFFF} para se cadastrar.", "Cadastrar", "Sair");
    }
    return 1;
}
