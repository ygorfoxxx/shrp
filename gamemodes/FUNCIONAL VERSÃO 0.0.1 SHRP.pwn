function EnviarPombo(playerid)
{
    new carta = 56, quanti = 1, string[128];
    for (new i = 0; i < 20; i++) {
        if (Inventario[playerid][pSave][i] == carta && Inventario[playerid][pTotalSave][i] >= quanti) {
            if (CartasPombo[playerid][pombosEnviado] == 1){SendClientMessage(playerid, -1, "{AB7C4E}(POMBO-CORREIO) Voc? j? enviou um pombo correio."); return 1;}
            if(RecebendoPombo[playerid] == 1){SendClientMessage(playerid, -1, "{AB7C4E}(POMBO-CORREIO) Voc? esta recebendo um pombo aguarde."); return 1;}
            new item = Inventario[playerid][pSave][slotX[playerid]];
            if (item == 166) {
                ShowPlayerDialog(playerid, DIALOG_POMBOI, DIALOG_STYLE_INPUT, "{AB7C4E}Carta", "{FFFFFF} Escreva sua carta:", "OK", "Fechar");
            } else if (item == 167) {
                ShowPlayerDialog(playerid, DIALOG_POMBOII, DIALOG_STYLE_INPUT, "{AB7C4E}Carta", "{FFFFFF} Escreva sua carta:", "OK", "Fechar");
            } else if (item == 168) {
                ShowPlayerDialog(playerid, DIALOG_POMBOIII, DIALOG_STYLE_INPUT, "{AB7C4E}Carta", "{FFFFFF} Escreva sua carta:", "OK", "Fechar");
            }
            FecharInventario(playerid);
            Audio_Play(playerid, 58);
            return 1;
        }
    }
    SendClientMessage(playerid, -1, "{AB7C4E}(POMBO-CORREIO) Voc? precisa de uma carta para usar o pombo correio");
    return 0;
}
