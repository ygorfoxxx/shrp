                GivePlayerItemEx(playerid, 45, 3);

            }else{
                GivePlayerItemEx(playerid, 45, 3);
                GivePlayerItemEx(playerid, 179, 1);
            }
            PegouPedraAlgodao[h] = 1;
            TimingPedraAlgodaoRespawn[h] = gettime() + PEDRA_COOLDOWN;
