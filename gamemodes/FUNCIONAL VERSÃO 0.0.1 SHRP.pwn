function LastHitArenaPvP(playerid, targetid)
{
    UltimoHitArenaPvP[targetid] = playerid;
    HittedArenaPvP[targetid] = 1;
    ResetHitTempoPvP[targetid] = gettime() + TEMPO_RESETHITPVP;
    return 1;
}
function ArenaPvPFunction(playerid, type)
{
    new string[128];
    switch(type)
    {
        case 0: return 0;
        case 1:{ // Queda
            CaiuArenaPvP[playerid] = 1;
            Info[playerid][pHealthEnUso] = Info[playerid][pHealthMaximo];
            Audio_Play(playerid, 134);
            if(PontoArenaPvP[playerid] >= 1){PontoArenaPvP[playerid] --;}else{PontoArenaPvP[playerid] = 0;}
            for(new i; i < MAX_PLAYERS; i++){
                    format(string, sizeof(string), "{AB7C4E}(ARENA){FFFFFF} o ninja {AB7C4E}%s{FFFFFF} caiu da arena.", PlayerNameDados(playerid));
                    if(EntrouArenaPvP[i] == 1){SendClientMessage(i, -1, string);}
                }
        }
        case 2:{ // Morte
            if(HittedArenaPvP[playerid] == 1){ // Hitted
                HittedArenaPvP[playerid] = 0;
                ResetHitTempoPvP[playerid] = 0;
                PontoArenaPvP[UltimoHitArenaPvP[playerid]] ++;
                Info[playerid][pHealthEnUso] = Info[playerid][pHealthMaximo];
                Audio_Play(playerid, 134);
                Audio_Play(UltimoHitArenaPvP[playerid], 134);
                for(new i; i < MAX_PLAYERS; i++){
                    format(string, sizeof(string), "{AB7C4E}(ARENA){FFFFFF} o ninja {AB7C4E}%s{FFFFFF} derrubou o ninja {AB7C4E}%s{FFFFFF}.", PlayerNameDados(UltimoHitArenaPvP[playerid]), PlayerNameDados(playerid));
                    if(EntrouArenaPvP[i] == 1){SendClientMessage(i, -1, string);}
                }
            }
            MorreuArenaPvP[playerid] = 1;
        }
    }
    return 1;
}
function VenceuArena(playerid)
{
    if(PontoArenaPvP[playerid] > 9){
        if(EntrouArenaPvP[playerid] == 1){
            for(new i; i < MAX_PLAYERS; i++){
                {
                    SetPlayerPos(i, -716.7190, -635.9840, 2001.6406);
                    Info[i][pHealthEnUso] = Info[i][pHealthMaximo];
                    PontoArenaPvP[i] = 0;
                    MorreuArenaPvP[i] = 0;
                    CaiuArenaPvP[i] = 0;
                    Audio_Play(i, 135);
                }
            }
        }
    }
    return 1;
}
