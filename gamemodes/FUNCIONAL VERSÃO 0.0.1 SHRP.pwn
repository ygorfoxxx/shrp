CMD:criarcasa(playerid, params[])
{
    new idcasa, tipo, mundo, valor, string[100];
    if(Info[playerid][pAdminZC] == 0) return SendClientMessage(playerid, 0xFF0000AA, "[ERRO]: Este comando n?o existe!");
    if(sscanf(params, "ddd", idcasa, tipo, mundo)) return SendClientMessageEx(playerid, COLOR_WHITE, "[INFO]: Use /criarcasa [ID] [1P, 2P, 3M 4G], [ID MUNDO]");
    if(idcasa < 0 || idcasa >= MAX_CASAS) return SendClientMessage(playerid, COLOR_WHITE, "[INFO]: ID desta casa ? invalido");
    if(InfoCasa[idcasa][CasaExteriorX] == 0.0)
    {
        InfoCasa[idcasa][Comprada] = 0; // Armazena o valor se a casa ta comprada ou n?o
        InfoCasa[idcasa][MundoCasa] = mundo;
        format(InfoCasa[idcasa][Dono], MAX_PLAYER_NAME, "Ninguem"); // Armazena o dono da casa
        GetPlayerPos(playerid, InfoCasa[idcasa][CasaExteriorX], InfoCasa[idcasa][CasaExteriorY], InfoCasa[idcasa][CasaExteriorZ]); //Pega o Lado de Fora da Casa
        InfoCasa[idcasa][Fechado] = 1;
        InfoCasa[idcasa][CasaExteriorR] = 0.0;
        PosicaoCasas(tipo);
        switch(tipo)
        {
            case 0: return 0;
            case 1: valor = 15000;
            case 2: valor = 30000;
            case 3: valor = 40000;
            case 4: valor = 60000;
        }
        InfoCasa[idcasa][Valor] = valor;
        InfoCasa[idcasa][CasaPickup] = CreateDynamicPickup(15305, 0, InfoCasa[idcasa][CasaExteriorX], InfoCasa[idcasa][CasaExteriorY], InfoCasa[idcasa][CasaExteriorZ], -1, -1); //Cria??o do Pickup da Casa
        InfoCasa[idcasa][CasaIconMap] = CreateDynamicMapIcon(InfoCasa[idcasa][CasaExteriorX], InfoCasa[idcasa][CasaExteriorY], InfoCasa[idcasa][CasaExteriorZ], 31, 0, -1, -1);
        SalvarCasa(idcasa);
        format(string, sizeof(string), "ID (%d) VALOR (%d) MUNDO CASA (%d)", idcasa, InfoCasa[idcasa][Valor], InfoCasa[idcasa][MundoCasa]);
        SendClientMessage(playerid, COLOR_WHITE, string);
    }
    return 1;
}
CMD:comprarcasa(playerid, params[])
{
    new string[256];
    new playername[MAX_PLAYER_NAME];
    for(new h = 0; h < sizeof(InfoCasa); h++)
    {
        if(IsPlayerInRangeOfPoint(playerid,2.0,InfoCasa[h][CasaExteriorX], InfoCasa[h][CasaExteriorY], InfoCasa[h][CasaExteriorZ]))
        {
            if(InfoCasa[h][Comprada] == 0 )
            {
                if(Info[playerid][pChaveCasa] >= 0) return SendClientMessage(playerid, COLOR_WHITE, "Voc? ja tem uma casa");
                if(GetPlayerCash(playerid) >= InfoCasa[h][Valor])
                {
                    Info[playerid][pChaveCasa] = h;
                    InfoCasa[h][Comprada] = 1;
                    new nome[32];
                    format(nome, sizeof(nome), "%s", PlayerNameDados(playerid));
                    strmid(InfoCasa[h][Dono], nome, 0, strlen(nome), 32);
                    GivePlayerCash(playerid,-InfoCasa[h][Valor]);
                    SendClientMessageEx(playerid, COLOR_WHITE, "Parab?ns pela sua nova compra.");
                    OnPlayerUpdate(playerid);
                    DestroyDynamicPickup(InfoCasa[h][CasaPickup]);
                    DestroyDynamicMapIcon(InfoCasa[h][CasaIconMap]);
                    InfoCasa[h][CasaPickup] = CreateDynamicPickup(15306, 0, InfoCasa[h][CasaExteriorX], InfoCasa[h][CasaExteriorY], InfoCasa[h][CasaExteriorZ], -1, -1);
                    InfoCasa[h][CasaIconMap] = CreateDynamicMapIcon(InfoCasa[h][CasaExteriorX], InfoCasa[h][CasaExteriorY], InfoCasa[h][CasaExteriorZ], 32, 0, -1, -1);
                    SalvarCasa(h);
                    return 1;
                }
                else
                {
                    format(string, sizeof(string), "{AB7C4E}(CASA) O valor da casa ? de ({FFFFFF}%d Ryos{AB7C4E}).", InfoCasa[h][Valor]);
                    SendClientMessageEx(playerid, COLOR_WHITE, string);
                    return 1;
                }
            } else {SendClientMessage(playerid, COLOR_WHITE, "{AB7C4E}(AVISO) Esta casa j? tem dono,");}
        }
    }
    return 1;
}
