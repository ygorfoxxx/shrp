    PlayerTextDrawHide(playerid, BtnUparStats[playerid][1]); // Status Upar
    PlayerTextDrawHide(playerid, BtnUparStats[playerid][2]); // Status Upar
    PlayerTextDrawHide(playerid, BtnUparStats[playerid][3]); // Status Upar
    PlayerTextDrawHide(playerid, BtnUparStats[playerid][4]); // Status Upar
    PlayerTextDrawHide(playerid, BtnUparStats[playerid][5]); // Status Upar
    PlayerTextDrawHide(playerid, BKGRyos[playerid][0]); // BKG Ryos
    PlayerTextDrawHide(playerid, InventRyos[playerid][0]); // Ryos
    for(new i; i<20; i++)
    {
        PlayerTextDrawHide(playerid, Slots[playerid][i]);
        PlayerTextDrawHide(playerid, SlotQuantidade[playerid][i]);
    }
    for(new a; a<6; a++)
    {
        PlayerTextDrawHide(playerid, AcessorioSlots[playerid][a]);
    }
    ApplyAnimation(playerid, "BOMBER", "BOM_Plant_2Idle", 4.1, 0, 1, 1, 0, 0, 1);
