    if(Urushi[playerid][urushiCast]) return 0;
    Urushi[playerid][urushiCast] = 1;
    new Float:x, Float:y, Float:z, Float:a;
    GetPlayerFacingAngle(playerid, a);
    GetPlayerPos(playerid, x, y, z);
    GetXYInFrontOfPlayer(playerid, x, y, 2.0);
    Urushi[playerid][urushiActor] = CreateDynamicActor(298, x,y,z+0.1,a); //CreateActor(298, x, y, z+0.1, a);
    Urushi[playerid][urushiEffect] = CreateObject(18737, x, y, z-2.6, 0, 0, 0);
    SetTimerEx("UrushiOff", 20000, false, "i", playerid);
    SetTimerEx("UrushiAnim", 185, false, "i", playerid);
    return 1;
}
