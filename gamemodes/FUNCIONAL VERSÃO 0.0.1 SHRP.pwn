stock SL_CreateObject(modelid, Float:x, Float:y, Float:z, Float:rx, Float:ry, Float:rz, Float:drawdistance = 300.0)
{
    // Procurar um ?ndice livre para o objeto
    new index = -1;
    for (new i = 0; i < MAX_SL_OBJECTS; i++)
    {
        if (SL_ObjectList[i][0] == false) // Verifica se o ?ndice n?o est? em uso
        {
            index = i;
            break;
        }
    }

    // Verifica se encontramos um ?ndice livre
    if (index == -1) return -1;

    // Criar o objeto no mundo do SA-MP
    new id = CreateObject(modelid, x, y, z, rx, ry, rz, drawdistance);
    if (id == INVALID_OBJECT_ID) return -1;

    // Armazena o ID e marca o ?ndice como utilizado
    SL_ObjectList[index][0] = true;     // Marca como usado
    SL_ObjectList[index][1] = id;       // Armazena o ID do objeto criado

    return id;
}
stock SL_DestroyObject(objectid)
{
    for (new i = 0; i < MAX_SL_OBJECTS; i++)
    {
        if (SL_ObjectList[i][1] == objectid) // Encontra o ?ndice pelo ID do objeto
        {
            DestroyObject(SL_ObjectList[i][1]); // Destroi o objeto no jogo
            SL_ObjectList[i][0] = false;        // Libera o ?ndice na lista
            SL_ObjectList[i][1] = -1;           // Reseta o ID
            return 1;
        }
    }
    return 0;
}
stock SL_IsValidObject(objectid)
{
    // Percorre a lista de objetos SL para verificar se o objeto existe e ? v?lido
    for (new i = 0; i < MAX_SL_OBJECTS; i++)
    {
        if (SL_ObjectList[i][0] == true && SL_ObjectList[i][1] == objectid) // Verifica se o ?ndice est? em uso e o ID corresponde
        {
            return SL_ObjectList[i][1]; // Retorna o ID do objeto
        }
    }
    return INVALID_SL_OBJECT_ID; // Retorna um valor indicando que o objeto n?o ? v?lido
}
stock SL_SetObjectPos(objectid, Float:x, Float:y, Float:z)
{
    // Verifica se o objeto ? v?lido usando SL_IsValidObject
    if (SL_IsValidObject(objectid) != INVALID_SL_OBJECT_ID)
    {
        SetObjectPos(objectid, x, y, z); // Ajusta a posi??o do objeto
        return 1; // Retorna 1 para indicar que a posi??o foi ajustada com sucesso
    }
    return 0; // Retorna 0 se o objeto n?o for v?lido
}

