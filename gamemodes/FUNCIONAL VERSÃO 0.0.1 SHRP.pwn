function TeclaSabio(playerid, newkeys, oldkeys)
{
    if(BtnDireito[playerid] == 1) return 0;

    // Checa cooldown do modo s?bio
    if(TimerSabio[playerid] > gettime()) return 0;

    new Float:NinjutsuEmUso = Info[playerid][pNinjutsu];
    new Float:NinjutsuMaximo = Info[playerid][pHealthMaximo];
    new Float:PorcentagemDNinjutsu = (NinjutsuEmUso/NinjutsuMaximo)*100;
    new string[128];

    // Desativar modo s?bio manualmente
    if(SabioAtivado[playerid] == 1 && PRESSED(newkeys, KEY_SPRINT | KEY_CROUCH) && oldkeys != KEY_CROUCH)
    {
        DesativarSabio(playerid, true);
    }
    // Ativar modo s?bio
    else if(PorcentagemDNinjutsu <= 100 && SabioAtivado[playerid] == 0 && PRESSED(newkeys, KEY_SPRINT | KEY_CROUCH) && oldkeys != KEY_CROUCH)
    {
        // Checa chakra m?nimo
        if(Info[playerid][pChakra] < 5000.0) return 0;
        if(Info[playerid][pChakraEnUso] < Info[playerid][pChakra]) return SemChakra(playerid);
        // Aplica b?nus de Ninjutsu
        Info[playerid][pNinjutsu] += 100;
        new skin = GetPlayerSkinEx(playerid);
        AudioInPlayer(playerid, 30.0, 103);

        if(skin == 20189) // Aogetsu Jovem
        {
            SetPlayerSkin(playerid, 20190);
            SetPlayerAttachedObject(playerid, 0, 19503, 1, -0.65, 0.00, -0.01, 0.00, 88.90, 122.58, 1.00, 1.00, 1.00);
            ApplyAnimation(playerid, "Shinobi_Anim", "Sasuke_AWK", 1.0, 0, 0, 0, 0, 0, 1);
        }else if(skin == 20220) // Aogetsu Jovem
        {
            SetPlayerSkin(playerid, 20228);
            SetPlayerAttachedObject(playerid, 0, 19503, 1, -0.65, 0.00, -0.01, 0.00, 88.90, 122.58, 1.00, 1.00, 1.00);
            ApplyAnimation(playerid, "Shinobi_Anim", "Sasuke_AWK", 1.0, 0, 0, 0, 0, 0, 1);
        }else if(skin == 20108)
        { // Barroso Sai
            SetPlayerAttachedObject(playerid, 0, 18696, 1, -10.267013, -0.053999, -0.139999, 2.799999, 89.00000, 0.00000, 6.0, 6.0, 6.0);
            ApplyAnimation(playerid, "Shinobi_Anim", "Katsuyu_02", 1.0, 0, 0, 0, 0, 0, 1);

        }
        else{ // Padr?o
            SetPlayerAttachedObject(playerid, 0, 18702, 1, -10.267013, -0.053999, -0.139999, 2.799999, 89.00000, 0.00000, 6.0, 6.0, 6.0);
            ApplyAnimation(playerid, "Shinobi_Anim", "Katsuyu_02", 1.0, 0, 0, 0, 0, 0, 1);
        }

        SabioAtivado[playerid] = 1;

        format(string, sizeof(string), "{AB7C4E}(AVISO) Voc? ativou seu ({FFFFFF}Modo S?bio{AB7C4E}).");
        SendClientMessage(playerid, -1, string);
        TogglePlayerControllable(playerid, 1);

        // Timer para desativar automaticamente
        SetTimerEx("TimerDesativarSabio", COOLDOWN_DESATIVARSABIO * 7000, false, "i", playerid);
    }

    return 1;
}

// Fun??o para desativar modo s?bio (manual ou autom?tica)
stock DesativarSabio(playerid, manual)
{
    new skin = GetPlayerSkinEx(playerid);
    if(!IsPlayerConnected(playerid)) return 0;
    if(SabioAtivado[playerid] == 0) return 0;

    if(skin == 20190)
    {
        SetPlayerSkin(playerid, 20189);
        TogglePlayerControllable(playerid, 1);
    }else if(skin == 20228)
    {
        SetPlayerSkin(playerid, 20220);
        TogglePlayerControllable(playerid, 1);
    }
    SabioAtivado[playerid] = 0;


    // Remove apenas o b?nus
    Info[playerid][pNinjutsu] -= 100;
    if(Info[playerid][pNinjutsu] < 0) Info[playerid][pNinjutsu] = 0;

    // Remove anima??o / objeto
    RemovePlayerAttachedObject(playerid, 0);
    TogglePlayerControllable(playerid, 1);

    // Mensagem
    new string[128];
    if(manual)
        format(string, sizeof(string), "{AB7C4E}(AVISO) Voc? desativou seu ({FFFFFF}Modo S?bio{AB7C4E}).");
    else
        format(string, sizeof(string), "{AB7C4E}(AVISO) Seu ({FFFFFF}Modo S?bio{AB7C4E}) se Esgotou.");
    SendClientMessage(playerid, -1, string);

    // Aplica cooldown
    TimerSabio[playerid] = gettime() + COOLDOWN_SABIO;

    return 1;
}

