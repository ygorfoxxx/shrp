        if(encontrouPapelBomba)
        {
            //Verificar se o caixa j? esta sendo roubado
            if(IsPlayerInRangeOfPoint(playerid, 5.0, 2294.130126, -2668.384277, 29.215276) && CaixaSendoExplodido[2] == 1){SendClientMessage(playerid, -1, "{AB7C4E}(ROUBO) Esse caixa j? esta sendo explodido aguarde um tempo antes de explodir ele novamente."); return 1;}
            else if(IsPlayerInRangeOfPoint(playerid, 5.0, 2652.984619, -2321.688964, 29.010021) && CaixaSendoExplodido[3] == 1){SendClientMessage(playerid, -1, "{AB7C4E}(ROUBO) Esse caixa j? esta sendo explodido aguarde um tempo antes de explodir ele novamente."); return 1;}
            else if(IsPlayerInRangeOfPoint(playerid, 5.0, -1812.917114, 1694.345214, 1.664391) && CaixaSendoExplodido[0] == 1){SendClientMessage(playerid, -1, "{AB7C4E}(ROUBO) Esse caixa j? esta sendo explodido aguarde um tempo antes de explodir ele novamente."); return 1;}
            else if(IsPlayerInRangeOfPoint(playerid, 5.0, -1730.933227, 1897.269165, 13.045327) && CaixaSendoExplodido[1] == 1){SendClientMessage(playerid, -1, "{AB7C4E}(ROUBO) Esse caixa j? esta sendo explodido aguarde um tempo antes de explodir ele novamente."); return 1;}
            //Verificar se o caixa esta desativado
            else if(IsPlayerInRangeOfPoint(playerid, 5.0, 2294.130126, -2668.384277, 29.215276) && CaixaDesativado[2] == 1){SendClientMessage(playerid, -1, "{AB7C4E}(ROUBO) Esse caixa j? foi explodido aguarde um tempo antes de explodir ele novamente."); return 1;}
            else if(IsPlayerInRangeOfPoint(playerid, 5.0, 2652.984619, -2321.688964, 29.010021) && CaixaDesativado[3] == 1){SendClientMessage(playerid, -1, "{AB7C4E}(ROUBO) Esse caixa j? foi explodido aguarde um tempo antes de explodir ele novamente."); return 1;}
            else if(IsPlayerInRangeOfPoint(playerid, 5.0, -1812.917114, 1694.345214, 1.664391) && CaixaDesativado[0] == 1){SendClientMessage(playerid, -1, "{AB7C4E}(ROUBO) Esse caixa j? foi explodido aguarde um tempo antes de explodir ele novamente."); return 1;}
            else if(IsPlayerInRangeOfPoint(playerid, 5.0, -1730.933227, 1897.269165, 13.045327) && CaixaDesativado[1] == 1){SendClientMessage(playerid, -1, "{AB7C4E}(ROUBO) Esse caixa j? foi explodido aguarde um tempo antes de explodir ele novamente."); return 1;}
