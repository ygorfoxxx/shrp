function JikuujutsuInicio(playerid)
{
    if(Info[playerid][pAnbuPatente] == 0) return 0;
    new str[200], Float:Position[3], vw = GetPlayerVirtualWorld(playerid);
    if(Jinkuu[playerid][jinkuuTimer] > gettime()) return JutsuNotReady2(playerid, COLOR_WHITE, "Jikuujutsu Shiki", Jinkuu[playerid][jinkuuTimer]);
    if(Info[playerid][pChakraEnUso] < 350) return SemChakra(playerid);
    ClearSelo(playerid);
    for(new i; i<MAX_PLAYERS; i++)
    {
        switch(GetLookingTarget(2.0, 2.0, Invunerable[i] == 1, false, vw, playerid, i))
        {
            case NONE:
            {
                if(i != MAX_PLAYERS-1) continue;
                format(str, sizeof(str), "{EF0D02}[JIKUUJUTSU] Voc? precisa est? proximo para marcar");
                SendClientMessage(playerid, COLOR_WHITE, str);
                break;
            }
            case PLAYER:
            {
                if(Info[i][pHealthMaximo] >= 32000){
                    format(str, sizeof(str), "{AB7C4E}[JIKUUJUTSU] O ninja n?o tem vida suficiente para ser selado.", PlayerNameDados(i));
                    SendClientMessage(i, COLOR_WHITE, str);
                }
                else if(Info[i][pHealthMaximo] >= 14999 || Info[i][pHealthMaximo] <= 31000)
                {
                    Jinkuu[playerid][jinkuuAlvo] = i; // Marcando o Alvo
                    Jinkuu[playerid][jinkuuUsado] = 1;
                    if(JikuuMarcacao[i] == 2){
                        if(Info[playerid][pMaskuse] == 1)
                        {
                            format(str, sizeof(str), "{C2A2DA}(Mascarado) (%d){FFFFFF}: Fuuinjutsu: Jikuujutsu Shiki no Jutsu!", playerid);
                            SendMessageProx(playerid, 20.0, str, COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);
                        }
                        else
                        {
                            format(str, sizeof(str), "%s (%d): Fuuinjutsu: Jikuujutsu Shiki no Jutsu!", PlayerNameDados(playerid), playerid);
                            SendMessageProx(playerid, 20.0, str, COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);
                        }
                        new Float:targetPos[3];
                        ApplyAnimation(playerid, "Shinobi_Anim", "Suiton2_1", 4.0, 0, 0, 0, 0, 0, 1);
                        AudioInPlayer(playerid, 30.0, 106);
                        GetPlayerPos(i, targetPos[0], targetPos[1], targetPos[2]);
                        SetPlayerPos(i, targetPos[0], targetPos[1], targetPos[2]+1000.0);
                        SetPlayerPos(i, -1568.4086, 436.4622, 1573.1881);
                        SetPlayerWeather(i, 9);
                        format(str, sizeof(str), "{AB7C4E}[JIKUUJUTSU] Voc? foi selado pelo ninja {FFFFFF}%s{AB7C4E}.", PlayerNameDados(i));
                        SendClientMessage(i, COLOR_WHITE, str);
                        SetPlayerAttachedObject(playerid, 3, 15887, 1, 0.0, -0.3, 0.0, 108.900009, 4.799986, 87.799972, 0.778999, 0.808999, 0.754000); // Criar Objeto nas costas
                        JinkuuSairSelaTimer[i] = gettime() + JINKUU_TIMERSAIR;
                        JinkuuIDSelador[i] = playerid;
                        JinkuuIDSelado[playerid] = i;
                        JikuuSelado[playerid] = 1;
                    }else{
                        JikuuMarcacao[i]++;
                        JinkuuRemoverTimer[i] = gettime() + JINKUU_REMOVER;
                    }
                    ApplyAnimation(playerid, "Shinobi_Anim", "Suiton3_1", 4.0, 0, 0, 0, 0, 500, 1);
                    format(str, sizeof(str), "{AB7C4E}[JIKUUJUTSU] Voc? marcou o jogador {FFFFFF}%s{AB7C4E} com o seu Fuuinjutsu.", PlayerNameDados(i));
                    SendClientMessage(playerid, COLOR_WHITE, str);
                    format(str, sizeof(str), "{AB7C4E}[JIKUUJUTSU] O jogador {FFFFFF}%s{AB7C4E} lhe marcou com o Fuuinjutsu", PlayerNameDados(playerid));
                    SendClientMessage(i, COLOR_WHITE, str);
                    Jinkuu[playerid][jinkuuTimer] = gettime() + JINKUU_COOLDOWN;
                    break;
                }else{
                    format(str, sizeof(str), "{AB7C4E}[JIKUUJUTSU] O ninja {FFFFFF}%s{AB7C4E} n?o tem vida suficiente para ser selado.", PlayerNameDados(i));
                    SendClientMessage(playerid, -1, str);
                    break;
                }
            }
        }
    }
    return 1;
}
