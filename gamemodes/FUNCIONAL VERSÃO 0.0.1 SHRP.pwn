function EntrarSairCasa(playerid)
{
    new str[64];
    for(new idcasa = 0; idcasa < MAX_CASAS; idcasa++)
    {
        if(IsPlayerInRangeOfPoint(playerid, 2.0, InfoCasa[idcasa][CasaInteriorX], InfoCasa[idcasa][CasaInteriorY], InfoCasa[idcasa][CasaInteriorZ]) && InfoCasa[idcasa][Fechado] == 1 && CheckMundoCasa(idcasa) == GetPlayerVirtualWorld(playerid))
        {
            Audio_Play(playerid, 99);
            PlayerTextDrawShow(playerid, Cadeados[playerid][0]);
            TimingCasaAorF[playerid] = SetTimerEx("KillTimerCasa", 250, 0, "i", playerid);
            return 1;
        }
        else if(IsPlayerInRangeOfPoint(playerid, 2.0, InfoCasa[idcasa][CasaInteriorX], InfoCasa[idcasa][CasaInteriorY], InfoCasa[idcasa][CasaInteriorZ]) && InfoCasa[idcasa][Fechado] == 0 && CheckMundoCasa(idcasa) == GetPlayerVirtualWorld(playerid))
        {
            SetPlayerVirtualWorld(playerid, 0);
            LoadObjects(playerid);
            Info[playerid][pDentroDeCasa] = 0;
            SetPlayerPos(playerid, InfoCasa[idcasa][CasaExteriorX], InfoCasa[idcasa][CasaExteriorY], InfoCasa[idcasa][CasaExteriorZ]);
