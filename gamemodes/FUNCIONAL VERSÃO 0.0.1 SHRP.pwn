{ // 900 Segundos. - Timer se repite cada 15 Minutos. En 30 minutos kick.
    foreach(Player, zc)
    {
        if(gPlayerLogged{zc})
        {
            if(Info[zc][pEstado] == 0)
            {
                if(Info[zc][pAdminZC] <= 999999)
                {
                    GetPlayerPos(zc, PlayerPos[zc][0], PlayerPos[zc][1], PlayerPos[zc][2]);
                    if(PlayerPos[zc][0] == PlayerPos[zc][3] && PlayerPos[zc][1] == PlayerPos[zc][4] && PlayerPos[zc][2] == PlayerPos[zc][5])
                    {

                        OnPlayerSavedStats(zc);
                        ClearChatbox(zc);
                        gPlayerLogged{zc} = 0;
                        SetPlayerPos(zc, 1375.74207, -807.15186, 83.94895);
                        SetPlayerFacingAngle(zc, -90.0);
                        SetPlayerCameraPos(zc, 1416.4598, -914.8869, 68.2429);
                        SetPlayerCameraLookAt(zc, 1412.5050, -813.9489, 82.6266);
                        SendClientMessage(zc, COLOR_GENERAL, "{E9FE23}[INFO]{FFFFFF}: Voc? foi {E9FE23}expulso{FFFFFF} do servidor por ter ficado {E9FE23}AFK{FFFFFF}.");
                        new string[128];
                        format(string, sizeof(string), "{E9FE23}[ANTI-AFK]{FFFFFF} O usuario {E9FE23}%s{FFFFFF} foi {E9FE23}expulsado{FFFFFF} do servidor, por ter ficado {E9FE23}ausente{FFFFFF} por mais de {E9FE23}20 minutos{FFFFFF}.", PlayerNameDados(zc));
                        ABroadCast(COLOR_ORANGE, string, 2);
                        format(string, sizeof(string), "{FFFFFF}Expulsado por {E9FE23}inatividade{FFFFFF}!\nColoque a sua {E9FE23}senha{FFFFFF} para {E9FE23}re-logar{FFFFFF} no servidor.\n\n{5FB404}Senha:");
                        SafeDialogEx(zc,DLOGIN,DIALOG_STYLE_PASSWORD ,"{E9FE23}Re-Logar",string,"Entrar","sair");
                    }
                    PlayerPos[zc][3] = PlayerPos[zc][0];
                    PlayerPos[zc][4] = PlayerPos[zc][1];
                    PlayerPos[zc][5] = PlayerPos[zc][2];
                } // No es admin.
            } // No est? esposado ni congelado.
        } // Est? logeado.
    } // Conteo de todas las ID conectadas.
}
// Timer Name: SpecUpdate() TickRate: 3 secs.
timer SpecUpdate[3000](){
    foreach(Player, i){
        if(Info[i][pAdminZC] >= 1200000){
            if(Spectating[i] >= 1){
                if(Spectate[i] < 553){
                    new targetid = Spectate[ i ];
                    if( !IsPlayerConnected( targetid ) ){
                        SendClientMessageEx( i, COLOR_WHITE, "El jugador que estabas specteando salio del servidor." );
                        GettingSpectated[Spectate[i]] = 999;
                        Spectating[i] = 0;
                        Spectate[i] = 999;
                        SetPVarInt(i, "SpecState", -1);
                        SetPVarInt(i, "SpecOff", 1 );
                        TogglePlayerSpectating( i, false );
                        SetCameraBehindPlayer(i);
                        SetPlayerColor(i, COLOR_POINTGEN);
                    }
                }
                if(Spectate[i] == 553){
                    TogglePlayerControllable(i, 1);
                    TogglePlayerSpectating(i, 0);
                    DeletePVar(i, "MedicBill");
                    SpawnPlayer( i );
                    Spectate[i] = 999;
                    Spectating[i] = 0;
                    SetPlayerColor(i, COLOR_POINTGEN);
                }
                if(Spectate[i] == 554){
                    TogglePlayerControllable(i, 1);
                    SetPlayerInterior(i,Unspec[i][sPint]);
                    Info[i][pInt] = Unspec[i][sPint];
                    SetPlayerPos(i, Unspec[i][Coords][0], Unspec[i][Coords][1], Unspec[i][Coords][2]);
                    Spectate[i] = 999;
                    SetPlayerColor(i, COLOR_POINTGEN);
                }
                    if(Spectate[i] == 556){
                    Spectate[i] = 999;
                    SetPlayerColor(i, COLOR_POINTGEN);
                }
                    if(Spectate[i] == 557){
                    Spectate[i] = 554;
                    SetPlayerColor(i, COLOR_POINTGEN);
                }
            }
        }
    }
}
