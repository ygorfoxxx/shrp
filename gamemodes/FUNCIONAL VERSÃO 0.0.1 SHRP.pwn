function UrushiJutsu(playerid)
{
    if(Info[playerid][pProgressoDP] >= 1 || Info[playerid][pDPPatente] == 3 || Info[playerid][pDPPatente] == 4 || Info[playerid][pDPPatente] == 5 || Info[playerid][pDPPatente] == 6 ||
       Info[playerid][pDPPatente] == 9 || Info[playerid][pDPPatente] == 10 || Info[playerid][pDPPatente] == 11 || Info[playerid][pDPPatente] == 12)
    {
        new str[64], vw = GetPlayerVirtualWorld(playerid);
        new Float:x, Float:y, Float:z;
        GetPlayerVelocity(playerid, x, y, z);
        new Float:pPos[3];
        if(z != 0) return SendClientMessageEx(playerid, COLOR_WHITE, "{EF0D02}(JUTSU) Voc? tem que estar no ch?o para usar este jutsu.");

        if(Info[playerid][pChakraEnUso] < 250.0) return SemChakra(playerid);
        if(Urushi[playerid][urushiTimer] > gettime()) return JutsuNotReady2(playerid, COLOR_WHITE, "Kuchiyose Urushi", Urushi[playerid][urushiTimer]);
        //lastJutsu[playerid][jutsuLast] = 9;
        ClearSelo(playerid);
        for(new i; i<MAX_PLAYERS; i++)
        {
            switch(GetLookingTarget(35.0, 4.0, Invunerable[i] == 1, false, vw, playerid, i))
            {
                case NONE:
                {
                    if(i != MAX_PLAYERS-1) continue;
                    format(str, sizeof(str), "{EF0D02}[JUTSU] Voc? precisa est? mirando em alguem");
                    SendClientMessage(playerid, COLOR_WHITE, str);
                    break;
                }
                case PLAYER:
                {
                    Urushi[playerid][urushiAtivo] = 1;
                    Info[playerid][pChakraEnUso] -= 250.0;
                    AudioInPlayer(playerid, 50.0, 72);
                    AparecerMapaJutsu(playerid, 50.0);
                    Urushi[playerid][urushiTimer] = gettime() + URUSHI_COOLDOWN;
                    UrushiSay(playerid);
                    GetPlayerPos(playerid, pPos[0], pPos[1], pPos[2]);
                    GetXYInFrontOfPlayer(playerid, pPos[0], pPos[1], 2.0);
                    SetPlayerFacingAngleToCamera(playerid);
                    SistemaDeProgressoDP(playerid);
                    Urushi[playerid][urushiAlvo] = i;
                    SendoFarejadoID[i] = playerid;
                    Urushi[playerid][urushiObj] = CreateObject(18711, pPos[0], pPos[1], pPos[2]-2.6, 0, 0, 96);
                    InvocacaoUrushi(playerid);
                    TimerUrushi[playerid] = gettime() + URUSIHI_TIMING;
                    SetPlayerMarkerForPlayer(playerid, i, 0xFFFF00AA);
                    break;
                }
            }
        }
    }
    return 0;
}
function InvocacaoUrushi(playerid)
