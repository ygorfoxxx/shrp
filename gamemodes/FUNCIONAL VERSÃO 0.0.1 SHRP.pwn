    if(z != 0)
    {
        SendClientMessageEx(playerid, COLOR_WHITE, "{E9FE23}[AVISO]{FFFFFF} Voc? tem que estar no ch?o para fazer isso.");
        return 1;
    }
    new string[128], Float:Position[4];
    if(Info[playerid][pCamping] == 1)
