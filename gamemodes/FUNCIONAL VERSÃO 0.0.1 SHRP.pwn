    new Anbu = Info[playerid][pAnbuPatente], SkinAtual = GetPlayerSkin(playerid), str[128];
    if(!Info[playerid][pAnbuPatente]) return 0;
    if(Anbu == 1 && IsPlayerInRangeOfPoint(playerid, 0.5, -1355.7422, 1395.2744, 2002.4219)){ // Iwgakure
        if(PontoBatidoAnbu[playerid] == 1){SairPontoAnbu(playerid); QuantidadeAnbulIwaga--;}else{
            Info[playerid][pMascarado] = Info[playerid][pMember];
            SkinAntesAnbu[playerid] = SkinAtual;
            SetPlayerSkin(playerid, 20174);
            PontoBatidoAnbu[playerid] = 1;
            Info[playerid][pMaskuse] = 1;
            Info[playerid][pMember] = 9;
            SistemaBandanaIDStatus(playerid);
            QuantidadeAnbulIwaga++;
            format(str, sizeof(str), "{AB7C4E}(AVISO) Voc? bateu o ponto na Anbu.");
            SendClientMessage(playerid, -1, str);
        }
    }
    else if(Anbu == 2 && IsPlayerInRangeOfPoint(playerid, 0.5, 2197.1045, -2336.0149, 2002.4219)){ // Kirigakure
        if(PontoBatidoAnbu[playerid] == 1){SairPontoAnbu(playerid); QuantidadeAnbulKiri--;}else{
            if(Info[playerid][pSex] == 1){
                Info[playerid][pMascarado] = Info[playerid][pMember];
                SkinAntesAnbu[playerid] = SkinAtual;
                SetPlayerSkin(playerid, 20202);
                PontoBatidoAnbu[playerid] = 1;
                Info[playerid][pMaskuse] = 1;
                Info[playerid][pMember] = 9;
                SistemaBandanaIDStatus(playerid);
                QuantidadeAnbulKiri++;
                format(str, sizeof(str), "{AB7C4E}(AVISO) Voc? bateu o ponto na Anbu.");
                SendClientMessage(playerid, -1, str);

            }else if(Info[playerid][pSex] == 2)
            {
                Info[playerid][pMascarado] = Info[playerid][pMember];
                SkinAntesAnbu[playerid] = SkinAtual;
                SetPlayerSkin(playerid, 20208);
                PontoBatidoAnbu[playerid] = 1;
                Info[playerid][pMaskuse] = 1;
                Info[playerid][pMember] = 9;
                SistemaBandanaIDStatus(playerid);
                QuantidadeAnbulKiri++;
                format(str, sizeof(str), "{AB7C4E}(AVISO) Voc? bateu o ponto na Anbu.");
