function PegarCogumelo(playerid)
{
    new str[64];
    for(new h = 0; h < sizeof(Cogumelos); h++)
    {
        if(IsPlayerInRangeOfPoint(playerid, 1.5, Cogumelos[h][pCogumeloX], Cogumelos[h][pCogumeloY], Cogumelos[h][pCogumeloZ]))
