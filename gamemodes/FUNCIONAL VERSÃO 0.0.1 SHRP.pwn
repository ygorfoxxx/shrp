function DamageRasengan(playerid, targetid)
{
    new Float:targetPos[3], Float:AngleFacing;
    if(Rasengan[playerid][RasenganTick] == 0) return 0;
    if(Rasengan[playerid][RasenganTick] >= 1)
    {
        if(Rasengan[playerid][RasenganTick] == 7){
            //SendClientMessage(playerid, -1, "PASSOU NA finaliza??o DO TICK");

            SetDamageToPlayer(playerid, targetid, GetDamageToPlayer(playerid, FUTON, 0, 70.0));

            ////reseta variaveis do targetid
            PlayerParalisado[targetid] = 0;
            Invunerable[targetid] = 0;
            GetPlayerPos(targetid, targetPos[0], targetPos[1], targetPos[2]);
            GetPlayerFacingAngle(targetid, AngleFacing);
            Rasengan[playerid][RasenganTick] = 0;
            RasenganSpeed(targetid, -0.45);//Jogar o Alvo pra Longe
            Rasengan[playerid][RasenganObjEffect] = CreateObject(15120, targetPos[0], targetPos[1], targetPos[2], 0.0, 0.0, AngleFacing);//Efeito
            AudioInPlayer(targetid, 50.0, 137);
            //reseta as variaveis e remove os obj do playerid
            ApplyAnimation(playerid, "Mtr_nt","NWE_NARUTO_3", 4.0, 0, 1, 1, 0, 1, 1);
            Invunerable[playerid] = 0;
            LastJutsuReset(playerid);
            RasenganOff(playerid);
            FraturarNinja(targetid);
            SetTimerEx("RasenganOffObj", 500, false, "d", playerid);
            return 1;

        }else if(Rasengan[playerid][RasenganTick] <= 6){
            //SendClientMessage(playerid, -1, "PASSOU NA CONTAGEM DO TICK");
            SetDamageToPlayer(playerid, targetid, GetDamageToPlayer(playerid, FUTON, 0, 20.0));
            //SetDamageToPlayer(playerid, targetid, GetDamageToPlayer(playerid, MENTOR, 1, 95.0)); //da dano
            AudioInPlayer(targetid, 50.0, 100); //faz o som do tick
            AparecerMapaJutsu(targetid, 100.0);
            PlayerParalisado[targetid] = 1;
            //ApplyAnimation(targetid, "Mtr_nt","NWE_NARUTO_6", 4.0, 0, 1, 1, 1, 700, 1); // PARALISAR ALVO
            SetPlayerInFrontOfPlayer(targetid, playerid);
            //Invunerable[playerid] = 1; Invunerable[targetid] = 1;
            Rasengan[playerid][RasenganTick]++; HitCountCombos(playerid);
        }
    }
    SetTimerEx("DamageRasengan", 200, false, "dd", playerid, targetid);
    return 0;
}
