CMD:darsenin(playerid, params[])
{
    new id, senin, string[128], cargo[64];
    if(Info[playerid][pAdminZC] == 0 || Info[playerid][pAdminZC] == 2 || Info[playerid][pAdminZC] == 3 || Info[playerid][pAdminZC] == 4 || Info[playerid][pAdminZC] == 5) return SendClientMessage(playerid, 0xFF0000AA, "[ERRO]: Este comando n?o existe!");
    if(sscanf(params, "dd", id, senin)) return SendClientMessage(playerid, 0xFF230AFF, "[ERRO] Use /darsenin [PLAYER] [1-4].");
    if(IsPlayerConnected(id)){
        switch(senin)
        {
            case 0: cargo = "Nenhum";
            case 1: cargo = "Hirashin";
            case 2: cargo = "Maldi??o";
            case 3: cargo = "Cura";
            case 4: cargo = "Rinen-Sharingan";
        }
        format(string, sizeof(string), "{AB7C4E}(SENIN) Voc? deu ao jogador{FFFFFF} %s{AB7C4E} o senin de ({FFFFFF}%s{AB7C4E}).", PlayerNameDados(id), cargo);
