{
    new Float:rY[2] = { 45.0, -45.0},
        Float: rX = 180.0;

    KiruNoChakra[playerid][kiruObj][wind] = LanzarJutsu(playerid, 15410, 60.0, 50.0, .rxOffset = rX, .ryOffset = rY[wind]);
