new player_obj[MAX_PLAYERS];

CMD:criarobj(playerid, params[])
{
    if(Info[playerid][pAdminZC] == 0 || Info[playerid][pAdminZC] == 2 || Info[playerid][pAdminZC] == 3 || Info[playerid][pAdminZC] == 4 || Info[playerid][pAdminZC] == 5) return 0;
    new modelid;
    if(sscanf(params, "i", modelid)) return SendClientMessage(playerid, -1, "{FF0000}[ERRO]{FFFFFF} Digite /criarobj [modelid].");

    if(IsValidDynamicObject(player_obj[playerid]))
    {
        DestroyDynamicObject(player_obj[playerid]);
    }

    new Float:x, Float:y, Float:z;
    GetPlayerPos(playerid, x, y, z);
    GetXYInFrontOfPlayer(playerid, x, y, 2.0);z = z+1.0;

    player_obj[playerid] = CreateDynamicObject(modelid, x, y, z, 0.0, 0.0, 0.0, -1, -1, -1, 300.0, 300.0);

    EditDynamicObject(playerid, player_obj[playerid]);
    SendClientMessage(playerid, 0x33CCFFFF, "[INFO] Objeto criado. Ap?s editar, clique em 'Salvar' para receber as coordenadas.");
    return 1;
}


