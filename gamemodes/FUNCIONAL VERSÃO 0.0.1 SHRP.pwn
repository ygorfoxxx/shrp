function PilulaDVitalidade(playerid)
{
    new slot = slotX[playerid];
    new Float:VidaMaxima = Info[playerid][pHealthMaximo];
    new Float:VidaAgora = Info[playerid][pHealthEnUso];
    new Float:porcentagem = (VidaAgora / VidaMaxima) * 100;
    if (porcentagem < 100.0){
        if(PilulaDVitalidadeUse[playerid] == 1)
        {
            SendClientMessage(playerid, -1, "{AB7C4E}(PILULA) Voc? j? est? usando a P?lula de Vitalidade.");
        }else{
            VidaFaltando[playerid] = VidaMaxima - VidaAgora;
            PilulaDVitalidadeUse[playerid] = 1;
            RemoveItemSlot(playerid, 3, 1, slot);
            IniciarLoopPilulaDVitalidade(playerid);
        }
    }else{
        SendClientMessage(playerid, -1, "{AB7C4E}(PILULA) Sua vida j? est? completa.");
    }
    return 1;
}
function IniciarLoopPilulaDVitalidade(playerid)
{
    if(PilulaDVitalidadeUse[playerid] == 1)
    {
        TimerPilulaDeVitalidade[playerid] = SetTimerEx("PilulaDVitalidade_Tick", PILULAVITA_INTERVALO, true, "i", playerid);
    }

    return 1;
}
function PilulaDVitalidade_Tick(playerid)
{
    if(PilulaDVitalidadeUse[playerid] == 0) return 0;
    new Float:VidaMaxima = Info[playerid][pHealthMaximo];
    new Float:VidaAgora = Info[playerid][pHealthEnUso];
    new Float:porcentagem = (VidaAgora / VidaMaxima) * 100;
    if (porcentagem >= 100.0)
    {
        VidaAgora = VidaMaxima;
        ResetarVariavelPilulaVitalidade(playerid);
        return 1;
    }
    if (PilulaDVitalidadeUse[playerid] == 1)
    {
        if(porcentagem < 100.0)
        {
            new Float:CuraPorTick = VidaMaxima * 0.05; // Cura 5% por tick
            if (CuraPorTick > VidaFaltando[playerid]) // Verifica se a cura por tick excede a quantidade de vida faltante
            {
                CuraPorTick = VidaFaltando[playerid]; // Define a cura por tick como a quantidade de vida faltante
            }
            // Cura parcial
            VidaAgora += CuraPorTick;
            VidaFaltando[playerid] -= CuraPorTick; // Atualiza a quantidade que falta ser curada
            NewSetHP(playerid, VidaAgora);
            new str[320];
            //format(str, sizeof(str), "{AB7C4E}(PILULA) Voc? foi curado em %.2f%%. Faltando %.2f de vida para ser completamente curado.", CuraPorTick * 100 / VidaMaxima, VidaFaltando[playerid]);
            //SendClientMessage(playerid, -1, str);
            if (VidaFaltando[playerid] == 0.0)
            {
                // Cura completa
                VidaAgora = VidaMaxima;
                ResetarVariavelPilulaVitalidade(playerid);
            }
        }
    }

    return 1;
}
function ResetarVariavelPilulaVitalidade(playerid)
{
    PilulaDVitalidadeUse[playerid] = 0; // Desativa o uso da p?lula de vitalidade
    KillTimer(TimerPilulaDeVitalidade[playerid]); // Cancela o timer
    SendClientMessage(playerid, -1, "{AB7C4E}(PILULA) Sua Pilula de Vitalidade n?o tem mais efeito.");
    return 1;
}
