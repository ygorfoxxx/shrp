    InterpolateCameraPos(playerid, playerPos[0], playerPos[1], playerPos[2]+100, playerPos[0], playerPos[1], playerPos[2], 10000, CAMERA_MOVE);
    InterpolateCameraLookAt(playerid, playerPos[0], playerPos[1], playerPos[2]+100, playerPos[0], playerPos[1], playerPos[2], 10000, CAMERA_MOVE);
