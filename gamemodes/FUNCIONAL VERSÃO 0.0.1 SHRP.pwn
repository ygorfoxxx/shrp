stock ConfrontPlayer(playerid, targetid)
{
    new Float:pV[3], Float:x, Float:y, Float:z, Float:a;
    if(!IsPlayerConnected(playerid) || !IsPlayerConnected(targetid)) return 0;
    GetPlayerPos(targetid, x, y, z);
    GetPlayerVelocity(targetid, pV[0], pV[1], pV[2]);
    GetPlayerFacingAngle(targetid, a);
    x += (1.2 * floatsin(-a, degrees));
    y += (1.2 * floatcos(-a, degrees));
    SetPlayerPos(playerid, x, y, z);
    SetPlayerVelocity(playerid, pV[0], pV[1], pV[2]);
    return 1;
}
// ==========================================================
//  [FIX] ConfrontPlayer no chao (ground) - versao segura
//  Evita "snap/teleporte" quando o atacante esta de lado.
//  Mantem ConfrontPlayer normal para air-combo.
// ==========================================================
#if !defined GROUND_CONFRONT_OFFSET
    #define GROUND_CONFRONT_OFFSET    (0.70)   // distancia pequena (0.60 ~ 0.90)
#endif

#if !defined GROUND_CONFRONT_MAX_DIST
    #define GROUND_CONFRONT_MAX_DIST  (2.2)    // so se estiver bem perto
#endif

#if !defined GROUND_CONFRONT_MAX_ANGLE
    #define GROUND_CONFRONT_MAX_ANGLE (55.0)   // +/- graus aceitos (quanto menor, mais "preciso")
#endif

stock bool:ShouldConfrontGround(attackerid, victimid)
{
    if(!IsPlayerConnected(attackerid) || !IsPlayerConnected(victimid)) return false;

    new Float:ax, Float:ay, Float:az;
    new Float:vx, Float:vy, Float:vz;
    GetPlayerPos(attackerid, ax, ay, az);
    GetPlayerPos(victimid, vx, vy, vz);

    new Float:dx = vx - ax;
    new Float:dy = vy - ay;

    new Float:dist = floatsqroot(dx*dx + dy*dy);
    if(dist > GROUND_CONFRONT_MAX_DIST) return false;
    if(dist < 0.001) return true;

    dx /= dist;
    dy /= dist;

    new Float:a;
    GetPlayerFacingAngle(attackerid, a);

    // Vetor "pra frente" do atacante (mesmo padrao do seu dash/knock)
    new Float:fx = floatsin(-a, degrees);
    new Float:fy = floatcos(-a, degrees);

    // dot = cos(angulo). Quanto mais perto de 1.0, mais "de frente".
    new Float:dot = (fx * dx) + (fy * dy);
    new Float:minDot = floatcos(GROUND_CONFRONT_MAX_ANGLE, degrees);

    return (dot >= minDot);
}

stock ConfrontPlayerGroundSafe(playerid, targetid)
{
    // playerid = quem vai ser movido (normalmente a vitima)
    // targetid = referencia (normalmente o atacante)
    if(!IsPlayerConnected(playerid) || !IsPlayerConnected(targetid)) return 0;

    // Se estiver no ar, deixa o sistema antigo controlar (air combo)
    if(IsPlayerInAir[playerid] || IsPlayerInAir[targetid]) return ConfrontPlayer(playerid, targetid);

    // No chao: so confronta se o atacante estiver realmente de frente e perto
    if(!ShouldConfrontGround(targetid, playerid)) return 0;

    new Float:x, Float:y, Float:z, Float:a;
    new Float:pvx, Float:pvy, Float:pvz;

    GetPlayerPos(targetid, x, y, z);
    GetPlayerFacingAngle(targetid, a);

    // Preserva a velocidade original do player movido (evita "puxar" junto)
    GetPlayerVelocity(playerid, pvx, pvy, pvz);

    x += (GROUND_CONFRONT_OFFSET * floatsin(-a, degrees));
    y += (GROUND_CONFRONT_OFFSET * floatcos(-a, degrees));

    SetPlayerPos(playerid, x, y, z);
    SetPlayerVelocity(playerid, pvx, pvy, pvz);
    return 1;
}

