        KillTimer(TimerS[playerid]);
        if(Info[playerid][pChuninIns] == 3){
            format(string, sizeof(string), "{AB7C4E}(AVISO) Voc? est? desclassificado do ({FFFFFF}Exame Chunin{AB7C4E}).");
