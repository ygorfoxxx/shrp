function JutsuSelamento(playerid)
{
    if(JikuuSelado[playerid] == 1) return SendClientMessage(playerid, COLOR_WHITE, "[Selamento]: Voc? est? no meio do jutsus do Selamento (JIKUUJUTSU).");
    new Float:vPos[3], jogadorselado,
                       Float:X, Float:Y, Float:Z;
    GetPlayerVelocity(playerid, vPos[0], vPos[1], vPos[2]);
    if(vPos[2] != 0) return SendClientMessage(playerid, COLOR_WHITE, "[Selamento]: Voc? precisa est? no ch?o para usar est? jutsu!");
    if(PedindoSela[playerid] == 0)
    {
        //SendClientMessage(playerid, COLOR_WHITE, "1");
        ShowPlayerDialog(playerid, DIALOG_SELAM, DIALOG_STYLE_INPUT, "{E9FE23}Selar Corpo", "{FFFFFF}Escolha o {E9FE23}ID{FFFFFF} do ninja que deseja selar.", "OK", "");
        JutsuSelamentoSay(playerid);
    }
    if(SealedCorpo[playerid] == 1)
    {
        AnimSelar(playerid);
        PedindoSela[playerid] = 0;
        SetTimerEx("SeladoPosCorpo", 250, false, "d", playerid);
        RemovePlayerAttachedObject(playerid, 3);
        JutsuSelamentoSay(playerid);
        //SendClientMessage(playerid, COLOR_WHITE, "soltar corpo");
    }
    if(SealedPlayer[playerid] == 1)
    {
        //SendClientMessage(playerid, COLOR_WHITE, "2");
        jogadorselado = GetPVarInt(playerid, "IDDoSelado");
        GetPlayerPos(playerid, X, Y, Z);
        SetPVarFloat(jogadorselado, "SeladoX", X);
        SetPVarFloat(jogadorselado, "SeladoY", Y);
        SetPVarFloat(jogadorselado, "SeladoZ", Z);
        AnimSelar(playerid);
        SealedPlayer[playerid] = 0;
        SetTimerEx("SeladoPos", 250, false, "d", jogadorselado);
        SetTimerEx("ResetSelamento", 250, false, "d", playerid, jogadorselado);
        RemovePlayerAttachedObject(playerid, 3);
        JutsuSelamentoSayOff(playerid);
        //SendClientMessage(playerid, COLOR_WHITE, "soltar jogador");
    }
    return 1;
}
