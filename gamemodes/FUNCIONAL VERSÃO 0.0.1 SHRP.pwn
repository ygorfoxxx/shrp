function CreateActorMorto(playerid)
{
    new string[128], year, month, day, pSkin, Aldea[64], Nome[32];
    Nome = PlayerNameDados(playerid);
    pSkin = GetPlayerSkinEx(playerid);
    getdate(year, month, day);

    GetPlayerPos(playerid, CorpoData[playerid][corpoX], CorpoData[playerid][corpoY], CorpoData[playerid][corpoZ]);
    CorpoData[playerid][corpoID] = playerid;
    CorpoData[playerid][corpoNome] = Nome;
    CorpoData[playerid][corpoSkin] = pSkin;
    CorpoData[playerid][corpoVuneravel] = 1;
    CorpoData[playerid][corpoVW] = GetPlayerVirtualWorld(playerid);
    CorpoData[playerid][corpoVila] = Info[playerid][pMember];
    ActorMorto[CorpoData[playerid][corpoID]] = CreateDynamicActor(CorpoData[playerid][corpoSkin], CorpoData[playerid][corpoX], CorpoData[playerid][corpoY], CorpoData[playerid][corpoZ], CorpoData[playerid][corpoVuneravel], 100.0, CorpoData[playerid][corpoVW]);
    ApplyDynamicActorAnimation(ActorMorto[CorpoData[playerid][corpoID]], "ped", "EV_step", 4.1, 0, 0, 0, 1, 1);

    //Status
    format(string, sizeof(string), "{FFFFFF}Status {B85903}Inconsciente");
    CorpoData[playerid][corpoStatus] = CreateDynamic3DTextLabel(string, COLOR_WHITE, CorpoData[playerid][corpoX], CorpoData[playerid][corpoY], CorpoData[playerid][corpoZ]+0.2, 6);
    //Bandana
    switch(Info[playerid][pMember])
    {
        case 1: Aldea = "{DDA303}Iwa";
        case 2: Aldea = "{168500}Suna";
        case 3: Aldea = "{030085}Kiri";
        case 4: Aldea = "{DD0200}Konoha";
        case 5: Aldea = "{FEFF00}Kumo";
        case 6: Aldea = "{504035}Renegado";
        case 7: Aldea = "{030085}Renegado";
        case 8: Aldea = "{C5CA49}Myoboku";
        case 9: Aldea = "{C5CA49}Mascarado";
        case 10: Aldea = "{DD0200}Feudal";
        default: Aldea = "SemBandana";
    }
    format(string, sizeof(string), "{FFFFFF}Bandana %s", Aldea);
    CorpoData[playerid][corpoBandana] = CreateDynamic3DTextLabel(string, COLOR_WHITE, CorpoData[playerid][corpoX], CorpoData[playerid][corpoY], CorpoData[playerid][corpoZ]+0.1, 6);
    //Id
    format(string, sizeof(string), "(%d)",playerid);
    CorpoData[playerid][corpoIDSt] = CreateDynamic3DTextLabel(string, COLOR_WHITE, CorpoData[playerid][corpoX], CorpoData[playerid][corpoY], CorpoData[playerid][corpoZ], 6);

    SalvarCorpo(playerid);
    format(string, sizeof(string), "[LOG]: %s levou CK por %s,(%d-%d-%d)", PlayerNameDados(playerid), PlayerNameDados(playerid), month, day, year);
    Log("LOGS/ck.log", string);
    return 1;
}
stock SalvarCorpo(idcorpo)
{
