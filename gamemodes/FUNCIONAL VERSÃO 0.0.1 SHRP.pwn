function CartaReset(playerid)
{
    RemoveItem(playerid, 169, 1);
    new level = Info[playerid][pLevel];
    Info[playerid][pHealthMaximo] = 500+10*level; // Reset Vida
    Info[playerid][pHealthEnUso] = 500+10*level; // Reset Vida
    Info[playerid][pChakra] = 500+10*level; // Reset Chakra
    Info[playerid][pChakraEnUso] = 500+10*level; // Reset Chakra
    // agora reset de tai, nin, gen - com base no treinamento
    new TaijutsuTreino = Info[playerid][pTaijutsuPM];
    new NinjutsuTreino = Info[playerid][pNinjutsuPM];
    new GenjutsuTreino = Info[playerid][pGenjutsuPM];
    // Agora resetar os pontos de atributos
    Info[playerid][pTaijutsu] = 0;
    Info[playerid][pNinjutsu] = 0;
    Info[playerid][pGenjutsu] = 0;
    // Calcular os atributos finais com base no treinamento acumulado
    if(TaijutsuTreino == 5){Info[playerid][pTaijutsu] = 0;}
    if(NinjutsuTreino == 5){Info[playerid][pNinjutsu] = 0;}
    if(GenjutsuTreino == 5){Info[playerid][pGenjutsu] = 0;}
    if(TaijutsuTreino != 5){Info[playerid][pTaijutsu] += Info[playerid][pTaijutsuPM] / 5;}
    if(NinjutsuTreino != 5){Info[playerid][pNinjutsu] += Info[playerid][pNinjutsuPM] / 5;}
    if(GenjutsuTreino != 5){Info[playerid][pGenjutsu] += Info[playerid][pGenjutsuPM] / 5;}
    // Resetar os pontos de treinamento para a pr?xima rodada de treinamento
    Info[playerid][pTaijutsuP] = 0;
    Info[playerid][pNinjutsuP] = 0;
    Info[playerid][pGenjutsuP] = 0;
    Info[playerid][pPuntosNinja] = 0;
    Info[playerid][pPuntosNinja] = 5*level;

    return 1;
}
