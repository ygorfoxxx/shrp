    if(Info[playerid][pChakraEnUso] < 300.0)  return SemChakra(playerid);
    if(Rasengan[playerid][RasenganTimer] > gettime()) return JutsuNotReady2(playerid, COLOR_WHITE, "Estilo Vento: Espiral de Vento", Rasengan[playerid][RasenganTimer]);
    {
        if(Info[playerid][pFuton] == 0) return 0;
        if(Info[playerid][pFuton] >= 1)
        RasenganAt[playerid] = 1;
        Rasengan[playerid][RasenganClick] = 1;
        Rasengan[playerid][RasenganAtivo] = 1;
