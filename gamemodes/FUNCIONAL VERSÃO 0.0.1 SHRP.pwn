TeclaVerCemiterio(playerid, newkeys, oldkeys)
{
    new string[500], str[50];
    if(BtnDireito[playerid] == 1) return 0;
    if(PRESSED(KEY_CTRL_BACK))
    {
        if(IsPlayerInRangeOfPoint(playerid, 0.5, 673.9154, -1239.2898, 7.7222))//Cemiterio Konoha
        {
            Audio_Play(playerid, 58);
            // Verificar quantos ninjas foram enterrados
            new numEnterrados;
            for(new c = 0; c < MAX_CEMITERIO; c++)
            {
                if(Cemiterio[c][cemiterioCorpo] != 0)
                {
                    numEnterrados++;
                }
            }
            if(numEnterrados > 0)
            {
                // Exibir os nomes dos ninjas enterrados
                for(new c = 0; c < MAX_CEMITERIO; c++)
                {
                    if(Cemiterio[c][cemiterioCorpo] != 0)
                    {
                        format(str, sizeof(str), "{FFFFFF}%s\n", Cemiterio[c][cemiterioNome]);
                        strcat(string, str);
                    }
                }
            }
            else
            {
                // Caso nenhum ninja tenha sido enterrado
                format(str, sizeof(str), "{FFFFFF}Nenhum ninja foi enterrado.");
                strcat(string, str);
            }
            ShowPlayerDialog(playerid, DIALOG_CEMITERIO, DIALOG_STYLE_LIST, "{E9FE23}Ninjas Enterrados", string, "Ver", "Fechar");
        }
    }
}
TeclaFalarCemiterio(playerid, newkeys, oldkeys)
{
    if(BtnDireito[playerid] == 1) return 0;
    if(PRESSED(KEY_CTRL_BACK))
    {
        if(IsPlayerInRangeOfPoint(playerid, 1.5, 695.4669, -1213.5353, 7.7632))//Cemiterio Konoha
        {
            Audio_Play(playerid, 58);
            ShowPlayerDialog(playerid, DIALOG_VELORIO, DIALOG_STYLE_LIST, "{E9FE23}Cemiterio", "{FFFFFF}Iniciar Velorio", "Aceitar", "Fechar");
        }
    }
}
stock DeletarCorpo(corpoid)
{
    new string[128], file[128];
    format(file , sizeof file, PASTA_CORPOS, corpoid);
    if(DOF2_FileExists(file))
    {
        DOF2_RemoveFile(file);
    }
    return 1;
}
stock SalvarCemiterio(tumuloid)
{
    new string[128], file[128];
    format(file, sizeof(file), PASTA_CEMITERIO, tumuloid);
    if(!DOF2_FileExists(file)) DOF2_CreateFile(file);
    {
        format(string, sizeof(string), "IDTumulo", Cemiterio[tumuloid][cemiterioTumulo]);
        DOF2_SetInt(file, string, Cemiterio[tumuloid][cemiterioTumulo]);
        format(string, sizeof(string), "NomeTumulo", Cemiterio[tumuloid][cemiterioNome]);
        DOF2_SetString(file, string, Cemiterio[tumuloid][cemiterioNome]);
        format(string, sizeof(string), "CorpoTumulo", Cemiterio[tumuloid][cemiterioCorpo]);
        DOF2_SetInt(file, string, Cemiterio[tumuloid][cemiterioCorpo]);
        DOF2_SaveFile();
    }
    return 1;
}
stock CarregarCemiterio(tumuloid)
{
    new string[128], file[128];
    format(file, sizeof(file), PASTA_CEMITERIO, tumuloid);
    if(DOF2_FileExists(file))
    {
        format(string, sizeof(string), "IDTumulo", Cemiterio[tumuloid][cemiterioTumulo]);
        Cemiterio[tumuloid][cemiterioTumulo] = DOF2_GetInt(file, string);
        format(string, sizeof(string), "NomeTumulo%s", Cemiterio[tumuloid][cemiterioNome]);
        format(Cemiterio[tumuloid][cemiterioNome], 32, DOF2_GetString(file, string));
        format(string, sizeof(string), "CorpoTumulo", Cemiterio[tumuloid][cemiterioCorpo]);
        Cemiterio[tumuloid][cemiterioCorpo] = DOF2_GetInt(file, string);
    }
    return 1;
}
