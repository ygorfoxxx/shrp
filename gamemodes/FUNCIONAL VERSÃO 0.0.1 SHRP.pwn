ChidoriCast(playerid, newkeys)
{
    new str[64], vw = GetPlayerVirtualWorld(playerid);
    if(PlayerParalisado[playerid] == 1 || ChuteCimaHitted[playerid] == 1 || HittedDoroji[playerid] == 1 || HittedKageshibari[playerid] == 1) return 0;
    if(AnimChidoriJutsu[playerid] == 2 || AnimChidoriJutsu[playerid] == 3) return 0;
    if(AnimChidoriJutsu[playerid] == 1 && ChidoriHitImpossible[playerid] == 0)
    if(newkeys & KEY_FIRE)
    {
        if(ChidoriHitImpossible[playerid] == 1) return 0;
        for(new i; i<MAX_PLAYERS; i++){
            switch(GetLookingTarget(80.0, 3.0, Invunerable[i] == 1, false, vw, playerid, i))
            {
                case NONE:
                {
                    if(i != MAX_PLAYERS-1) continue;
                    break;
                }
                case PLAYER:
                {
                    SetPlayerFacingAngleToCamera(playerid);
                    SeguindoPlayer[playerid] = 1;
                    TargetSeguindoArashi[playerid] = 1;
                    TargetSeguir[playerid] = i;
                    ChidoriHitImpossible[playerid] = 1;
                    lastJutsu[playerid][jutsuLast] = 1; // Arashi
                    ApplyAnimation(playerid, "Shinobi_Anim","Raikiri2", 4.0, 0, 1, 1, 0, 700, 1);
                    AnimChidoriJutsu[playerid] = 2;
                    AudioInPlayer(playerid, 50.0, 48);
                    AparecerMapaJutsu(playerid, 50.0);
                    ChidoriON[playerid] = 2;
                    MeleeJutsu(playerid);
                    SetTimerEx("ChidoriOFF", 700, 0, "d", playerid);
                    SetTimerEx("FollowPlayer", 85, 0, "ddf", playerid, i, 0.8);
                    SeguindoPlayerTimer[playerid] = SetTimerEx("ResetFollowArashi", 200, false, "d", playerid);
                    break;
                }
            }
        }
    }
    return 0;
}
