function PagarDinheiro(playerid, amount)
{
    new RyosBolso = GetPlayerCash(playerid),
        RyosBanco = Info[playerid][pAccount],
        string[128];
    if(RyosBolso >= amount){//verificar se o jogador tem dinheiro no bolso se n?o paga com o banco
