    PlayerTextDrawHide(playerid, SlotsBreve[playerid][1]); // Slots Breve
    PlayerTextDrawHide(playerid, SlotsBreve[playerid][2]); // Slots Breve
    PlayerTextDrawHide(playerid, SlotsBreve[playerid][3]); // Slots Breve
