function ComboCombate(playerid, victimid)
{
    // auto-limpeza simples caso voc? N?O tenha isso em outro lugar
    if(ComboTimerResetClick[playerid] && gettime() >= ComboTimerResetClick[playerid])
        ComboTimerResetClick[playerid] = 0;

    if(ComboTimerResetClick[playerid] || UtilizouComboClick[playerid]) return 0;

    // S? arma no CH?O (evita armar finisher em air)
    if(IsPlayerInAir[playerid] || IsPlayerInAir[victimid])
    {
        ComboCount[playerid] = 0;
        ComboTimerReset[playerid] = gettime() + RESET_COMBO;
        return 1;
    }

    // Conta hits
    ComboCount[playerid]++;
    ComboTimerReset[playerid] = gettime() + RESET_COMBO;

    // A partir de 3 hits, ARMA (mas n?o executa)
    if(ComboCount[playerid] == 3)
    {
        ComboTargetID[playerid] = victimid;
        QuemIniciouOCombo[victimid] = playerid;

        ComboCount[playerid] = 0;
        ComboCombateUtilizando[playerid] = 1;

        // trava rearmar por um tempinho (igual voc? j? fazia antes)
        ComboTimerResetClick[playerid] = gettime() + RESET_CLICK;

        AtivarCombo(playerid); // vai instruir RMB + F
    }
    return 1;
}

