function LoopCura1(playerid, giveplayerid)
{
    if(GetPVarInt(giveplayerid, "LoopCura") == 1)
    {
        new
            Float:HealthEnUso, Float:HealthMaximo;
        HealthEnUso = Info[giveplayerid][pHealthEnUso];
        HealthMaximo = Info[giveplayerid][pHealthMaximo];
        if(ImpossibleCurarFraturado[playerid] == 0 && CurandoQuebrado[playerid] == 0){CuraDoFraturado(playerid, giveplayerid);}
        if(ImpossibleCurarQuebrado[playerid] == 0 && CurandoFraturado[playerid] == 0){CuraDoQuebrado(playerid, giveplayerid);}
        if(CurandoQuebrado[playerid] == 0 && CurandoFraturado[playerid] == 0)
        {
            //SendClientMessage(playerid, -1, "curando normal");
            if(HealthEnUso >= HealthMaximo)
            {
                ImpossibleCurarQuebrado[playerid] = 0;
                ImpossibleCurarFraturado[playerid] = 0;
                CuraUsada[playerid] = 0;
                ApplyAnimation(playerid, "Shinobi_Anim", "Cura_1", 4.0, 0, 0, 0, 0, 1, 1);
                RemovePlayerAttachedObject(playerid, 9);
                NewSetHP(giveplayerid, HealthMaximo);
                KillTimer(timerCura[playerid]);
                KillTimer(timerCura[giveplayerid]);

                return 1;
            }
            else
            {
                if((HealthEnUso + 150.0) > HealthMaximo)
                {
                    NewSetHP(giveplayerid, HealthMaximo);
                }
                else
                {
                    NewSetHP(giveplayerid, HealthEnUso + 150.0);
                }
                AudioInPlayer(playerid, 25.0, 14);
            }
        }

    }
    SetTimerEx("LoopCura1", 4000, false, "ii", playerid, giveplayerid);
    return 1;
}
