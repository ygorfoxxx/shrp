function LivroAcademiaNinja(playerid)
{
    new str[128];
    Audio_Play(playerid, 132);
    LendoLivro[playerid] = 2;
    QualLivro[playerid] = 2;
    PaginasAtualLivro[playerid] = 1;
    LivroNinjaAcademia(playerid, 0);
    //LivroAcademiaNinjaCogumelosA(playerid, 0);
    FecharInventarioLivro(playerid);
    SelectTextDraw(playerid, 0x00FF00FF); // Ativar Sele??o
    if(Info[playerid][pMaskuse] == 1)
    {
        format(str, sizeof(str), "(Mascarado) est? lendo um livro (A Origem: Academia Ninja).");
        SendMessageProx(playerid, 25.0, str, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
    }
    else
    {
        format(str, sizeof(str), "%s est? lendo um livro (A Origem: Academia Ninja).", PlayerNameDados(playerid));
        SendMessageProx(playerid, 25.0, str, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
    }
    return 1;
}
