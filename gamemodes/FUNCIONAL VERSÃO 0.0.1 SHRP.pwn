        format(string, sizeof(string), "bancoKumogakure", DataBancoKage[bancoid][bancoKumogakure]);
        DataBancoKage[bancoid][bancoKumogakure] = DOF2_GetInt(file, string);
    }
}
function DarRyosKages(bancoid, ryos)
{
    new desconto = floatround(ryos * 0.5);
    new QuantidadeDescontada = ryos - desconto;
    switch(bancoid){
        case 0:{DataBancoKage[0][bancoKirigakure] += QuantidadeDescontada;}
        case 1:{DataBancoKage[0][bancoIwagakure] += QuantidadeDescontada;}
        case 2:{DataBancoKage[0][bancoKumogakure] += QuantidadeDescontada;}
    }
    return 1;
}
CMD:addryosbanco(playerid, params[])
{
    new bancoid, quant, banco, nome[32], str[128];
    if(Info[playerid][pAdminZC] == 0 || Info[playerid][pAdminZC] == 2 || Info[playerid][pAdminZC] == 3 || Info[playerid][pAdminZC] == 4 || Info[playerid][pAdminZC] == 5) return SendClientMessage(playerid, 0xFF0000AA, "[ERRO]: Este comando n?o existe!");
    if(sscanf(params, "ddd", bancoid, quant, banco)) return SendClientMessage(playerid, -1, "Use /addryosbanco 0, quantidade, banco (0 kiri | 1 iwagakure)");
    switch(banco)
    {
        case 0:{nome = "Kirigakure"; DataBancoKage[bancoid][bancoKirigakure] += quant;}
        case 1:{nome = "Iwagakure"; DataBancoKage[bancoid][bancoIwagakure] += quant;}
        case 2:{nome = "Kumogakure"; DataBancoKage[bancoid][bancoKumogakure] += quant;}
    }
    format(str, sizeof(str), "{AB7C4E} (AVISO)Voc? adicionou no banco de ({FFFFFF}%s{AB7C4E}) a quanidade de ({FFFFFF}%d{AB7C4E}) ryo's.", nome, quant);
    SendClientMessage(playerid, -1, str);
    return 1;
}
CMD:verbanco(playerid, params[])
{
    new bancoid, nome[32], str[128], ryos, banco;
    if(Info[playerid][pAdminZC] == 0 || Info[playerid][pAdminZC] == 2 || Info[playerid][pAdminZC] == 3 || Info[playerid][pAdminZC] == 4 || Info[playerid][pAdminZC] == 5) return SendClientMessage(playerid, 0xFF0000AA, "[ERRO]: Este comando n?o existe!");
    if(sscanf(params, "dd", bancoid, banco)) return SendClientMessage(playerid, -1, "Use /verbanco 0, banco (0 kiri | 1 iwagakure)");
    switch(banco){
        case 0:{
            nome = "Kirigakure";
            ryos = DataBancoKage[bancoid][bancoKirigakure];
        }
        case 1:{
            nome = "Iwagakure";
            ryos = DataBancoKage[bancoid][bancoIwagakure];
        }
        case 2:{
            nome = "Kumogakure";
            ryos = DataBancoKage[bancoid][bancoKumogakure];
        }
    }
    format(str, sizeof(str), "O banco de %s tem exatamente %d", nome, ryos);
    SendClientMessage(playerid, -1, str);
    return 1;
}
