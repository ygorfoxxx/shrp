        format(string, sizeof(string), "{AB7C4E}(AVISO) O Ninja ({FFFFFF}%s{AB7C4E}) n?o tem horas suficiente para ser promovido.", PlayerNameDados(giveplayerid));
        SendClientMessage(playerid, -1, string);
    }
    return 1;
