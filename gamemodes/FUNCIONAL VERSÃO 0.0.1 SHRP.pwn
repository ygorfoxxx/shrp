function VerificarPosicaoDP(playerid)
{
    if(IsPlayerInRangeOfPoint(playerid, 1.5, -1406.4342, 1564.0348, 7.8914))
    {
        if(Info[playerid][pDPPatente] >= 1 && Info[playerid][pMember] == 1) // DelegaciaDP Iwagakure
        {
            VerificarPonto(playerid);
        }
    }
    else if(IsPlayerInRangeOfPoint(playerid, 1.5, 2303.8323, -2262.5166, 30.6717))
    {
        if(Info[playerid][pDPPatente] >= 7 && Info[playerid][pMember] == 3) // DelegaciaDP Kiri
        {
            VerificarPonto(playerid);
        }
    }
    return 1;
