function KatsuyuCast(playerid)
{
    if(!Katsuyu[playerid][katsuCast]) return 0;
    for(new i; i<MAX_PLAYERS; i++)
    {
        if(IsPlayerInRangeOfPoint(i, 2.5, Katsuyu[playerid][katsuPos][0], Katsuyu[playerid][katsuPos][1], Katsuyu[playerid][katsuPos][2]) && GetPlayerVirtualWorld(i) == Katsuyu[playerid][katsuVW])
        {
            AudioInArea(i, 14, Katsuyu[playerid][katsuPos][0], Katsuyu[playerid][katsuPos][1], Katsuyu[playerid][katsuPos][2], 50.0);
            if(GetPVarInt(i, "Inconsciente") >= 1){
                KillTimer(InconscienteTimerA[i]);
                KillEMSQueue(i);
                ClearAnimations(i);
                ClearInconsciente(i);
                SetPlayerWeapons(i);
                UnfreezePlayer(i);
                NewSetHP(i, 3);
                SeladoInconsciente[i] = 0;
            }
            curakatsuyu(i);
        }
    }
    SetTimerEx("KatsuyuCast", 3500, false, "d", playerid);
    return 0;
}
function curakatsuyu(playerid)
{
    new Float:HealthEnUso, Float:HealthMaximo;
        HealthEnUso = Info[playerid][pHealthEnUso];
        HealthMaximo = Info[playerid][pHealthMaximo];
    new Float:QuantidadeCura = (HealthMaximo * 0.05); // 5% de cura
    new Float:NovaVida = HealthEnUso + QuantidadeCura;
        if(NovaVida > HealthMaximo){
            NewSetHP(playerid, HealthMaximo);
        }else{
            NewSetHP(playerid, NovaVida);
    }
}
function KatsuyuOFF(playerid)
{
    if(!Katsuyu[playerid][katsuCast]) return 0;
    Katsuyu[playerid][katsuCast] = 0;
    DestroyActor(Katsuyu[playerid][katsuActor]);
    AudioInArea(playerid, 29, Katsuyu[playerid][katsuPos][0], Katsuyu[playerid][katsuPos][1], Katsuyu[playerid][katsuPos][2], 30.0);
    for(new x; x<3; x++)
    {
        DestroyDynamicObject(Katsuyu[playerid][katsuObj][x]);
    }
    return 1;
}
