    if(Info[playerid][pAdminZC] >= 2) return SendClientMessage(playerid, 0xFF0000AA, "[ERRO]: Este comando n?o existe!");
    {
        new string[128], giveplayerid;
        if(sscanf(params, "d", giveplayerid)) return SendClientMessageEx(playerid, COLOR_GRAD2, "USE: /multiconta [playerid]");
        if(IsPlayerConnected(giveplayerid))
        {
            if(Info[giveplayerid][pMulticuenta] == 0)
            {
                new autoip[32]; GetPlayerIp(giveplayerid, autoip, 32);
                format(string, sizeof(string), "[STAFF]: O Admin %s autorizou a multi conta a %s. (IP.Info: %s)", PlayerNameDados(playerid), PlayerNameDados(giveplayerid), autoip);
                ABroadCast(COLOR_ORANGE, string,2);
                Log("LOGS/multicuenta.log", string);
                Info[giveplayerid][pMulticuenta] = 1; // Multicuenta Autorizada.
                return 1;
            }
            else
            {
                format(string, sizeof(string), "[STAFF]: O Admin %s rejeitou a multi conta a %s. So pode ter uma conta.", PlayerNameDados(playerid), PlayerNameDados(giveplayerid));
                ABroadCast(COLOR_ORANGE, string,2);
                Log("LOGS/multicuenta.log", string);
                Info[giveplayerid][pMulticuenta] = 0; // Mlticuenta No Autorizada.
                return 1;
            }
        }
    }
    return 1;
}
