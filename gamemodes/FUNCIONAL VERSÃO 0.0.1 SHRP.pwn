stock SeguirTargetPorAngulo(playerid, modelid, Float:distancia, Float:velocidad, targetid, Float:zOffset = 0.0, Float:rxOffset = 0.0, Float:ryOffset = 0.0, Float:castedfrom = 1.5)
{
    new Float:fPX, Float:fPY, Float:fPZ,
    Float:fVX, Float:fVY, Float:fVZ,
    Float:object_x, Float:object_y, Float:object_z,
    Float:fZPitch, Float:arc, objectid, Float:CP[3];
    GetPlayerPos(playerid, fPX, fPY, fPZ);
    GetPlayerCameraFrontVector(playerid, fVX, fVY, fVZ);
    CP[0] = fPX + floatmul(fVX, castedfrom);
    CP[1] = fPY + floatmul(fVY, castedfrom);
    CP[2] = fPZ+zOffset;
    GetPlayerPos(targetid, object_x, object_y, object_z);
    fZPitch = atan2(object_y - fPY, object_x - fPX) - 90.0;
    arc = asin((object_z - fPZ) / distancia);
    objectid = CreateDynamicObject(modelid, CP[0], CP[1], CP[2], arc+rxOffset, ryOffset, fZPitch, GetPlayerVirtualWorld(playerid));
    Streamer_UpdateEx(playerid, CP[0], CP[1], CP[2]);
    MoveDynamicObject(objectid, object_x, object_y, object_z, velocidad, arc+rxOffset, ryOffset, fZPitch);
    return objectid;
}
stock LanzarJutsu(playerid, modelid, Float:distancia, Float:velocidad, Float:zOffset = 0.0, Float:rxOffset = 0.0, Float:ryOffset = 0.0, Float:castedfrom = 1.5)
{
    new Float:fPX, Float:fPY, Float:fPZ,
    Float:object_x, Float:object_y, Float:object_z,
    Float:fFacingAngle, Float:fZPitch, Float:arc, objectid, Float:CP[3];
    // Pegar a posi??o e o ?ngulo de dire??o do jogador
    GetPlayerPos(playerid, fPX, fPY, fPZ);
    GetPlayerFacingAngle(playerid, fFacingAngle);
    // Converter o ?ngulo de rota??o do jogador em um vetor de movimento (eixos X e Y)
    new Float:fVX = floatsin(-fFacingAngle, degrees);
    new Float:fVY = floatcos(-fFacingAngle, degrees);
    // Definir a posi??o inicial do objeto baseado no jogador
    CP[0] = fPX + floatmul(fVX, castedfrom);
    CP[1] = fPY + floatmul(fVY, castedfrom);
    CP[2] = fPZ + zOffset;
    // Definir a posi??o final do objeto baseado na dist?ncia e dire??o do jogador
    object_x = fPX + floatmul(fVX, distancia);
    object_y = fPY + floatmul(fVY, distancia);
    object_z = fPZ;  // Se quiser adicionar um offset no eixo Z, pode ajustar aqui
    // Calcular o pitch para a rota??o correta
    fZPitch = atan2(object_y - fPY, object_x - fPX) - 90.0;
    arc = 0.0;  // Para evitar inclina??o no lan?amento, zOffset ou arc pode ser ajustado
    // Criar o objeto din?mico
    objectid = CreateDynamicObject(modelid, CP[0], CP[1], CP[2], arc + rxOffset, ryOffset, fZPitch, GetPlayerVirtualWorld(playerid));
    // Atualizar o streamer para garantir que o objeto apare?a corretamente
    Streamer_UpdateEx(playerid, CP[0], CP[1], CP[2]);
    // Mover o objeto na dire??o que o jogador est? olhando
    MoveDynamicObject(objectid, object_x, object_y, object_z, velocidad, arc + rxOffset, ryOffset, fZPitch);
    return objectid;
}


