function Bandagem(playerid) // Bandagem
{
    new slot = slotX[playerid];
    new Float:VidaMaxima = Info[playerid][pHealthMaximo];
    new Float:VidaAgora = Info[playerid][pHealthEnUso];
    new Float:porcentagem = (VidaAgora / VidaMaxima) * 100;
    if(TimerBandagem[playerid] > gettime()) return ItemNotReady(playerid, COLOR_WHITE, "Bandagem", TimerBandagem[playerid]);
    if(porcentagem >= 100.0)
    {
        SendClientMessage(playerid, -1, "{AB7C4E}(BANDAGEM) Sua vida se encontra cheia.");
        return 1;
    }
    else if(porcentagem <= 90.0)
    {
        new Float:VidaFaltando = VidaMaxima * 0.1;
        VidaAgora += VidaFaltando;
    }
    else if(porcentagem < 100.0)
    {
        new Float:VidaFaltando = VidaMaxima - VidaAgora;
        VidaAgora += (VidaFaltando * (porcentagem - 90.0) / 10.0);
    }
    TimerBandagem[playerid] = gettime() + BANDAGEM_COOLDOWN;
    NewSetHP(playerid, VidaAgora);
    RemoveItemSlot(playerid, 2, 1, slot);
    return 1;
}
