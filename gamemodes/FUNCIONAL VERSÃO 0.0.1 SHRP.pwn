#define NT_Z_MAIN            (0.32)
#define NT_SHADOW_Z_OFFSET   (0.006)

#define NT_COLOR_MAIN        (0xFFFFFFFF) // branco
#define NT_COLOR_SHADOW      (0x000000FF) // preto
