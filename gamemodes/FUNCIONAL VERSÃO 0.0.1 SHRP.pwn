function AtualizarInventarioRoubo(playerid, targetid)
{
    new string[40];
    for(new x = 0; x<20; x++)
    {
        switch(GetItemType(Inventario[targetid][pSave][x]))
        {
            case 0: // Sem Item
            {
                Inventario[targetid][pSave][x] = 0;
                Inventario[targetid][pTotalSave][x] = 0;
                PlayerTextDrawSetString(playerid, SlotQuantidade[playerid][x], "_");
                PlayerTextDrawSetString(playerid, Slots[playerid][x], ObjetosNaruto[0][itemimage]);
            }
            case 1: // Item
            {
                format(string, sizeof(string), "%d", Inventario[targetid][pTotalSave][x]);
                PlayerTextDrawSetString(playerid, SlotQuantidade[playerid][x], string);
                PlayerTextDrawSetString(playerid, Slots[playerid][x], ObjetosNaruto[Inventario[targetid][pSave][x]][itemimage]);
            }
        }
    }
    return 1;
}
AoClicarNoItemRoubo(playerid, PlayerText:playertextid)
{
    new str[64];
    for(new x; x<20; x++)
    {
        if(playertextid == Slots[playerid][x] && Inventario[EstaRoubandoAlvo[playerid]][pSave][x] != 0)
        {
            slotX[playerid] = x;
            if(InventarioClickRoubo[playerid] == 2){
                InventarioClickRoubo[playerid] = 0;
                RoubarItem(playerid, Inventario[EstaRoubandoAlvo[playerid]][pSave][x], Inventario[EstaRoubandoAlvo[playerid]][pTotalSave][x]);
            }
            InventarioClickRoubo[playerid] ++;
            PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
            OcultarDescricao(playerid);
            return 1;
        }
        else if(playertextid == Slots[playerid][x] && Inventario[EstaRoubandoAlvo[playerid]][pSave][x] == 0)
        {
            slotX[playerid] = x;
            PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
            OcultarDescricao(playerid);
            return 1;
        }
        else if(playertextid == BotaoENegociar[playerid][1])
        {
            RoubarRyos(playerid);
            return 1;
        }
    }
}
function RoubarItem(playerid, item, quant)
{
    switch(item)
    {
        case 20,29,31..32,57..62,89..172,175..178:{SendClientMessage(playerid, -1, "{AB7C4E}(ROUBO) Voc? n?o pode roubar esse item."); return 1;}
    }
    new string[128]; new x = slotX[playerid]; new targetid = EstaRoubandoAlvo[playerid];
    format(string, sizeof(string), "{AB7C4E}(ROUBO) Voc? roubou o item ({FFFFFF}%s (%d){AB7C4E}) do ninja ({FFFFFF}%s{AB7C4E}).", ObjetosNaruto[Inventario[targetid][pSave][x]][itemname], Inventario[targetid][pTotalSave][x], PlayerNameDados(EstaRoubandoAlvo[playerid]));
    SendClientMessage(playerid, -1, string);
    format(string, sizeof(string), "{AB7C4E}(ROUBO) Voc? sentiu seu item ({FFFFFF}%s (%d){AB7C4E}) sendo vasculhado.", ObjetosNaruto[Inventario[targetid][pSave][x]][itemname], Inventario[targetid][pTotalSave][x]);
    SendClientMessage(targetid, -1, string);
    RemoveItem(EstaRoubandoAlvo[playerid], item, quant);
    GivePlayerItemEx(playerid, item, quant);
    AtualizarInventarioRoubo(playerid, EstaRoubandoAlvo[playerid]);
    return 1;
}
function RoubarRyos(playerid)
{
    new string[128], ryos = GetPVarInt(EstaRoubandoAlvo[playerid], "Cash"), giveplayerid;
    giveplayerid = EstaRoubandoAlvo[playerid];
    if(ryos != 0){
        format(string, sizeof(string), "{AB7C4E}(ROUBO) Voc? sentiu seus {FFFFFF}%d{AB7C4E} ryo's sendo vasculhados.", ryos);
        SendClientMessage(giveplayerid, -1, string);
        format(string, sizeof(string), "{AB7C4E}(ROUBO) Voc? roubou {FFFFFF}%d{AB7C4E} ryo's do ninja {FFFFFF}%s{AB7C4E}.", ryos, PlayerNameDados(giveplayerid));
        SendClientMessage(playerid, -1, string);
        GivePlayerItemEx(playerid, 50, ryos);
        Tax += ryos; Bought(giveplayerid, ryos);
        AtualizarInventarioRoubo(playerid, EstaRoubandoAlvo[playerid]);
        return 1;
    }else{SendClientMessage(playerid, -1, "{AB7C4E}(ROUBO) Esse ninja n?o possui ryo's para ser roubado."); return 1;}
    return 0;
}
function RoubandoAc(playerid)
