CMD:testecauda(playerid, params[])
{
    new giveplayerid, cauda, str[64];
    if(sscanf(params, "dd", giveplayerid, cauda)) return SendClientMessage(playerid, 0xFF230AFF, "{E9FE23}[ERRO]{FFFFFF} Use {E9FE23}/testecauda [ID] [1-9]{FFFFFF}.");
    Info[giveplayerid][pJinchuriki] = cauda;
    format(str, sizeof(str), "Voc? deu ao jogador (ID:%d), a Cauda (%d)", giveplayerid, cauda);
    SendClientMessage(playerid, COLOR_WHITE, str);
    return 1;
}
function TeclaAtivacaoJinchuriki(playerid, newkeys, oldkeys)
{
    if(Info[playerid][pPrisaoBase] == 1 || ByakuganON[playerid] == 1) return 0;
    if(Jinchuriki[playerid][jinchuCast] == 0 && IsPlayerActiveJinchuriki(playerid) && PRESSED(newkeys, KEY_SPRINT | KEY_NO) && oldkeys != KEY_NO){AtivacaoJinchuriki(playerid);}
    else if(Jinchuriki[playerid][jinchuCast] == 1 && IsPlayerActiveJinchuriki(playerid) && PRESSED(newkeys, KEY_SPRINT | KEY_NO) && oldkeys != KEY_NO){DesativacaoJinchuriki(playerid);}
    return 1;
}
function AtivacaoJinchuriki(playerid, newkeys, oldkeys)
{
    if(Info[playerid][pRank] == 0 || Info[playerid][pRank] == 1){return 1;}
    if(Info[playerid][pRank] == 2){ // Chunin
