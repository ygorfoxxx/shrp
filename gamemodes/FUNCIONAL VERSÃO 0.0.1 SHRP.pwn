                Streamer_UpdateEx(playerid, Position[0], Position[1], Position[2]);
                MoveDynamicObject(Iwakai[playerid][iwaObject], Position[0], Position[1], Position[2], 17.0, 0.0, 0.0, 0.0);
                ClearSelo(i);
                EmCombateIniciar(playerid);
                EmCombateIniciar(i);
                PCarregandoChakra(i);
                AudioInPlayer(i, 30.0, 53);
                if(Izanagi[i][IzanagiAtivado]){SetTimerEx("IzanagiVoltar", 100, false, "d", i);}else{DamageIwakai(playerid, i);}
