function CarregarBanco(bancoid)
{
    new string[128], file[128];
    format(file , sizeof file, PASTA_BANCOKAGE, bancoid);
    if(DOF2_FileExists(file))
    {
        format(string, sizeof(string), "bancoKirigakure", DataBancoKage[bancoid][bancoKirigakure]);
        DataBancoKage[bancoid][bancoKirigakure] = DOF2_GetInt(file, string);
