function PlayerLogin(playerid)
{
    //INICIO OCULTAR LOGIN
    PlayerTextDrawHide(playerid, TXDLogin[playerid][0]);
    PlayerTextDrawHide(playerid, TXDLogin[playerid][1]);
    PlayerTextDrawHide(playerid, TXDRegistro[playerid][0]);
    PlayerTextDrawHide(playerid, TXDRegistro[playerid][1]);
    TextDrawHideForPlayer(playerid, BotaoRegistro[0]);
    TextDrawHideForPlayer(playerid, BotaoLogin[0]);
    CancelSelectTextDraw(playerid);
    //ElementoIn(playerid);
    //EscolhaCla(playerid);
    //=== Corre??o
    //FINAL OCULTAR LOGIN
    // Sistema Fome Sono Sede
    //TimerVerificaoFss[playerid] = gettime() + TEMPO_VERIFICAOSFSS;
    //Adicionar como Logado
    new INI:File = INI_Open(UserPath(playerid));
    INI_SetTag(File,"data");
    INI_WriteInt(File, "Online", Info[playerid][pOnline] = 1);
    INI_Close(File);
    Info[playerid][pOnline] = 1;
    PlayerIsLogado[playerid] = 1;
    PlayerStumWait[playerid] = 1;
    if(Info[playerid][pPontosResetadoCorrigido] == 0 || Info[playerid][pPontosResetadoCorrigido] == 1 || Info[playerid][pPontosResetadoCorrigido] == 2 || Info[playerid][pPontosResetadoCorrigido] == 3){CorrecaoPontos(playerid);}
    //Finalizar como Logado
    new string [128];
    TotalLogin++;
    ResetPlayerMoney(playerid);
    CurrentMoney[playerid] = GetPVarInt(playerid, "Cash");
    if(Info[playerid][pLvlAdminZC] > 0)
    {
        Info[playerid][pLvlAdminZC] = 0;
        Info[playerid][pAdminZC] = 0;
        Info[playerid][pBanAppealer] = 0;
        Info[playerid][pFactionModerator] = 0;
        Info[playerid][pModStaff] = 0;
        Info[playerid][pGangModerator] = 0;
        Info[playerid][pShopTech] = 0;
        Info[playerid][pHelper] = 0;
    }
    if(Info[playerid][pDisabledZC] == 1){
        SetPlayerName(playerid, "BannedPlayer");
        //SendClientMessageEx(playerid, COLOR_WHITE,  "{FFFF00}Administraci?n:{FFFFFF} Esta cuenta est? deshabilitada.");
        Kick(playerid);
    }
    if(Info[playerid][pBannedZC] >= 2){
        SetPlayerName(playerid, "BannedPlayer");
        //SendClientMessageEx(playerid, COLOR_WHITE, "{FFFF00}Administraci?n:{FFFFFF} T? est?s baneado del servidor.");
        Kick(playerid);
    }
    // Sistema de perdida de casa. NOTA: No se quitar? el label de propietario, motivo: El servidor tiene su propio Timer (HouseUpdate).
    // NOTA: Esto solo sirve para quitar las llaves de las casas cuyo tiempo de renovaci?n se han agotado. [/hinfo] - [hTiempo] puesto a 30 d?as.
    if(Info[playerid][pPhousekeyX] != INVALID_HOUSE_ID) // Tiene casa 3 y dispone de tiempo en esta.
    {
        format(string, sizeof(string), "Atenci?n, una de sus casas (ID: %d) forma parte del nuevo sistema de propiedades, cualquier bug report?lo.", Info[playerid][pPhousekeyX]);
        SendClientMessageEx(playerid, COLOR_GREY, string);
        SendClientMessageEx(playerid, COLOR_GREY, "Comandos disponibles temporalmente: /casamenu - /armario - /vendercasa3 (M?s ayuda acerca del Sistema en el foro).");
    }
    if(Info[playerid][pWRestricted] >= 1) ResetPlayerWeaponsEx(playerid);
    GetPlayerIp(playerid, Info[playerid][pIP], 32);
    if(Info[playerid][pReg] == 0){
        Info[playerid][pLevel] = 0;
        Info[playerid][pHealth] = 100.0;
        Info[playerid][pHealthMaximo] = 500.0;
        Info[playerid][pHealthEnUso] = 500.0;
        Info[playerid][pPos_x] = 1612.3240;
        Info[playerid][pPos_y] = -2330.1670;
        Info[playerid][pPos_z] = 13.5469;
        Info[playerid][pInt] = 0;
        Info[playerid][pTeam] = 3;
        Info[playerid][pChar] = 299;
        Info[playerid][pPhousekey] = INVALID_HOUSE_ID;
        Info[playerid][pPhousekey2] = INVALID_HOUSE_ID;
        Info[playerid][pAccount] = 0;
        Info[playerid][pPbizzkey] = 9999;
    }
    if(Info[playerid][pPos_x] == 0.0 && Info[playerid][pPos_y]== 0.0&& Info[playerid][pPos_z]== 0.0){
        Info[playerid][pPos_x] = 1715.12;
        Info[playerid][pPos_y] = -1903.17;
        Info[playerid][pPos_z] = 13.56;
    }
    if(Info[playerid][pShopTech] >= 2) Info[playerid][pShopTech] = 0;
    if(Info[playerid][pAdminZC] == 0) { Info[playerid][pGangModerator] = 0; Info[playerid][pShopTech] = 0; Info[playerid][pBanAppealer] = 0; Info[playerid][pFactionModerator] = 0; Info[playerid][pModStaff] = 0; }
    if(Info[playerid][pHospital] == 1){ Info[playerid][pHospital] = 0; SetPVarInt(playerid, "Hospital", 1); }
    if(Info[playerid][pLevel] < 10 || Info[playerid][pAdminZC] > 0 || Info[playerid][pHelper] > 0) gNewbie[playerid] = 0;
    if(Info[playerid][pNMute] == 1) SetTimerEx("NewbieUnMuted", 300000, 0, "d", playerid);
    if(Info[playerid][pTut] == 1) Info[playerid][pReg] = 1;
    new text[128];

    switch(Info[playerid][pHelper])
    {
        case 1: SendClientMessageEx(playerid, COLOR_GENERAL, "{F81414}SH-RP:{FFFFFF} Voc? entrou como Auxiliar.");
        case 2: SendClientMessageEx(playerid, COLOR_GENERAL, "{F81414}SH-RP:{FFFFFF} Voc? se conectou como L?der Auxiliar.");
    }
    SendClientMessageEx(playerid, COLOR_GENERAL, GlobalMOTD);
    CheckVIPExpire(playerid);
    if(Info[playerid][pVIP] >= 1) SendClientMessageEx(playerid, COLOR_VIP, VIPMOTD);
    if(gTeam[playerid] == 0){
        gTeam[playerid] = 3;
    }
    else{
        gTeam[playerid] = Info[playerid][pTeam];
    }
    // === Corre??es === //
    if(Pescando[playerid]){
        KillTimer(TimingPescarAgain[playerid]);
        Pescando[playerid] = 0;
        CountSistemaPesca[playerid] = 0;
        ImpossiblePescar[playerid] = 0;
        PlayerTextDrawHide(playerid, PressTD[playerid]);
        ApplyAnimation(playerid, "Shinobi_Anim", "Pescar_1", 4.0, 0, 0, 0, 0, 1, 1);
        SetCameraBehindPlayer(playerid);
    }
    //Spawn
    SetSpawnInfo(playerid, Info[playerid][pTeam], Info[playerid][pChar], 2306.1470, -683.3868, 28.9453, 1.0, -1, -1, -1, -1, -1, -1);
    gPlayerLogged{playerid} = 1;
    SpawnPlayer(playerid);
    //SetTimerEx("CorrecaoSpawn", 8000, false, "d", playerid);
    new ip[32];
    GetPlayerIp(playerid, ip, 32);
    if(Info[playerid][pAdminZC] >= 1000000) SendClientMessageEx(playerid, TEAM_AZTECAS_COLOR, AdminMOTD);
    if(Info[playerid][pHelper] >= 1050000) SendClientMessageEx(playerid, TEAM_AZTECAS_COLOR, CAMOTD);
    INI_ParseFile(UserPath(playerid), "Load_Nome", .bExtra = true, .extra = playerid);
    SetPlayerFightingStyle (playerid, Info[playerid][pFStyle]);
    Info[playerid][pContactoAgendar] = 0;
    Info[playerid][pSpeedo] = 0; // Se desactiva speedo.
    WeedLogin(playerid);
    SetPlayerToTeamColor(playerid);
    return 1;
}
function CorrecaoSpawn(playerid)
{
    KillTimer(TimerSpawn[playerid]);
    SetCameraBehindPlayer(playerid);
    UnfreezePlayer(playerid);
    PlayerStumWait[playerid] = 0;
    SetPlayerPos(playerid, Info[playerid][pPos_x], Info[playerid][pPos_y], Info[playerid][pPos_z]);
    return 1;
}
function TesteBug(playerid)
{
    KillTimer(TimerBugSpawn[playerid]);
    new Float:playerPos[3];
