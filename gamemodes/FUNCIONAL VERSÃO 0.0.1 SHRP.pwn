        }
        case 16, 17, 18, 39:{
            Info[playerid][pWeapons][ 8 ] = WeaponID;
            GivePlayerWeapon( playerid, WeaponID, 5);
        }
        case 41, 42, 43:{
            Info[playerid][pWeapons][ 9 ] = WeaponID;
