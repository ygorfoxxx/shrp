CMD:atender(playerid, params[])
{
    new id, cargolevel[64];
    if(Info[playerid][pAdminZC] == 0) return SendClientMessage(playerid, 0xFF0000AA, "[ERRO]: Este comando n?o existe!");
    if(sscanf(params, "d", id)) return SendClientMessage(playerid, 0xFF230AFF, "{E9FE23}[ERRO]{FFFFFF} Use {E9FE23}/atender [ID] .");
    foreach(Player, i)
    {
        if( GetPVarInt( i, "COMMUNITY_ADVISOR_REQUEST" ) >= 1 )
        {
            AdvisorCall = i;
        }
    }
    if(AdvisorCall < 999)
    {
        if(IsPlayerConnected(AdvisorCall))
        {
            new string[168];
            if(HelpingNewbie[playerid] != 999)
            {
                SendClientMessageEx(playerid, COLOR_WHITE, "{E9FE23}[AVISO]{FFFFFF} Voc? j? est? atendendo algu?m.");
                return 1;
            }
            if(GetPVarInt(playerid, "AdvisorDuty") == 0)
            {
                SendClientMessageEx(playerid, COLOR_WHITE, "{E9FE23}[AVISO]{FFFFFF} Use para entrar em servi?o /helperon.");
                return 1;
            }
            format(string, sizeof(string), "{E9FE23}[AVISO] %s{FFFFFF} aceitou o atendimento de{E9FE23} %s.",PlayerNameDados(playerid), PlayerNameDados(AdvisorCall));
            SendDutyAdvisorMessage(TEAM_AZTECAS_COLOR, string);
            format(string, sizeof(string), "{E9FE23}[AVISO]{FFFFFF} Voc? aceitou o pedido de atendimento{E9FE23} %s{FFFFFF}, assim que terminar use {E9FE23}/finalizaratendimento{FFFFFF} para finalizar o atendimento.",PlayerNameDados(AdvisorCall));
            SendClientMessageEx(playerid, COLOR_LIGHTBLUE, string);
            switch(Info[playerid][pAdminZC])
            {
                case 1: cargolevel = "Admin";
                case 2: cargolevel = "Gerente Geral";
                case 3: cargolevel = "Gerente";
                case 4: cargolevel = "Helper";
            }
            format(string, sizeof(string), "{AB7C4E}O %s {FFFFFF}%s{AB7C4E} veio lhe atender.", cargolevel, PlayerNameDados(playerid));
            SendClientMessage(AdvisorCall, COLOR_WHITE, string);
            //format(string, sizeof(string), "{E9FE23}[AVISO]{FFFFFF} O STAFF{E9FE23} %s{FFFFFF} aceitou a sua {E9FE23}solicita??o de atendimento.",PlayerNameDados(playerid));
            //SendClientMessageEx(AdvisorCall, COLOR_LIGHTBLUE, string);
            //SendClientMessageEx(AdvisorCall, COLOR_WHITE, "{E9FE23}[AVISO] {FFFFFF}Por favor, aguarde em sua posi??o atual, muito obrigado.");
            if(sscanf(params, "d", id)) return SendClientMessage(playerid, 0xFF230AFF, "[ERRO] Use /atender [ID] .");
            {
                GetPlayerPos(playerid, atendimentoPos[0], atendimentoPos[1], atendimentoPos[2]);
                GetPlayerFacingAngle(playerid, atendimentoPos[3]);
                HelpingNewbie[playerid] = AdvisorCall;
                AdvisorCall = 999;
                TogglePlayerSpectating(playerid, 1);
                PlayerSpectatePlayer(playerid, id);
                ChatAtendimentoStaff[playerid] = id;
                SolicitadoAtendimento[id] = 0;
                ChatAtendimentoPlayer[id] = playerid;
                Status[playerid] = 5;
                Status[id] = 5;
                Atendendo[playerid] = 1;
                SendoAtendido[id] = 1;
                SistemaBandanaIDStatus(playerid);
                SistemaBandanaIDStatus(id);
            }
            return 1;
        }
    }
    else
    {
        SendClientMessageEx(playerid, COLOR_WHITE, "{E9FE23}[AVISO]{FFFFFF} Ningu?m ainda pediu ajuda ou j? aceitou a que voc? estava indo.");
        return 1;
    }
    return 1;
}
CMD:finalizaratendimento(playerid, params[])
{
    if(Info[playerid][pAdminZC] == 0) return SendClientMessage(playerid, 0xFF0000AA, "[ERRO]: Este comando n?o existe!");
    if(HelpingNewbie[playerid] < 999)
    {
        new string[128], cargostaff[64];
        format(string, sizeof(string), "{E9FE23}[AVISO] %s{FFFFFF} finalizou o pedido de atendimento de {E9FE23}%s.",PlayerNameDados(playerid), PlayerNameDados(HelpingNewbie[playerid]));
        SendDutyAdvisorMessage(TEAM_AZTECAS_COLOR, string);
        SetPlayerPos(playerid, atendimentoPos[0], atendimentoPos[1], atendimentoPos[2]);
        SetPlayerFacingAngle(playerid, atendimentoPos[3]);
        SetPlayerVirtualWorld(playerid, 0);
        SetPVarInt(HelpingNewbie[playerid], "COMMUNITY_ADVISOR_REQUEST", 0 );
        SetPVarInt(playerid, "FinalizarAt", 1);
        switch(Info[playerid][pAdminZC])
        {
            case 1: cargostaff = "Admin";
            case 2: cargostaff = "Gerente";
            case 3: cargostaff = "Gerente Geral";
            case 4: cargostaff = "GM";
            case 5: cargostaff = "Helper";
        }
        format(string, sizeof(string), "{AB7C4E}O %s {FFFFFF}%s{AB7C4E} finalizou o atendimento.", cargostaff, PlayerNameDados(playerid));
        SendClientMessage(ChatAtendimentoStaff[playerid], COLOR_WHITE, string);
        Status[playerid] = 0;
        Status[ChatAtendimentoStaff[playerid]] = 0;
        SistemaBandanaIDStatus(playerid);
        SistemaBandanaIDStatus(ChatAtendimentoStaff[playerid]);
        Atendendo[playerid] = 0;
        SendoAtendido[ChatAtendimentoStaff[playerid]] = 0;
        ChatAtendimentoPlayer[ChatAtendimentoStaff[playerid]] = -1;
        ChatAtendimentoStaff[playerid] = -1;
        HelpingNewbie[playerid] = 999;
        TogglePlayerSpectating(playerid, 0);
        SetCameraBehindPlayer(playerid);
        return 1;
    }
    else SendClientMessageEx(playerid, COLOR_GREY, "{E9FE23}[AVISO]{FFFFFF} Voc? n?o est? ajudando ningu?m!");
    return 1;
}
