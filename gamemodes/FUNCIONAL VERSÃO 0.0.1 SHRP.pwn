    GetPlayerPos(targetid, x, y, z);
    GetPlayerFacingAngle(targetid, a);
    a += 180.0; // Inverte o ?ngulo em 180 graus
    x += (1.0 * floatsin(-a, degrees)); // Inverte o c?lculo de x
    y += (1.0 * floatcos(-a, degrees)); // Inverte o c?lculo de y
    SetPlayerPos(playerid, x, y, z + 1.0);
    return 1;
}
