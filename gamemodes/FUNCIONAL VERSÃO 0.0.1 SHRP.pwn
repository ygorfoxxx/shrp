function firstaid5(playerid)
{
    if(GetPVarInt(playerid, "usingfirstaid") == 1)
    {
        new
            Float:HealthEnUso, Float:HealthMaximo;
        HealthEnUso = Info[playerid][pHealthEnUso];
        HealthMaximo = Info[playerid][pHealthMaximo];
        if(HealthEnUso >= HealthMaximo)
        {
            if(GetPVarInt(playerid, "Inconsciente") >= 1){
                KitMedicoInconsciente(playerid);
            }else{
                NewSetHP(playerid, HealthMaximo);
                firstaidexpire(playerid);
            }
        }
        else
        {
            if(GetPVarInt(playerid, "Inconsciente") >= 1){
                KitMedicoInconsciente(playerid);
            }else if((HealthEnUso + HealthMaximo/40) > HealthMaximo){
                NewSetHP(playerid, HealthMaximo);
            }else{
                NewSetHP(playerid, HealthEnUso + HealthMaximo/40);
            }
        }
    }
}
function firstaidexpire(playerid)
{
    KillTimer(GetPVarInt(playerid, "firstaid5"));
    KillTimer(GetPVarInt(playerid, "firstaidexpire"));
    SetPVarInt(playerid, "usingfirstaid", 0);
    Audio_Play(playerid, 10);
    SendClientMessage(playerid, -1, "{5FDE35}(KIT-MEDICO){FFFFFF} Seu kit-m?dico j? pode ser usado novamente.");
}
function KitMedicoInconsciente(playerid)
{
    KillTimer(GetPVarInt(playerid, "firstaid5"));
    SetPVarInt(playerid, "usingfirstaid", 0);
    Audio_Play(playerid, 10);
    SendClientMessage(playerid, -1, "{5FDE35}(KIT-MEDICO){FFFFFF} Seu kit-m?dico n?o tem mais efeito (VOC? EST? INCONSCIENTE).");
    return 1;
}
function UsingKitMedico(playerid)
{
    new str[64];
    SetPVarInt(playerid, "firstaid5", SetTimerEx("firstaid5", 12000, 1, "d", playerid));
    SetPVarInt(playerid, "firstaidexpire", SetTimerEx("firstaidexpire",450000, 0, "d", playerid));
    SetPVarInt(playerid, "usingfirstaid", 1);
    format(str, sizeof(str), "* %s terminou de usar um kit m?dico tempor?rio.", PlayerNameDados(playerid));
    ProxDetector(40.0, playerid, str, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
    UsandoKit[playerid] = 0;
    ClearAnimations(playerid);
}
