CMD:criarcogumelo(playerid, params[])
{
    new idcogu, tipo, mundo = 0, nome[32], str[264];
    if(Info[playerid][pAdminZC] == 0 || Info[playerid][pAdminZC] == 2 || Info[playerid][pAdminZC] == 3 || Info[playerid][pAdminZC] == 4 || Info[playerid][pAdminZC] == 5) return SendClientMessage(playerid, 0xFF0000AA, "[ERRO]: Este comando n?o existe!");
    if(sscanf(params, "dd", idcogu, tipo)) return SendClientMessageEx(playerid, COLOR_WHITE, "[INFO]: Use /criarcogumelo [ID] [1 - 5]");
    if(idcogu < 1 || idcogu >= MAX_COGUMELOS) return SendClientMessage(playerid, COLOR_WHITE, "[INFO]: ID deste cogumelo ? invalido");
    if(Cogumelos[idcogu][pCogumeloX] == 0.0)
    {
        if(tipo == 0) return SendClientMessage(playerid, -1, "Cogumelo tipo 0 n?o existe");
        Cogumelos[idcogu][pCoguTipo] = tipo;
        Cogumelos[idcogu][pCoguMundo] = mundo;
        GetPlayerPos(playerid, Cogumelos[idcogu][pCogumeloX], Cogumelos[idcogu][pCogumeloY], Cogumelos[idcogu][pCogumeloZ]);
        if(tipo == 1 && QuantidadeEnoki >= 120){SendClientMessage(playerid, -1, "Voc? atingiu o limite de Quantidade Enoki"); return 1;}
        if(tipo == 2 && QuantidadeShimeji >= 165){SendClientMessage(playerid, -1, "Voc? atingiu o limite de Quantidade Shimeji"); return 1;}
        if(tipo == 3 && QuantidadeShitake >= 100){SendClientMessage(playerid, -1, "Voc? atingiu o limite de Quantidade Shitake"); return 1;}
        if(tipo == 4 && QuantidadeGyromitra >= 15){SendClientMessage(playerid, -1, "Voc? atingiu o limite de Quantidade Gyromitra"); return 1;}
        if(tipo == 5 && QuantidadeEryngui >= 100){SendClientMessage(playerid, -1, "Voc? atingiu o limite de Quantidade Eryungui"); return 1;}
        switch(tipo)
        {
            case 0: return 0;
            case 1: {QuantidadeEnoki++; nome = "Cogumelo Enoki"; Cogumelos[idcogu][pCoguID] = 15811;} // Enoki
            case 2: {QuantidadeShimeji++; nome = "Cogumelo Shimeji"; Cogumelos[idcogu][pCoguID] = 15812;} // Shimeji
            case 3: {QuantidadeShitake++; nome = "Cogumelo Shitake"; Cogumelos[idcogu][pCoguID] = 15814;} // Shitake
            case 4: {QuantidadeGyromitra++; nome = "Cogumelo Gyromitra"; Cogumelos[idcogu][pCoguID] = 15813;} // Gyromitra
            case 5: {QuantidadeEryngui++; nome = "Cogumelo Eryungui"; Cogumelos[idcogu][pCoguID] = 15815;} // Eryungui
        }
        Cogumelos[idcogu][pCoguNome] = nome;
        Cogumelos[idcogu][pCoguObj] = CreateDynamicObject(Cogumelos[idcogu][pCoguID], Cogumelos[idcogu][pCogumeloX], Cogumelos[idcogu][pCogumeloY], Cogumelos[idcogu][pCogumeloZ]-1.05, 0.0, 0.0, 0.0, mundo);
        format(str, sizeof(str), "{FFFFFF}%s\nPressione {AB7C4E}ALT{FFFFFF} para pegar.", nome);
        Cogumelos[idcogu][pCoguText] = CreateDynamic3DTextLabel(str, COLOR_WHITE, Cogumelos[idcogu][pCogumeloX], Cogumelos[idcogu][pCogumeloY], Cogumelos[idcogu][pCogumeloZ]-1.05, 6);
        SalvarCogumelo(idcogu);
    }
    return 1;
}
