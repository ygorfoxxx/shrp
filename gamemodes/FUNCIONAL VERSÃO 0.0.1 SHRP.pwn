        format(szMessage, sizeof szMessage, "{BE81F7}%s "COL_WHITE"has creado un nuevo graffiti: '{BE81F7}%s"COL_WHITE"'", PlayerNameDados(playerid), spraytag_text[playerid]);
        Log("LOGS/graffitis.log", szMessage);
