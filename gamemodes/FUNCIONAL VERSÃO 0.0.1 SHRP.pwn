    SetTimerEx("DaikodanJutsu", 850, 0, "d", playerid);
    return 1;
}
function DaikodanJutsu(playerid)
{
    #define SPEED (90.0)
    #define Z_SPEED (0.0)
    new modelid = 15077;
    new Float:x, Float:y, Float:z, Float:ang;
    AudioInPlayer(playerid, 30.0, 79);
    GetPlayerPos(playerid, x, y, z);
    GetPlayerFacingAngle(playerid, ang);
    DaikodanBall[playerid] = CreateObject(modelid, x, y - 0.5 * floatcos(-(ang + 90.0), degrees), z, 0.000000, - 0.0, ang - 0.000000);
    PHY_InitObject(DaikodanBall[playerid]);
    PHY_SetObjectVelocity(DaikodanBall[playerid], SPEED * floatsin(-ang, degrees), SPEED * floatcos(-ang, degrees), Z_SPEED);
    PHY_SetObjectFriction(DaikodanBall[playerid], 100); // This will stop the object when it touchs the ground.
    PHY_SetObjectZBound(DaikodanBall[playerid], z - 1.0, _, 0.0);
    PHY_ToggleObjectPlayerColls(DaikodanBall[playerid]);
    DaikodanHitted(playerid);
    SetTimerEx("DaikodanOFFExplosion", 850, 0, "d", playerid); //Timer pra Sair
    return 1;
}
