{
    new str[64], Float:rPos[3];
    for(new h = 0; h < sizeof(PedraAlgodao); h++)
    {
        if(IsPlayerInRangeOfPoint(playerid, 1.5, PedraAlgodao[h][paFloatX], PedraAlgodao[h][paFloatY], PedraAlgodao[h][paFloatZ]))
        {
            //if(!IsValidDynamicObject(PedraAlgodao[h][paObj])) return 0;
            if(PedraAlgodao[h][paTipo] == 1 && PegouPedraAlgodao[h] == 0){
            new chance  = random(100);
