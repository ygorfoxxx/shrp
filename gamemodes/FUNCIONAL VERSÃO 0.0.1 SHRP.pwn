                        Info[i][pEstado] = 0;
                        DeletePVar(i, "PlayerCuffed");
                        PlayerCuffedTime[i] = 0;
                    }
                    else{PlayerCuffedTime[i] = 60;}
                }
                else{PlayerCuffedTime[i] -= 1;}
            }
            if(PlayerDrunk[i] > 9)
            {
                if(PlayerDrunkTime[i] > 20)
                {
                    ApplyAnimation(i,"PED", "WALK_DRUNK",6.0,0,1,0,0,PlayerDrunk[i]*1000);
