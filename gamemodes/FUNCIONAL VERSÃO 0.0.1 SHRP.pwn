function SalvarLogSaida(playerid, reason)
{
    new string[160], year, month,day;
    getdate(year, month, day);
    switch(reason)
    {
        case 0:
        {
            format(string, sizeof(string), "%s saiu do servidor (Possivel Crash).", PlayerNameDados(playerid));
            ProxDetector(8.0, playerid, string, COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW);
            format(string, sizeof(string), "[LOG-SAIDA]: %s saiu do servidor com o (ID: %d) no dia (%d-%d-%d). (Possivel Crash)", PlayerNameDados(playerid), playerid, month, day, year);
            Log("LOGS/entrada.log", string);
        }
        case 1:
        {
            format(string, sizeof(string), "%s saiu do servidor (Abandono).", PlayerNameDados(playerid));
            ProxDetector(8.0, playerid, string, COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW);
            format(string, sizeof(string), "[LOG-SAIDA]: %s saiu do servidor com o (ID: %d) no dia (%d-%d-%d). (Abandono)", PlayerNameDados(playerid), playerid, month, day, year);
            Log("LOGS/entrada.log", string);
        }
        case 2:
        {
            format(string, sizeof(string), "%s saiu do servidor (Kick/Ban).", PlayerNameDados(playerid));
            ProxDetector(8.0, playerid, string, COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW,COLOR_YELLOW);
            format(string, sizeof(string), "[LOG-SAIDA]: %s saiu do servidor com o (ID: %d) no dia (%d-%d-%d). (Kick/Ban)", PlayerNameDados(playerid), playerid, month, day, year);
            Log("LOGS/entradasaida.log", string);
        }
    }
    return 1;
}
