function SubirDLevel(playerid)
{
    new str[128];
    new limite;

    // Definir limite conforme Rank
    switch(Info[playerid][pRank])
    {
        case 0: limite = 30;   // Aluno
        case 1: limite = 50;   // Genin
        case 2: limite = 70;   // Chunin
        case 3: limite = 150;  // Jounin
        default: limite = 999; // Se for outro rank ainda n?o configurado
    }

    // Verifica??o do limite
    if(Info[playerid][pLevel] >= limite)
    {

        return 1;
    }

    if(Info[playerid][pExpAtual] >= Info[playerid][pExpNextLevel])
    {
        Info[playerid][pExpAtual] = Info[playerid][pExpNextLevel];
        format(str, sizeof(str), "{AB7C4E}(UP) Parab?ns voc? subiu do rank {FFFFFF}%d{AB7C4E} para {FFFFFF}%d{AB7C4E}.",
            Info[playerid][pLevel], Info[playerid][pLevel]+1);
        SendClientMessageEx(playerid, -1, str);

        Info[playerid][pLevel] ++;
        Info[playerid][pPuntosNinja] += 5;
        Info[playerid][pHealthMaximo] += 10;
        Info[playerid][pChakra] += 10;
        Info[playerid][pExpAtual] = 0;

        CheckLevelTimer[playerid] = SetTimerEx("ResetExpNextLevel", 100, 0, "d", playerid);
    }

    SalvarConta(playerid);
    return 1;
}

function SubirRol(playerid)
{
    if(Info[playerid][pPuntosRol] >=1 && Info[playerid][pPuntosRol] <=10)
