function Mizurappa(playerid)
{
    LastJutsuReset(playerid);
    AudioInPlayer(playerid, 50.0, 62);
    AparecerMapaJutsu(playerid, 50.0);
    Mizuball[playerid] = LanzarJutsu(playerid, 15071, 150.0, 60.0);
    DarXPPorTreinamento(playerid);
    IsMizurappaHitted(playerid);
    SetTimerEx("MizuOff2", 850, 0, "i", playerid);
    return 1;
}
function IsMizurappaHitted(playerid)
{
    if(!IsValidDynamicObject(Mizuball[playerid])) return 0;
    new Float:mizuPos[3], vw = GetPlayerVirtualWorld(playerid);
    GetDynamicObjectPos(Mizuball[playerid], mizuPos[0], mizuPos[1], mizuPos[2]);
    for(new i; i<MAX_PLAYERS; i++)
    {
        switch(GetShootedJutsu(mizuPos[0], mizuPos[1], mizuPos[2], 2.0, false, true, vw, playerid, i))
        {
            case WALL:
            {
                MizuOff(playerid);
                AudioInArea(i, 70, mizuPos[0], mizuPos[1], mizuPos[2], 100.0);
                //MizurappaEffect[playerid] = CreateDynamicObject(18684, mizuPos[0], mizuPos[1], mizuPos[2], 0.0, 0.0, 0.0, vw);
                MizurappaEffect[playerid] = CreateObject(18685, mizuPos[0], mizuPos[1], mizuPos[2], 0.0, 0.0, 0.0, 300.0);
                DarXPPorTreinamento(playerid);
                SetTimerEx("MizuOff3", 1500, false, "i", playerid);
            }
            case DUMMY:
            {
                MizuOff(playerid);
                AudioInArea(i, 70, mizuPos[0], mizuPos[1], mizuPos[2], 100.0);
                //MizurappaEffect[playerid] = CreateDynamicObject(18684, mizuPos[0], mizuPos[1], mizuPos[2], 0.0, 0.0, 0.0, vw);
                MizurappaEffect[playerid] = CreateObject(18684, mizuPos[0], mizuPos[1], mizuPos[2], 0.0, 0.0, 0.0, 300.0);
                SetTimerEx("MizuOff3", 1500, false, "i", playerid);
                return 1;
            }
            case PLAYER:
            {
                if(Izanagi[i][IzanagiAtivado]){
                    SetTimerEx("IzanagiVoltar", 100, false, "d", i);
                }else{
                    if(EntrouArenaPvP[i] == 1){LastHitArenaPvP(playerid, i);}
                    ClearSelo(i);
                    MizuOff(playerid);
                    DmgM(playerid, i);
                    PCarregandoChakra(i);
                    EmCombateIniciar(playerid);
                    EmCombateIniciar(i);
                    SetPlayerToFacePlayer(playerid, i);
                    ApplyAnimation(i, "BIKEh", "BIKEh_jumponR", 4.1, 0, 1, 1, 0, 0, 1);
                    //MizurappaEffect[playerid] = CreateDynamicObject(18684, mizuPos[0], mizuPos[1], mizuPos[2], 0.0, 0.0, 0.0, vw);
                    MizurappaEffect[playerid] = CreateObject(15076, mizuPos[0], mizuPos[1], mizuPos[2], 0.0, 0.0, 0.0, 300.0);
                    SetTimerEx("MizuOff3", 1500, false, "i", playerid);
                }
            }
        }
    }
    SetTimerEx("IsMizurappaHitted", 50, false, "d", playerid);
    return 0;
}
function DmgM(playerid, alvomizurappa)
{
    new Float:X, Float:Y, Float:Z;
    GetPlayerPos(alvomizurappa, X, Y, Z);
    if(FCNPC_IsValid(alvomizurappa)){SetDamageToAnimal(playerid, alvomizurappa, GetDamageToPlayer(playerid, Suiton, 0, 20.0));}
    SetDamageToPlayer(playerid, alvomizurappa, GetDamageToPlayer(playerid, Suiton, 0, 60.0));
    AudioInArea(alvomizurappa, 70, X, Y, Z, 100.0);
    AparecerMapaJutsu(alvomizurappa, 100.0);
    FraturarNinja(alvomizurappa);
    return 1;
}
MizuOff(playerid)
{
    if(!IsValidDynamicObject(Mizuball[playerid])) return 0;
    DestroyDynamicObject(Mizuball[playerid]);
    Mizuball[playerid] = INVALID_OBJECT_ID;
